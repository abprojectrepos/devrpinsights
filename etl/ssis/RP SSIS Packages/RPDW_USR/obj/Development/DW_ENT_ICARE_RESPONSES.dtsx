xA;&#x9;&#xA;  PRINT (@query);&#xA;  SET @all_queries=CONCAT(N'',@all_queries,@query);&#xA;  EXEC (@query);&#xA;&#xA;  &#xA;  INSERT INTO &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].&quot;+ @[User::TgtTableName]+&quot;               &#xA;         (  &#xA;&#x9;&#x9; [surveyid]&#xA;&#x9;&#x9;,[questionid]&#xA;&#x9;&#x9;,[response]&#xA;&#x9;&#x9;,[studentid]&#xA;&#x9;&#x9;,[CREATED_ON]&#xA;&#x9;&#x9;,[MODIFIED_ON]&#xA;&#x9;     ) &#xA;&#x9;SELECT &#xA;&#x9;&#x9;Survey_ID&#x9;&#x9;as surveyid&#xA;&#x9;&#x9;,Question_ID&#x9;as questionid&#x9;&#xA;&#x9;&#x9;,Response&#x9;&#x9;as response&#xA;&#x9;&#x9;,Student_ID&#x9;&#x9;as studentid&#xA;&#x9;&#x9;,getdate()&#xA;&#x9;    ,getdate()&#xA;&#x9;FROM ##TMP_ICareResponses&#xA;;&#xA;&#xA;&#xA;-- Check Source Table Count&#xA;SELECT @source_cnt=COUNT(*) FROM ##TMP_ICareResponses;&#xA;&#x9;&#xA;-- Check Tgt Table Count&#xA;SELECT @target_cnt=COUNT(*) FROM &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].&quot;+ @[User::TgtTableName]+&quot;;&#xA;    &#xA;-- Update Counts&#xA;UPDATE &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].[ETL_JOB_STATUS] &#xA;   SET [SRC_CNT]=@source_cnt,&#xA;       [TGT_CNT]=@target_cnt,&#xA;&#x9;   [REMARKS]=@all_queries,&#xA;&#x9;   [FILENAME]=@outputfilename&#xA;   WHERE [JOB_STATUS_ID]=@jobstatusid;&#x9;&#xA;    &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO &quot;+ @[$Project::RPDW_USR_Initial_Catalog] + &quot;.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;&#xA;-- Update SQL QUERIES EXECUTED&#xA;UPDATE &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].[ETL_JOB_STATUS] &#xA;   SET [REMARKS]=@all_queries&#xA;   WHERE [JOB_STATUS_ID]=@jobstatusid;&#x9;&#xA;   &#xA;END CATCH &#x