      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
IF OBJECT_ID(N'tempdb..##TMP_ICareResponses') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_ICareResponses
END

IF OBJECT_ID(N'tempdb..##TMP_TBL1') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_TBL1
END

DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;

SET @source_cnt=0;
SET @target_cnt=0;


select @jobstatusid=0;
select @date1=convert(varchar(10),GETDATE(),112);
select @outputfilename=CONCAT('DW_icare_',@date1,'.txt');


SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_STG.[dbo].[ETL_JOB_PARAMS]  
WHERE JOB_NAME='DW_ENT_ICARE_RESPONSES';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
; 

TRUNCATE TABLE RPDW_USR_STG.[dbo].DW_ENT_ICARE_RESPONSES  
;

  
-- Begin SET
SET @query=CONCAT(
    N'SELECT 
		 Survey_ID
		,Question_ID	
		,Response
		,Student_ID
	INTO ##TMP_ICareResponses
	FROM RPDW_STG.[dbo].[V_DW_ICARE_SURVEY]
	WHERE Question_ID in (''A03'', ''B01X'', ''B07'', ''B08'', ''B09X'', ''B10'', ''ICAREGHI'', ''ICAREPCI'')
    ',@condition
    ); -- End Set	
	
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);
  EXEC (@query);

  
  INSERT INTO RPDW_USR_STG.[dbo].DW_ENT_ICARE_RESPONSES               
         (  
		 [surveyid]
		,[questionid]
		,[response]
		,[studentid]
		,[CREATED_ON]
		,[MODIFIED_ON]
	     ) 
	SELECT 
		Survey_ID		as surveyid
		,Question_ID	as questionid	
		,Response		as response
		,Student_ID		as studentid
		,getdate()
	    ,getdate()
	FROM ##TMP_ICareResponses
;


-- Check Source Table Count
SELECT @source_cnt=COUNT(*) FROM ##TMP_ICareResponses;
	
-- Check Tgt Table Count
SELECT @target_cnt=COUNT(*) FROM RPDW_USR_STG.[dbo].DW_ENT_ICARE_RESPONSES;
    
-- Update Counts
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [SRC_CNT]=@source_cnt,
       [TGT_CNT]=@target_cnt,
	   [REMARKS]=@all_queries,
	   [FILENAME]=@outputfilename
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
    

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO RPDW_USR_STG.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on

-- Update SQL QUERIES EXECUTED
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [REMARKS]=@all_queries
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
   
END CATCH 
GO
