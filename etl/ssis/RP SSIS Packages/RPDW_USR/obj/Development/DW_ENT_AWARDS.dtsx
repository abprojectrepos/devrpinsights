------------------------------------------------------------------------------------------
-- 1) FRAMEWORK: ADD/REMOVE TEMP TABLE HERE AS NEEDED IN THE PROCESS
------------------------------------------------------------------------------------------
IF OBJECT_ID(N'tempdb..##TMP_FINAL_TBL') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_FINAL_TBL
END

IF OBJECT_ID(N'tempdb..##TMP_TBL1') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_TBL1
END


------------------------------------------------------------------------------------------
-- STORED VARIABLE DECLARATION AND SETTING OF INITIAL VALUES: DO NOT CHANGE ANYTHING 
------------------------------------------------------------------------------------------
DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;
SET @source_cnt=0;
SET @target_cnt=0;

SELECT @jobstatusid=0;
SELECT @date1=convert(varchar(10),GETDATE(),112);
SELECT @outputfilename=CONCAT('DW_acadawards_',@date1,'.txt');

------------------------------------------------------------------------------------------
-- EXTRACTION OF PRE-DEFINED FILTER CRITERIA FROM ETL_PARM: DO NOT CHANGE ANYTHING 
------------------------------------------------------------------------------------------
SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_PRD.[dbo].[ETL_JOB_PARAMS] WHERE JOB_NAME='DW_ENT_AWARDS';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
;

TRUNCATE TABLE RPDW_USR_PRD.[dbo].DW_ENT_AWARDS  
;


------------------------------------------------------------------------------------------
-- MAIN EXTRACTION QUERY: 
-- UPDATE THE FOLLOWING ACCORDINGLY
--   (1) COLUMN NAME1 TO COLUMN NAME 4
--   (2) SOURCE TABLE NAME
-- Note: DO NOT CHANGE THE WHERE CLAUSE. IF NEED TO SUBSET, PUT THE CONDITION IN THE ETL_PARM TABLE,
--       AND THIS QUERY WILL APPEND THE CONDITION ACCORDINGLY.
------------------------------------------------------------------------------------------

--  
-- Begin SET
--

SET @query=CONCAT(
    N'SELECT 
		 [Student_ID]
		 , [Acad_Term] as STRM
		 , [Qual_Type] as ACAD_Career
		 , [Award_ID]
		 , [Award_Type] as A_GRAW_TYPE
		 , [Award_Type_Desc] as A_GRAW_DESCFORMAL
		 , [RQMT_TERM]
		 , [Descr]
		 , [Award_Module_Code] 
	  
	INTO ##TMP_FINAL_TBL
	
	FROM RPDW_PRD.[dbo].[DW_AWARDS]
		
	WHERE Qual_Type IN	
    ',@QUAL_TYPE
    ,@condition

); -- End Set	
	
PRINT (@query);
SET @all_queries=CONCAT(N'',@all_queries,@query);
EXEC (@query);

------------------------------------------------------------------------------------------
-- MAIN INSERTION QUERY: 
-- UPDATE THE FOLLOWING ACCORDINGLY
--   (1) COLUMN NAME1 TO COLUMN NAME 4
--   (2) SOURCE TABLE NAME
-- Note: DO NOT CHANGE THE WHERE CLAUSE. IF NEED TO SUBSET, PUT THE CONDITION IN THE ETL_PARM TABLE,
--       AND THIS QUERY WILL APPEND THE CONDITION ACCORDINGLY.
------------------------------------------------------------------------------------------
  
  INSERT INTO RPDW_USR_PRD.[dbo].DW_ENT_AWARDS               
         (  
		 [Student_ID]
		, [STRM]
		, [ACAD_Career]
		, [Award_ID]
		, [A_GRAW_TYPE]
		, [A_GRAW_DESCFORMAL]
		, [RQMT_TERM]
		, [Descr]
		, [Award_Module_Code]	 
		,[CREATED_ON]
		,[MODIFIED_ON]
	     ) 
	SELECT 
		 [Student_ID]
		, [STRM]
		, [ACAD_Career]
		, [Award_ID]
		, [A_GRAW_TYPE]
		, [A_GRAW_DESCFORMAL]
		, [RQMT_TERM]
		, [Descr]
		, [Award_Module_Code]	 	 
		,getdate()
	    ,getdate()
	FROM ##TMP_FINAL_TBL
;


------------------------------------------------------------------------------------------
-- SOURCE AND TARGET TABLE COUNTS UPDATE: DO NOT CHANGE ANYTHING 
------------------------------------------------------------------------------------------
-- Check Source Table Count
SELECT @source_cnt=COUNT(*) FROM ##TMP_FINAL_TBL;
	
-- Check Tgt Table Count
SELECT @target_cnt=COUNT(*) FROM RPDW_USR_PRD.[dbo].DW_ENT_AWARDS;
    
-- Update Counts
UPDATE RPDW_USR_PRD.[dbo].[ETL_JOB_STATUS] 
   SET [SRC_CNT]=@source_cnt,
       [TGT_CNT]=@target_cnt,
	   [REMARKS]=@all_queries,
	   [FILENAME]=@outputfilename
   WHERE [JOB_STATUS_ID]=@jobstatusid;

--------------------------------------
COMMIT TRANSACTION
END TRY


------------------------------------------------------------------------------------------
-- ERROR HANDLING: DO NOT CHANGE ANYTHING 
------------------------------------------------------------------------------------------
BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO RPDW_USR_PRD.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on

-- Update SQL QUERIES EXECUTED
UPDATE RPDW_USR_PRD.[dbo].[ETL_JOB_STATUS] 
   SET [REMARKS]=@all_queries
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
   
END CATCH 
GO
