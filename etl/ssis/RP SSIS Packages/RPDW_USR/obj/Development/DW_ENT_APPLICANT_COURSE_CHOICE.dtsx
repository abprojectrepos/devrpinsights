DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;

SET @source_cnt=0;
SET @target_cnt=0;


select @jobstatusid=0;
select @date1=convert(varchar(10),GETDATE(),112);
select @outputfilename=CONCAT('DW_applicantcoursechoice_',@date1,'.txt');


SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_STG.[dbo].[ETL_JOB_PARAMS]  
WHERE JOB_NAME='DW_ENT_APPLICANT_COURSE_CHOICE';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
; 

TRUNCATE TABLE RPDW_USR_STG.[dbo].DW_ENT_APPLICANT_COURSE_CHOICE  
;

-- Begin SET
SET @query=CONCAT(
    N'SELECT 
	a.Applicant_Id
	,CONVERT(varchar(4), CONCAT(''20'',LEFT(a.Admit_Term,2))) AS Academic_Year
	,a.Admit_Type  AS Admission_Exercise
	,a.Admit_Term
	,a.App_Number
	,b.Applied_Institution
	,b.Applied_JAE_Course_Code
	,b.Applied_JAE_Course
	,b.Choice_Number
	,b.Entry_Qualification
	,ROW_NUMBER() OVER (PARTITION BY a.App_Number, a.Admit_Type, a.Applicant_Id, b.Academic_Year,  b.Choice_Number 
                  ORDER BY a.App_Number, a.Admit_Type, a.Applicant_Id, b.Academic_Year,  b.Choice_Number  ) as rec_cnt_by_pk
				  
	INTO ##TMP_TBL1
	FROM RPDW_STG.[dbo].[DW_APPLICANT_DETAILS] a
	
	LEFT JOIN RPDW_STG.[dbo].[DM_ORG_ADMISSION_EXERCISE] b
	
		ON a.Applicant_Id = b.Applicant_Id
		AND a.App_Number = b.Application_Number
		
	WHERE a.Admit_Term &lt;&gt; (''NA'') 
     AND a.Qual_Type IN '
	 ,@QUAL_TYPE
	 );  -- End Set	
	 
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);
  EXEC (@query);	 

  
  
-- Begin SET
SET @query=CONCAT(
  N'SELECT 
	 Applicant_Id
	,Academic_Year
	,Admission_Exercise
	,Admit_Term
	,App_Number
	,Applied_Institution
	,Applied_JAE_Course_Code
	,Applied_JAE_Course
	,Choice_Number
	,Entry_Qualification
 INTO ##TMP_App_Course_Choice
 FROM ##TMP_TBL1 
 WHERE rec_cnt_by_pk = 1
 ',@condition	 
	 
); -- End Set	
		
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);
  EXEC (@query);	 

  
