END

DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;

SET @source_cnt=0;
SET @target_cnt=0;


select @jobstatusid=0;
select @date1=convert(varchar(10),GETDATE(),112);
select @outputfilename=CONCAT('DW_student_',@date1,'.txt');


SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_STG.[dbo].[ETL_JOB_PARAMS]  
WHERE JOB_NAME='DW_ENT_STUDENT';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
;  

TRUNCATE TABLE RPDW_USR_STG.[dbo].DW_ENT_STUDENT  
;

-- Criteria1: Select Distinct record based on this grouping: Student_ID, Gender, Residency_Type, Admit_Term, Rqmt_Term
--            and the last digit of Admit_Term should end in '0'

-- Begin SET
SET @query=
    N'WITH TEMP1 AS ( 
		SELECT 
		Student_ID
		,Gender
		,Residency_Type
		,Admit_Term
		,Rqmt_Term
		,Student_Name
		,Date_of_Birth
		,Birth_Country
		,Birth_State
		,Ethnic_Group
		,Citizenship
		,Marital_Status
		,Applicatn_Number
		,Effective_Date
		,ROW_NUMBER() OVER (PARTITION BY Student_ID, Gender, Residency_Type, Admit_Term, Rqmt_Term 
							ORDER BY  Student_ID, Gender, Residency_Type, Admit_Term, Rqmt_Term, Effective_Date DESC) as rec_cnt_by_pk
							
		FROM RPDW_STG. dbo.DW_STUDENT_DETAILS
		
	) SELECT * INTO ##TMP_TBL1 
	  FROM TEMP1 
	  WHERE rec_cnt_by_pk = 1
	  AND RIGHT(Admit_Term,1) = ''0''
	 ';  -- End Set	
	 
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);  
  EXEC (@query);	 

  
-- Criteria2: Select Distinct record based on this grouping: Student_ID,  Admit_Term 
--            if any duplicate record, get the latest record based on Effective_Date

-- Begin SET
SET @query=
    N'WITH TEMP2 AS ( 
	SELECT  
			Student_ID
			,Gender
			,Residency_Type
			,Admit_Term
			,Rqmt_Term
			,Student_Name
			,Date_of_Birth
			,Birth_Country
			,Birth_State
			,Ethnic_Group
			,Citizenship
			,Marital_Status
			,Applicatn_Number
			,Effective_Date
			,ROW_NUMBER() OVER (PARTITION BY Student_ID,  Admit_Term 
						  ORDER BY  Student_ID, Admit_Term, Effective_Date DESC) as rec_cnt_by_pk

							
		FROM ##TMP_TBL1
		
	) SELECT * INTO ##TMP_Student 
	  FROM TEMP2 
	  WHERE rec_cnt_by_pk = 1
	 ';  -- End Set	
	 
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);  
  EXEC (@query);	
  
  INSERT INTO RPDW_USR_STG.[dbo].DW_ENT_STUDENT               
         (  
		[EMPLID]
		,[NAME]
		,[BIRTHDATE]
		,[BIRTH_COUNTRY]
		,[BIRTH_STATE]
		,[GENDER]
		,[ETHNIC_GROUP]
		,[CITIZENSHIP]
		,[RESIDENCY_TYPE]
		,[MARITAL_STATUS]
		,[ADMIT_TERM]
		,[REQT_TERM]		
		,[CREATED_ON]
		,[MODIFIED_ON]
	     ) 
	SELECT 
		Student_ID					as [EMPLID]
		,Student_Name				as [NAME]
		,Date_of_Birth				as [BIRTHDATE]
		,Birth_Country				as [BIRTH_COUNTRY]
		,Birth_State				as [BIRTH_STATE]
		,Gender						as [GENDER]
		,Ethnic_Group				as [ETHNIC_GROUP]
		,Citizenship				as [CITIZENSHIP]
		,Residency_Type				as [RESIDENCY_TYPE]
		,Marital_Status				as [MARITAL_STATUS]
		,Admit_Term					as [ADMIT_TERM]
		,Rqmt_Term					as [REQT_TERM]
		,getdate()
	    ,getdate()
	FROM ##TMP_Student 
;


-- Check Source Table Count
SELECT @source_cnt=COUNT(*) FROM ##TMP_Student;
	
-- Check Tgt Table Count
SELECT @target_cnt=COUNT(*) FROM RPDW_USR_STG.[dbo].DW_ENT_STUDENT;
    
-- Update Counts
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [SRC_CNT]=@source_cnt,
       [TGT_CNT]=@target_cnt,
	   [REMARKS]=@all_queries,
	   [FILENAME]=@outputfilename
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
    
    

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO RPDW_USR_STG.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on

-- Update SQL QUERIES EXECUTED
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [REMARKS]=@all_queries
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
   
END CATCH 
