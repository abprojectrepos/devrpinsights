      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
IF OBJECT_ID(N'tempdb..##TMP_Student') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_Student 
END

IF OBJECT_ID(N'tempdb..##TMP_TBL1') IS NOT NULL
BEGIN
