      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
IF OBJECT_ID(N'tempdb..##TMP_Student_Discipline') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_Student_Discipline
END

IF OBJECT_ID(N'tempdb..##TMP_TBL1') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_TBL1
END

DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;

SET @source_cnt=0;
SET @target_cnt=0;


select @jobstatusid=0;
select @date1=convert(varchar(10),GETDATE(),112);
select @outputfilename=CONCAT('DW_discipline_',@date1,'.txt');


SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_STG.[dbo].[ETL_JOB_PARAMS]  
WHERE JOB_NAME='DW_ENT_STUDENT_DISCIPLINE';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
; 

TRUNCATE TABLE RPDW_USR_STG.[dbo].DW_ENT_STUDENT_DISCIPLINE  
;

  
-- Begin SET
SET @query=
    N'SELECT 					
		 b.Student_ID							
		,b.Seq_num								
		,b.Offence_Date 
		,b.Offence_Time	
		,c.Offence_Lvl								
		,b.Offence_Code
		,ROW_NUMBER() OVER (PARTITION BY b.Student_ID,b.Seq_num, b.Offence_Date, b.Offence_Time, b.Offence_Code
			 ORDER BY  b.Student_ID,b.Seq_num, b.Offence_Date DESC, b.Offence_Time, b.Offence_Code) AS rec_cnt_by_pk
			 
	INTO ##TMP_TBL1
	
	FROM RPDW_STG.dbo.[DW_STUDENT_DISCIPLINARY_ACTION] b
	LEFT JOIN RPDW_STG.dbo.[DW_DISCIPLINARY_CASE_NUMBER] a 
	
		ON b.Student_ID = a.Student_ID
		AND b.Seq_num = a.Seq_Num
		AND b.Offence_Code = a.Offence_Code

	LEFT JOIN RPDW_STG.dbo.[DW_OFFENCE_CODE_SETUP] c 
		ON b.Offence_Code = c.Offence_Code
		
    '; -- End Set	
	
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);  
  EXEC (@query);



-- Begin SET
SET @query=CONCAT(
    N'SELECT 					
		 Student_ID							
		,Seq_num								
		,Offence_Date 
		,Offence_Time	
		,Offence_Lvl								
		,Offence_Code

	INTO ##TMP_Student_Discipline
	
	FROM ##TMP_TBL1
	
	WHERE rec_cnt_by_pk = 1
    ',@condition
    ); -- End Set	
	
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);  
  EXEC (@query);


  INSERT INTO RPDW_USR_STG.[dbo].DW_ENT_STUDENT_DISCIPLINE               
         (  
		 [EMPLID]	 							
		,[SEQNUM]									
		,[Offence_Date_Time]
		,[A_SDD_OFFENCE_LVL]							
		,[Offence_Code]
		,[CREATED_ON]
		,[MODIFIED_ON]
	     ) 
	SELECT 
		Student_ID				AS [EMPLID]	 							
		,Seq_num				AS [SEQNUM]									
		,CONCAT(FORMAT (Offence_Date,'dd/MM/yyyy'),N' ', Offence_Time)		AS [Offence_Date_Time]
		,Offence_Lvl			AS [A_SDD_OFFENCE_LVL]							
		,Offence_Code
		,getdate()
	    ,getdate()
	FROM ##TMP_Student_Discipline
;


-- Check Source Table Count
SELECT @source_cnt=COUNT(*) FROM ##TMP_Student_Discipline;
	
-- Check Tgt Table Count
SELECT @target_cnt=COUNT(*) FROM RPDW_USR_STG.[dbo].DW_ENT_STUDENT_DISCIPLINE;
    
-- Update Counts
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [SRC_CNT]=@source_cnt,
       [TGT_CNT]=@target_cnt,
	   [REMARKS]=@all_queries,
	   [FILENAME]=@outputfilename
   WHERE [JOB_STATUS_ID]=@jobstatusid;		
    

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO RPDW_USR_STG.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on

-- Update SQL QUERIES EXECUTED
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [REMARKS]=@all_queries
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
   
END CATCH 
