nce_Date, b.Offence_Time, b.Offence_Code&#xA;&#x9;&#x9;&#x9; ORDER BY  b.Student_ID,b.Seq_num, b.Offence_Date DESC, b.Offence_Time, b.Offence_Code) AS rec_cnt_by_pk&#xA;&#x9;&#x9;&#x9; &#xA;&#x9;INTO ##TMP_TBL1&#xA;&#x9;&#xA;&#x9;FROM &quot;+ @[$Project::RPDW_Initial_Catalog] +&quot;.dbo.[DW_STUDENT_DISCIPLINARY_ACTION] b&#xA;&#x9;LEFT JOIN &quot;+ @[$Project::RPDW_Initial_Catalog] +&quot;.dbo.[DW_DISCIPLINARY_CASE_NUMBER] a &#xA;&#x9;&#xA;&#x9;&#x9;ON b.Student_ID = a.Student_ID&#xA;&#x9;&#x9;AND b.Seq_num = a.Seq_Num&#xA;&#x9;&#x9;AND b.Offence_Code = a.Offence_Code&#xA;&#xA;&#x9;LEFT JOIN &quot;+ @[$Project::RPDW_Initial_Catalog] +&quot;.dbo.[DW_OFFENCE_CODE_SETUP] c &#xA;&#x9;&#x9;ON b.Offence_Code = c.Offence_Code&#xA;&#x9;&#x9;&#xA;    '; -- End Set&#x9;&#xA;&#x9;&#xA;  PRINT (@query);&#xA;  SET @all_queries=CONCAT(N'',@all_queries,@query);  &#xA;  EXEC (@query);&#xA;&#xA;&#xA;&#xA;-- Begin SET&#xA;SET @query=CONCAT(&#xA;    N'SELECT &#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9; Student_ID&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,Seq_num&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,Offence_Date &#xA;&#x9;&#x9;,Offence_Time&#x9;&#xA;&#x9;&#x9;,Offence_Lvl&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,Offence_Code&#xA;&#xA;&#x9;INTO ##TMP_Student_Discipline&#xA;&#x9;&#xA;&#x9;FROM ##TMP_TBL1&#xA;&#x9;&#xA;&#x9;WHERE rec_cnt_by_pk = 1&#xA;    ',@condition&#xA;    ); -- End Set&#x9;&#xA;&#x9;&#xA;  PRINT (@query);&#xA;  SET @all_queries=CONCAT(N'',@all_queries,@query);  &#xA;  EXEC (@query);&#xA;&#xA;&#xA;  INSERT INTO &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].&quot;+ @[User::TgtTableName]+&quot;               &#xA;         (  &#xA;&#x9;&#x9; [EMPLID]&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,[SEQNUM]&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,[Offence_Date_Time]&#xA;&#x9;&#x9;,[A_SDD_OFFENCE_LVL]&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,[Offence_Code]&#xA;&#x9;&#x9;,[CREATED_ON]&#xA;&#x9;&#x9;,[MODIFIED_ON]&#xA;&#x9;     ) &#xA;&#x9;SELECT &#xA;&#x9;&#x9;Student_ID&#x9;&#x9;&#x9;&#x9;AS [EMPLID]&#x9; &#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,Seq_num&#x9;&#x9;&#x9;&#x9;AS [SEQNUM]&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,CONCAT(FORMAT (Offence_Date,'dd/MM/yyyy'),N' ', Offence_Time)&#x9;&#x9;AS [Offence_Date_Time]&#xA;&#x9;&#x9;,Offence_Lvl&#x9;&#x9;&#x9;AS [A_SDD_OFFENCE_LVL]&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;,Offence_Code&#xA;&#x9;&#x9;,getdate()&#xA;&#x9;    ,getdate()&#xA;&#x9;FROM ##TMP_Student_Discipline&#xA;;&#xA;&#xA;&#xA;-- Check Source Table Count&#xA;SELECT @source_cnt=COUNT(*) FROM ##TMP_Student_Discipline;&#xA;&#x9;&#xA;-- Check Tgt Table Count&#xA;SELECT @target_cnt=COUNT(*) FROM &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].&quot;+ @[User::TgtTableName]+&quot;;&#xA;    &#xA;-- Update Counts&#xA;UPDATE &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].[ETL_JOB_STATUS] &#xA;   SET [SRC_CNT]=@source_cnt,&#xA;       [TGT_CNT]=@target_cnt,&#xA;&#x9;   [REMARKS]=@all_queries,&#xA;&#x9;   [FILENAME]=@outputfilename&#xA;   WHERE [JOB_STATUS_ID]=@jobstatusid;&#x9;&#x9;&#xA;    &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO &quot;+ @[$Project::RPDW_USR_Initial_Catalog] + &quot;.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;&#xA;-- Update SQL QUERIES EXECUTED&#xA;UPDATE &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].[ETL_JOB_STATUS] &#xA;   SET [REMARKS]=@all_queries&#xA;   WHERE [JOB_STATUS_ID]=@jobstatusid;&#x9;&#xA;   &#xA;END CATCH &#xA;GO&#xA;&quot;"
