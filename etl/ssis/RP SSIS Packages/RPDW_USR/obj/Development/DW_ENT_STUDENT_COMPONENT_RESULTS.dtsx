  SET @all_queries=CONCAT(N'',@all_queries,@query);  
  EXEC (@query);


-- Begin SET
SET @query=CONCAT(
    N'SELECT 	
		 Qual_Type
		,Student_Id
		,Acad_Year
		,Semester
		,Module_ID
		,Sub_Component_Name
		,Sub_Component_Full_Score
		,Sub_Component_Score_Raw_Score
		,Sub_Component_Grade_Raw_Grade
		,Sub_Component_Weighting
		,Semester_Code
		,Class_Code
		,Module_Code

	INTO ##TMP_Student_Component_Results
	
	FROM ##TMP_TBL1
	
	WHERE rec_cnt_by_pk = 1
	AND Qual_Type IN 
    ',@QUAL_TYPE,@condition
    ); -- End Set	
  ;
