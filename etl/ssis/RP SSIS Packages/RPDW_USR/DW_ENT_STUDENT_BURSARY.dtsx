IF OBJECT_ID(N'tempdb..##TMP_Student_Bursary') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_Student_Bursary
END


DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;

SET @source_cnt=0;
SET @target_cnt=0;


select @jobstatusid=0;
select @date1=convert(varchar(10),GETDATE(),112);
select @outputfilename=CONCAT('OASIS_SCH_BUR_',@date1,'.txt');


SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_PRD.[dbo].[ETL_JOB_PARAMS]  
WHERE JOB_NAME='DW_ENT_STUDENT_BURSARY';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
;  

TRUNCATE TABLE RPDW_USR_PRD.[dbo].DW_ENT_STUDENT_BURSARY  
;

-- Begin SET
SET @query=CONCAT(
    N'WITH TMP_Stud_Bursary AS (
	SELECT 
       [Qual_type]
      ,[Acad_Yr]
      ,[Sch_Ctr]
      ,[Student_ID]
      ,[App_ID]
      ,[App_Type]	
      ,[App_Type_Decr]
      ,[Schship_Bur_ID]
      ,[Schship_Bur_Name]
      ,[Amt]
      ,[App_Status]
      ,[App_Date]
	  ,ROW_NUMBER() OVER (PARTITION BY Acad_Yr, Sch_Ctr, Student_ID, App_ID, App_Type
							ORDER BY Acad_Yr, Sch_Ctr, Student_ID, App_ID, App_Type) as rec_cnt_by_pk					

    FROM RPDW_PRD.[dbo].[DW_SCHOLARSHIP_BURSARY_STATUS] 
    WHERE App_Type in (''APFP'', ''DFP'', ''SCH'', ''SPFP'')
    AND App_Status not in (''REJ'', ''RRP'', ''WDN'', ''WTD'', ''WTH'', ''AUS'')
    AND NOT (Qual_type=''DPFT'' AND  App_Type=''BUR'')
	)SELECT       
       [Qual_type]
      ,[Acad_Yr]
      ,[Sch_Ctr]
      ,[Student_ID]
      ,[App_ID]
      ,[App_Type]
      ,[App_Type_Decr]
      ,[Schship_Bur_ID]
      ,[Schship_Bur_Name]
      ,[Amt]
      ,[App_Status]
      ,[App_Date] 
	INTO ##TMP_Student_Bursary
	FROM TMP_Stud_Bursary 
    WHERE rec_cnt_by_pk = 1
    ',@condition
    ); -- End Set	
	
	 
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);
  EXEC (@query);	 
;

   INSERT INTO RPDW_USR_PRD.[dbo].DW_ENT_STUDENT_BURSARY                
         (  
		 [acadcareer]
		,[acadyear]
		,[school]
		,[studentid]
		,[applicationid]
		,[applicationtype]
		,[applicationtype_desc]
        ,[sch_bur_id]
		,[sch_bur_name]
		,[award_amt]
		,[application_status]
		,[application_date]
		,[CREATED_ON]
		,[MODIFIED_ON]
	     ) 
	SELECT 
	    CASE WHEN [App_Type] IN ('APFP','DFP','SPFP') THEN 'PFP' 
	         ELSE [Qual_Type] END as [acadcareer]
		,[Acad_Yr]	    as [acadyear]
		,[Sch_Ctr]	    as [school]
		,[Student_ID]	as [studentid]
		,[App_ID]		as [applicationid]
		,[App_Type]	    as [applicationtype]
		,[App_Type_Decr]		as [applicationtype_desc]
		,[Schship_Bur_ID]		as [sch_bur_id]
		,[Schship_Bur_Name]	    as [sch_bur_name]
		,[Amt]		    as [award_amt]
		,[App_Status]	as [application_status]
		,[App_Date]     as [application_date] 
		,getdate()
	    ,getdate()
	FROM ##TMP_Student_Bursary
;


