pp_Type) as rec_cnt_by_pk&#x9;&#x9;&#x9;&#x9;&#x9;&#xA;&#xA;    FROM &quot;+ @[$Project::RPDW_Initial_Catalog] +&quot;.[dbo].[DW_SCHOLARSHIP_BURSARY_STATUS] &#xA;    WHERE App_Type in (''APFP'', ''DFP'', ''SCH'', ''SPFP'')&#xA;    AND App_Status not in (''REJ'', ''RRP'', ''WDN'', ''WTD'', ''WTH'', ''AUS'')&#xA;    AND NOT (Qual_type=''DPFT'' AND  App_Type=''BUR'')&#xA;&#x9;)SELECT       &#xA;       [Qual_type]&#xA;      ,[Acad_Yr]&#xA;      ,[Sch_Ctr]&#xA;      ,[Student_ID]&#xA;      ,[App_ID]&#xA;      ,[App_Type]&#xA;      ,[App_Type_Decr]&#xA;      ,[Schship_Bur_ID]&#xA;      ,[Schship_Bur_Name]&#xA;      ,[Amt]&#xA;      ,[App_Status]&#xA;      ,[App_Date] &#xA;&#x9;INTO ##TMP_Student_Bursary&#xA;&#x9;FROM TMP_Stud_Bursary &#xA;    WHERE rec_cnt_by_pk = 1&#xA;    ',@condition&#xA;    ); -- End Set&#x9;&#xA;&#x9;&#xA;&#x9; &#xA;  PRINT (@query);&#xA;  SET @all_queries=CONCAT(N'',@all_queries,@query);&#xA;  EXEC (@query);&#x9; &#xA;;&#xA;&#xA;   INSERT INTO &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].&quot;+ @[User::TgtTableName]+&quot;                &#xA;         (  &#xA;&#x9;&#x9; [acadcareer]&#xA;&#x9;&#x9;,[acadyear]&#xA;&#x9;&#x9;,[school]&#xA;&#x9;&#x9;,[studentid]&#xA;&#x9;&#x9;,[applicationid]&#xA;&#x9;&#x9;,[applicationtype]&#xA;&#x9;&#x9;,[applicationtype_desc]&#xA;        ,[sch_bur_id]&#xA;&#x9;&#x9;,[sch_bur_name]&#xA;&#x9;&#x9;,[award_amt]&#xA;&#x9;&#x9;,[application_status]&#xA;&#x9;&#x9;,[application_date]&#xA;&#x9;&#x9;,[CREATED_ON]&#xA;&#x9;&#x9;,[MODIFIED_ON]&#xA;&#x9;     ) &#xA;&#x9;SELECT &#xA;&#x9;    CASE WHEN [App_Type] IN ('APFP','DFP','SPFP') THEN 'PFP' &#xA;&#x9;         ELSE [Qual_Type] END as [acadcareer]&#xA;&#x9;&#x9;,[Acad_Yr]&#x9;    as [acadyear]&#xA;&#x9;&#x9;,[Sch_Ctr]&#x9;    as [school]&#xA;&#x9;&#x9;,[Student_ID]&#x9;as [studentid]&#xA;&#x9;&#x9;,[App_ID]&#x9;&#x9;as [applicationid]&#xA;&#x9;&#x9;,[App_Type]&#x9;    as [applicationtype]&#xA;&#x9;&#x9;,[App_Type_Decr]&#x9;&#x9;as [applicationtype_desc]&#xA;&#x9;&#x9;,[Schship_Bur_ID]&#x9;&#x9;as [sch_bur_id]&#xA;&#x9;&#x9;,[Schship_Bur_Name]&#x9;    as [sch_bur_name]&#xA;&#x9;&#x9;,[Amt]&#x9;&#x9;    as [award_amt]&#xA;&#x9;&#x9;,[App_Status]&#x9;as [application_status]&#xA;&#x9;&#x9;,[App_Date]     as [application_date] &#xA;&#x9;&#x9;,getdate()&#xA;&#x9;    ,getdate()&#xA;&#x9;FROM ##TMP_Student_Bursary&#xA;;&#xA;&#xA;&#xA;-- Check Source Table Count&#xA;SELECT @source_cnt=COUNT(*) FROM ##TMP_Student_Bursary;&#xA;&#x9;&#xA;-- Check Tgt Table Count&#xA;SELECT @target_cnt=COUNT(*) FROM &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].&quot;+ @[User::TgtTableName]+&quot;;&#xA;    &#xA;-- Update Counts&#xA;UPDATE &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].[ETL_JOB_STATUS] &#xA;   SET [SRC_CNT]=@source_cnt,&#xA;       [TGT_CNT]=@target_cnt,&#xA;&#x9;   [REMARKS]=@all_queries,&#xA;&#x9;   [FILENAME]=@outputfilename&#xA;   WHERE [JOB_STATUS_ID]=@jobstatusid;&#x9;&#xA;    &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO &quot;+ @[$Project::RPDW_USR_Initial_Catalog] + &quot;.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;&#xA;-- Update SQL QUERIES EXECUTED&#xA;UPDATE &quot;+ @[$Project::RPDW_USR_Initial_Catalog] +&quot;.[dbo].[ETL_JOB_STATUS] &#xA;   SET [REMARKS]=@all_queries&#xA;   WHERE [JOB_STATUS_ID]=@jobstatusid;&#x9;&#xA;   &#xA;END CATCH &#xA;GO&#xA;&quot;"
