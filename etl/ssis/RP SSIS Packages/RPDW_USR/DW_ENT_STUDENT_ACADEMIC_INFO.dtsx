------------------------------------------------------------------------------------------
IF OBJECT_ID(N'tempdb..##TMP_Student_Academic_Info') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_Student_Academic_Info
END

IF OBJECT_ID(N'tempdb..##TMP_TBL1') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_TBL1
END

------------------------------------------------------------------------------------------
-- STORED VARIABLE DECLARATION AND SETTING OF INITIAL VALUES: DO NOT CHANGE ANYTHING 
------------------------------------------------------------------------------------------
DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;

SET @source_cnt=0;
SET @target_cnt=0;


select @jobstatusid=0;
select @date1=convert(varchar(10),GETDATE(),112);
select @outputfilename=CONCAT('DW_acadinfo_',@date1,'.txt');

------------------------------------------------------------------------------------------
-- EXTRACTION OF PRE-DEFINED FILTER CRITERIA FROM ETL_PARM: DO NOT CHANGE ANYTHING 
------------------------------------------------------------------------------------------
SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_STG.[dbo].[ETL_JOB_PARAMS]  
WHERE JOB_NAME='DW_ENT_STUDENT_ACADEMIC_INFO';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
;  

TRUNCATE TABLE RPDW_USR_STG.[dbo].DW_ENT_STUDENT_ACADEMIC_INFO  
;

------------------------------------------------------------------------------------------
-- MAIN EXTRACTION QUERY: 
-- UPDATE THE FOLLOWING ACCORDINGLY
--   (1) COLUMN NAME1 TO COLUMN NAME 4
--   (2) SOURCE TABLE NAME
-- Note: DO NOT CHANGE THE WHERE CLAUSE. IF NEED TO SUBSET, PUT THE CONDITION IN THE ETL_PARM TABLE,
--       AND THIS QUERY WILL APPEND THE CONDITION ACCORDINGLY.
------------------------------------------------------------------------------------------
   
-- Begin SET
SET @query=
    N'SELECT 	
	     Student_ID
        ,Qualification_Type
		,Term
		,Programme
		,Reqmt_Term
		,Semester_Status
		,Term_GPA
		,Cumulative_GPA		
		,ROW_NUMBER() OVER (PARTITION BY Qualification_Type, Student_ID, Term
							ORDER BY Qualification_Type, Student_ID, Term) as rec_cnt_by_pk
	  
	INTO ##TMP_TBL1
	
	FROM RPDW_STG.[dbo].[V_DW_STUDENT_SEMESTER_DETAILS]
    '; -- End Set	
	
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);  
  EXEC (@query);



-- Begin SET
SET @query=CONCAT(
    N'SELECT 	
	     Student_ID
        ,Qualification_Type
		,Term
		,Programme
		,Reqmt_Term
		,Semester_Status
		,Term_GPA
		,Cumulative_GPA		
		
	INTO ##TMP_Student_Academic_Info
	
	FROM ##TMP_TBL1
	
	WHERE rec_cnt_by_pk = 1
	AND Qualification_Type IN 
    ',@QUAL_TYPE,@condition
    ); -- End Set	
	
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);  
  EXEC (@query);

------------------------------------------------------------------------------------------
-- MAIN INSERTION QUERY: 
-- UPDATE THE FOLLOWING ACCORDINGLY
--   (1) COLUMN NAME1 TO COLUMN NAME 4
--   (2) SOURCE TABLE NAME
-- Note: DO NOT CHANGE THE WHERE CLAUSE. IF NEED TO SUBSET, PUT THE CONDITION IN THE ETL_PARM TABLE,
--       AND THIS QUERY WILL APPEND THE CONDITION ACCORDINGLY.
------------------------------------------------------------------------------------------
  
  INSERT INTO RPDW_USR_STG.[dbo].DW_ENT_STUDENT_ACADEMIC_INFO               
         (  
		 [Student_ID]
		,[Qual_Type]
		,[Acad_Term]
		,[Programme]
		,[Req_Term]
		,[Semester_Status]
		,[Term_GPA]
		,[Cumulative_GPA]	 
		,[CREATED_ON]
		,[MODIFIED_ON]
	     ) 
	SELECT 
		 Student_ID
		,Qualification_Type	as [Qual_Type]
		,Term				as [Acad_Term]
		,Programme
		,Reqmt_Term			as [Req_Term]
		,Semester_Status
		,Term_GPA
		,Cumulative_GPA
		,getdate()
	    ,getdate()
	FROM ##TMP_Student_Academic_Info
;

------------------------------------------------------------------------------------------
-- SOURCE AND TARGET TABLE COUNTS UPDATE: DO NOT CHANGE ANYTHING 
------------------------------------------------------------------------------------------
-- Check Source Table Count
SELECT @source_cnt=COUNT(*) FROM ##TMP_Student_Academic_Info;
	
-- Check Tgt Table Count
SELECT @target_cnt=COUNT(*) FROM RPDW_USR_STG.[dbo].DW_ENT_STUDENT_ACADEMIC_INFO;
    
-- Update Counts
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [SRC_CNT]=@source_cnt,
       [TGT_CNT]=@target_cnt,
	   [REMARKS]=@all_queries,
	   [FILENAME]=@outputfilename
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
    
    

