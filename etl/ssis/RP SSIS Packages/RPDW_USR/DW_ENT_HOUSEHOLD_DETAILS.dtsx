BEGIN TRY
------------
IF OBJECT_ID(N'tempdb..##TMP_Household_Details') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_Household_Details
END

IF OBJECT_ID(N'tempdb..##TMP_TBL1') IS NOT NULL
BEGIN
  DROP TABLE ##TMP_TBL1
END

DECLARE @jobstatusid varchar(5)
,@condition varchar(100)
,@filter_criteria varchar(100)
,@QUAL_TYPE varchar(100)
,@query nvarchar(max)
,@job_name varchar(100)
,@source_cnt INT
,@target_cnt INT
,@all_queries varchar(4000)
,@outputfilename varchar(200)
,@date1 varchar(10)
;

SET @source_cnt=0;
SET @target_cnt=0;


select @jobstatusid=0;
select @date1=convert(varchar(10),GETDATE(),112);
select @outputfilename=CONCAT('DW_household_details_',@date1,'.txt');


SELECT @filter_criteria=FILTER_CRITERIA, @QUAL_TYPE=QUAL_TYPE_CRITERIA FROM RPDW_USR_STG.[dbo].[ETL_JOB_PARAMS]  
WHERE JOB_NAME='DW_ENT_HOUSEHOLD_DETAILS';

IF  @filter_criteria='FULL-LOAD' 
  BEGIN
     SET @condition=';';
  END
ELSE IF @filter_criteria&lt;&gt;'' OR ISNULL(@filter_criteria,N'')&lt;&gt;'' 
    BEGIN
	    SET @condition=CONCAT('AND ',@filter_criteria,';');
    END
; 

TRUNCATE TABLE RPDW_USR_STG.[dbo].DW_ENT_HOUSEHOLD_DETAILS  
;

  
-- Begin SET
SET @query=CONCAT(
    N'SELECT 
		 Applicant_ID
		,No_of_Siblings
		,Household_Size
		,Type_of_residence
		,House_Ownership
		,Total_Mth_Family_Household_Income
		,Place_of_Birth  
	INTO ##TMP_Household_Details
	FROM RPDW_STG.[dbo].[DW_APPLICANT_HOUSEHOLD_DETAILS]
	WHERE 1=1
    ',@condition
    ); -- End Set	
	
  PRINT (@query);
  SET @all_queries=CONCAT(N'',@all_queries,@query);
  EXEC (@query);

  
  INSERT INTO RPDW_USR_STG.[dbo].DW_ENT_HOUSEHOLD_DETAILS               
         (  
		 [EMPLID]
		,[A_HOUSEHOLD_SIBLIN]
		,[A_HOUSEHOLD_MBR]
		,[HOUSEHOLD_TYPE]
		,[A_HOUSEHOLD_OWNER]
		,[A_HOUSEHOLD_INCOME]
		,[BIRTHCOUNTRY] 
		,[CREATED_ON]
		,[MODIFIED_ON]
	     ) 
	SELECT 
		 Applicant_ID						AS [EMPLID]
		,No_of_Siblings						AS [A_HOUSEHOLD_SIBLIN]
		,Household_Size						AS [A_HOUSEHOLD_MBR]
		,Type_of_residence					AS [HOUSEHOLD_TYPE]
		,House_Ownership					AS [A_HOUSEHOLD_OWNER]
		,Total_Mth_Family_Household_Income	AS [A_HOUSEHOLD_INCOME]
		,Place_of_Birth						AS [BIRTHCOUNTRY]
		,getdate()
	    ,getdate()
	FROM ##TMP_Household_Details
;


-- Check Source Table Count
SELECT @source_cnt=COUNT(*) FROM ##TMP_Household_Details;
	
-- Check Tgt Table Count
SELECT @target_cnt=COUNT(*) FROM RPDW_USR_STG.[dbo].DW_ENT_HOUSEHOLD_DETAILS;
    
-- Update Counts
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [SRC_CNT]=@source_cnt,
       [TGT_CNT]=@target_cnt,
	   [REMARKS]=@all_queries,
	   [FILENAME]=@outputfilename
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
    

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO RPDW_USR_STG.[dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on

-- Update SQL QUERIES EXECUTED
UPDATE RPDW_USR_STG.[dbo].[ETL_JOB_STATUS] 
   SET [REMARKS]=@all_queries
   WHERE [JOB_STATUS_ID]=@jobstatusid;	
   
END CATCH 
GO
