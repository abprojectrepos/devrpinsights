    <DTS:Executable
      DTS:refId="Package\PSTG Primary Key Validation"
      DTS:CreationName="Microsoft.ExecuteSQLTask"
      DTS:Description="Execute SQL Task"
      DTS:DTSID="{2BA0CAAA-68BE-414E-8D6B-77F44F2A5E91}"
      DTS:ExecutableType="Microsoft.ExecuteSQLTask"
      DTS:LocaleID="-1"
      DTS:ObjectName="PSTG Primary Key Validation"
      DTS:ThreadHint="0">
      <DTS:Variables />
      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{990C886A-A439-47E5-988C-669F737C6B13}"
          SQLTask:SqlStatementSource="&#xA;&#xA;WITH PSTG_VALIDATION&#xA;&#xA;AS&#xA;(&#xA;SELECT  REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Module_ID])),''),'NULL','') [Module_ID],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Term])),''),'NULL','') [Term],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Lecturer_ID])),''),'NULL','') [Lecturer_ID],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Acad_Day])),''),'NULL','') [Acad_Day],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [RP_Class])),''),'NULL','') [RP_Class],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Class_ID])),''),'NULL','') [Class_ID]&#xA;&#xA;&#xA;FROM [PSTG_EXT_CLASS_SCHEDULE])&#xA;&#xA;, PSTG_VALIDATION1 &#xA;AS&#xA;(&#xA;SELECT REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Module_ID])),''),'NULL','') [Module_ID],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Term])),''),'NULL','') [Term],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Lecturer_ID])),''),'NULL','') [Lecturer_ID],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Acad_Day])),''),'NULL','') [Acad_Day],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [RP_Class])),''),'NULL','') [RP_Class],&#xA;REPLACE (ISNULL(TRIM(TRIM('&quot;' FROM [Class_ID])),''),'NULL','') [Class_ID]&#xA;&#xA;&#xA;FROM  [PSTG_EXT_CLASS_SCHEDULE])&#xA;SELECT&#xA;CASE WHEN EXISTS (SELECT  COUNT(1) COUNTOFPK, [Module_ID],[Term],[Lecturer_ID],[Acad_Day],[Class_ID],[RP_Class]&#xA;FROM PSTG_VALIDATION&#xA;GROUP&#xA;BY  [Module_ID],[Term],[Lecturer_ID],[Acad_Day],[Class_ID],[RP_Class]&#xA;HAVING COUNT(1)&gt;1)&#xA;THEN 1 &#xA;ELSE 0  &#xA;END NoDuplicates&#xA;,&#xA; CASE WHEN EXISTS(select * from  PSTG_VALIDATION1&#xA;where ([Module_ID]='' or [Module_ID] is null OR [Term] IS NULL OR [Term]='' &#xA;--OR [Lecturer_ID]=''&#xA; OR [Lecturer_ID] IS NULL OR [Acad_Day] IS NULL OR [Acad_Day]=''&#xA;OR [Class_ID] IS NULL OR [Class_ID]='' or [RP_Class] is null or [RP_Class]=''))&#xA;THEN 1 &#xA;ELSE 0&#xA;END  as MandColValidation&#xA;&#xA;&#xA;&#xA;"
          SQLTask:ResultType="ResultSetType_SingleRow" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask">
          <SQLTask:ResultBinding
            SQLTask:ResultName="NoDuplicates"
            SQLTask:DtsVariableName="User::sqlprimarykeycheck" />
          <SQLTask:ResultBinding
            SQLTask:ResultName="MandColValidation"
            SQLTask:DtsVariableName="User::MndColumncheck" />
        </SQLTask:SqlTaskData>
      </DTS:ObjectData>
    </DTS:Executable>
