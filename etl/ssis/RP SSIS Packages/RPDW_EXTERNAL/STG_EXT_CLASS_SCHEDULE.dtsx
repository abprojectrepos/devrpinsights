      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
--------------DECLARE PROCESS DATE--------
DECLARE @PROCESSDATE DATETIME
,@jobstatusid varchar(5)
SELECT @PROCESSDATE=GETDATE()
select @jobstatusid=0
--------------------------------------


INSERT INTO 
[dbo].STG_EXT_CLASS_SCHEDULE
(
[Inst]
      ,[Qual_Type]
      ,[Term]
      ,[Sch_Ctr_of_Module]
      ,[Acad_Day]
      ,[Module_ID]
      ,[Lecturer_ID]
      ,[Module_Code]
      ,[Module_Desc]
      ,[Class_ID]
      ,[RP_Class]
      ,[Lecturer_Name]
      ,[Sch_of_Lecturer]
      ,[Lecturer_Type]
      ,[Class_Capacity]
      ,[Class_Vacancy]
,[Latest_Ind]
,[Created_On]
,[Modified_On])
SELECT 
[Inst]
      ,[Qual_Type]
      ,[Term]
      ,[Sch_Ctr_of_Module]
      ,[Acad_Day]
      ,[Module_ID]
      ,[Lecturer_ID]
      ,[Module_Code]
      ,[Module_Desc]
      ,[Class_ID]
      ,[RP_Class]
      ,[Lecturer_Name]
      ,[Sch_of_Lecturer]
      ,[Lecturer_Type]
      ,[Class_Capacity]
      ,[Class_Vacancy]
,'Y', GETDATE(),GETDATE()
FROM
   (
   MERGE INTO STG_EXT_CLASS_SCHEDULE AS dst
   USING TMP_STG_EXT_CLASS_SCHEDULE AS src
   ON( dst.module_id=src.module_id and src.Term=dst.Term AND
   dst.Acad_Day=src.Acad_Day and src.Module_ID=dst.Module_ID  AND
   dst.Lecturer_ID=src.Lecturer_ID 
   and    dst.RP_Class=src.RP_Class 

    )
    
	
   WHEN NOT MATCHED THEN
   INSERT
   ( [Inst]
      ,[Qual_Type]
      ,[Term]
      ,[Sch_Ctr_of_Module]
      ,[Acad_Day]
      ,[Module_ID]
      ,[Lecturer_ID]
      ,[Module_Code]
      ,[Module_Desc]
      ,[Class_ID]
      ,[RP_Class]
      ,[Lecturer_Name]
      ,[Sch_of_Lecturer]
      ,[Lecturer_Type]
      ,[Class_Capacity]
      ,[Class_Vacancy]
      ,[Latest_Ind]
      ,[Created_On]
      ,[Modified_On])
   VALUES(
 src.[Inst]
,src.[Qual_Type]
,src.[Term]
,src.[Sch_Ctr_of_Module]
,src.[Acad_Day]
,src.[Module_ID]
,src.[Lecturer_ID]
,src.[Module_Code]
,src.[Module_Desc]
,src.[Class_ID]
,src.[RP_Class]
,src.[Lecturer_Name]
,src.[Sch_of_Lecturer]
,src.[Lecturer_Type]
,src.[Class_Capacity]
,src.[Class_Vacancy]
 ,'Y', GETDATE(),GETDATE())
   WHEN MATCHED
   AND dst.Latest_Ind='Y'
   and( 


isnull(src.[Inst],'')&lt;&gt; isnull(dst.[Inst],'') OR 
isnull(src.[Qual_Type],'')&lt;&gt; isnull(dst.[Qual_Type],'') OR 
isnull(src.[Sch_Ctr_of_Module],'')&lt;&gt; isnull(dst.[Sch_Ctr_of_Module],'') OR 
isnull(src.[Module_Code],'')&lt;&gt; isnull(dst.[Module_Code],'') OR 
isnull(src.[Module_Desc],'')&lt;&gt; isnull(dst.[Module_Desc],'') OR 
isnull(src.[Lecturer_Name],'')&lt;&gt; isnull(dst.[Lecturer_Name],'') OR 
isnull(src.[Sch_of_Lecturer],'')&lt;&gt; isnull(dst.[Sch_of_Lecturer],'') OR 
isnull(src.[Lecturer_Type],'')&lt;&gt; isnull(dst.[Lecturer_Type],'') OR 
isnull(src.[Class_Capacity],'')&lt;&gt; isnull(dst.[Class_Capacity],'') OR 
isnull(src.[Class_Vacancy],'')&lt;&gt; isnull(dst.[Class_Vacancy],'')  

      )
   THEN UPDATE
   SET DST.Latest_Ind = 'N', Modified_On=GETDATE()
OUTPUT    
   
src.[Inst]
,src.[Qual_Type]
,src.[Term]
,src.[Sch_Ctr_of_Module]
,src.[Acad_Day]
,src.[Module_ID]
,src.[Lecturer_ID]
,src.[Module_Code]
,src.[Module_Desc]
,src.[Class_ID]
,src.[RP_Class]
,src.[Lecturer_Name]
,src.[Sch_of_Lecturer]
,src.[Lecturer_Type]
,src.[Class_Capacity]
,src.[Class_Vacancy],  $Action AS MergeAction
) AS MRG
WHERE MRG.MergeAction = 'UPDATE'
;
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO RPDW..ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@PROCESSDATE  as created_on
  
  
END CATCH  
GO</DTS:VariableValue>
