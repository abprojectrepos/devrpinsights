      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
--------------------------------------

declare @MandColValidation int,
@NoDuplicates int,
@Job_Status_ID int
SELECT  @NoDuplicates=?
SELECT @MandColValidation=?
SELECT @Job_Status_ID=?

if(@NoDuplicates&lt;&gt;0 or @MandColValidation&lt;&gt;0)

begin 
insert into [dbo].[ETL_ERROR_LOG](JOB_STATUS_ID,ERROR_RECORD,created_on) 
SELECT  @Job_Status_ID JOB_STATUS_ID,''+'Module_ID ='+[Module_ID]+', Term ='+[Term]+',
Lecturer_ID ='+[Lecturer_ID]+',
Acad_Day ='+[Acad_Day]+',
RP_Class ='+[RP_Class]+',
[Class_ID] ='+[Class_ID]+',
is duplicate primary key in source file' as ErrorColValues,GETDATE() AS CREATED_ON
FROM RPDW_PSTG.[dbo].[PSTG_EXT_CLASS_SCHEDULE]
GROUP
BY   [Module_ID],[Term],[Lecturer_ID],Acad_Day,[Class_ID],RP_Class
having count(1)&gt;1
union all
SELECT  @Job_Status_ID JOB_STATUS_ID,''+'Module_ID ='+[Module_ID]+', Term ='+[Term]+',
Lecturer_ID ='+[Lecturer_ID]+',
Acad_Day ='+[Acad_Day]+',
RP_Class ='+[RP_Class]+',
[Class_ID] ='+[Class_ID]+', is Violation of Mandatory key' as ErrorColValues,GETDATE() AS CREATED_ON
FROM RPDW_PSTG.[dbo].[PSTG_EXT_CLASS_SCHEDULE]
where  ([Module_ID]='' or [Module_ID] is null OR [Term] IS NULL OR [Term]='' 
--OR [Lecturer_ID]=''
 OR [Lecturer_ID] IS NULL OR [Acad_Day] IS NULL OR [Acad_Day]=''
OR [Class_ID] IS NULL OR [Class_ID]='' or [RP_Class] is null or [RP_Class]=''
  )
GROUP BY   [Module_ID],[Term],[Lecturer_ID],Acad_Day,[Class_ID],RP_Class
END
else
select 'Unknown Error','Check the file and rerun the package'


IF @NoDuplicates&lt;&gt;0

UPDATE [dbo].[ETL_JOB_STATUS]
SET [JOB_STATUS]='Fail',
[ERROR_MSG]='Duplicate primary key found in PSTG table'
WHERE JOB_STATUS_ID=@Job_Status_ID

ELSE IF @MandColValidation&lt;&gt;0
UPDATE [dbo].[ETL_JOB_STATUS]
SET [JOB_STATUS]='Fail',
[ERROR_MSG]='Mandatory Column constraint failed in PSTG table'
WHERE JOB_STATUS_ID=@Job_Status_ID

ELSE
SELECT 'UNKNOWN'


--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
END CATCH  
GO</DTS:VariableValue>
