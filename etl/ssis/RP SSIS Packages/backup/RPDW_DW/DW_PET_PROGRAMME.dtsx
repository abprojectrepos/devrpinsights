      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate DATETIME
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
SELECT @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') FROM [dbo].[DW_PET_PROGRAMME]
SELECT  @processeddatetime= GETDATE()



;WITH TMP_DW_PET_PROGRAMME
([School_Centre]
,[Prog_Chair_ID]
,[NMC_Code]
,[Prog_Code]
,[Prog_Descr]
,[Qual_Type]
,[Qual_Type_Descr]
,[Effective_Date])
AS
(
SELECT
[School_Centre]
,[Pc_Id] as Prog_Chair_ID
,[Nmc_Code] AS NMC_Code
,[Prog_Code]
,[Prog_Formal_Descr] as Prog_Descr
,[Qual_Type]
,MC.Long_Name AS Qual_Type_Descr
,PT.[Effective_Date]
FROM RPDW_STG.[DBO].[STG_OASIS_PROG_TABLE] PT
LEFT JOIN RPDW_STG.dbo.[STG_OASIS_MASTER_CODE] MC
ON PT.[QUAL_TYPE] =MC.FIELD_VALUE AND MC.FIELD_NAME='ACAD_CAREER' AND MC.EFFECTIVE_STATUS = 'A'
WHERE PT.LATEST_IND='Y'  and PT.[Modified_On]&gt;@LastDWProcessdate)



MERGE INTO DW_PET_PROGRAMME AS dst
   USING TMP_DW_PET_PROGRAMME AS src
   ON( dst.[Effective_Date]=src.[Effective_Date]
   AND dst.Qual_Type=src.Qual_Type
   	 AND dst.[Prog_Code]=src.[Prog_Code])

   WHEN NOT MATCHED THEN
   INSERT
	  (
[School_Centre]
,[Prog_Chair_ID]
,[NMC_Code]
,[Prog_Code]
,[Prog_Descr]
,[Qual_Type]
,[Qual_Type_Descr]
,[Effective_Date]
,[CREATED_ON]
,[MODIFIED_ON])
   VALUES(  
 src.[School_Centre]
,src.[Prog_Chair_ID]
,src.[NMC_Code]
,src.[Prog_Code]
,src.[Prog_Descr]
,src.[Qual_Type]
,src.[Qual_Type_Descr]
,src.[Effective_Date]
,@processeddatetime,
@processeddatetime)
   WHEN MATCHED
   
   THEN UPDATE
   SET 	 
   
 dst.[School_Centre]=src.[School_Centre]
,dst.[Prog_Chair_ID]=src.[Prog_Chair_ID]
,dst.[NMC_Code]=src.[NMC_Code]
--,dst.[Prog_Code]=src.[Prog_Code]
,dst.[Prog_Descr]=src.[Prog_Descr]
--,dst.[Qual_Type]=src.[Qual_Type]
,dst.[Qual_Type_Descr]=src.[Qual_Type_Descr]
--,dst.[Effective_Date]=src.[Effective_Date]
,dst.Modified_On=@processeddatetime;
		

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
  
END CATCH  
GO</DTS:VariableValue>
