      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_STUDENT_ID_MAPPING
select  @processeddatetime= getdate()
--select @LastDWProcessdate
;WITH TMP_DW_STUDENT_ID_MAPPING
AS
(SELECT [Student_ID]
      ,[Qual_Type]
      ,[Student_Career_Number]
      ,[Application_Nbr]
      ,[Admit_Type]
      ,[UIN]
      FROM RPDW_STG.[dbo].[STG_EXT_STUDENT_ID_UIN_MAPPING]
  where latest_ind='Y'
AND [Modified_On]&gt;@LastDWProcessdate
)


MERGE INTO DW_STUDENT_ID_MAPPING AS dst
   USING TMP_DW_STUDENT_ID_MAPPING AS src
   ON( dst.[Student_ID]=src.[Student_ID]
   AND dst.[Application_Nbr]=src.[Application_Nbr]
     )

   WHEN NOT MATCHED THEN
   INSERT
(
[Student_ID]
      ,[Qual_Type]
      ,[Student_Career_Number]
      ,[Application_Nbr]
      ,[Admit_Type]
      ,[UIN]
,[CREATED_ON]
,[MODIFIED_ON])
VALUES(  
 SRC.[Student_ID]
,SRC.[Qual_Type]
,SRC.[Student_Career_Number]
,SRC.[Application_Nbr]
,SRC.[Admit_Type]
,SRC.[UIN]
,  @processeddatetime,@processeddatetime)
   WHEN MATCHED
   
   THEN UPDATE
   SET 	 

DST.[Qual_Type] = SRC.[Qual_Type]
,DST.[Student_Career_Number] = SRC.[Student_Career_Number]
,DST.[Admit_Type] = SRC.[Admit_Type]
,DST.[UIN] = SRC.[UIN]
 ,dst.Modified_On=@processeddatetime;
		

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
  
END CATCH  
GO</DTS:VariableValue>
