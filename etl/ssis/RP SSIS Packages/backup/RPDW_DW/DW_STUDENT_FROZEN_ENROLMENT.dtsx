#xA;   VALUES(  &#xA;&#x9;   src.[Student_Id]&#xA;&#x9;   ,src.[Admit_Term]&#xA;&#x9;   ,src.[Frozen_Date]&#xA;&#x9;   ,src.[Acad_Prog]&#xA;&#x9;   ,src.School&#xA;&#x9;   ,frozen_intake_term&#xA;&#x9;&#x9;,  @processeddatetime,@processeddatetime&#xA;&#x9;&#x9;,src.Programme_Desc)&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET &#x9;dst.[Acad_Prog]=src.[Acad_Prog]&#xA;&#x9;&#x9;,dst.frozen_intake_term=src.frozen_intake_term&#xA;&#x9;&#x9;,dst.school=src.School&#xA;&#x9;&#x9;,dst.Programme_Desc=src.Programme_Desc&#xA;,dst.Modified_On=@processeddatetime;&#xA;&#x9;&#x9;&#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,@processeddatetime  as created_on&#xA;  &#xA;  &#xA;END CATCH  &#xA;GO&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="InsertQuery">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------


DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_STUDENT_FROZEN_ENROLMENT
select  @processeddatetime= getdate()
--select @LastDWProcessdate

;with TMP_OASIS_STUDENT_FROZEN_ENROLMENT ([Student_Id]
      ,[Admit_Term]
      ,[Frozen_Date]
      ,[Acad_Prog]
	  ,School
	  ,frozen_intake_term
	  ,Programme_Desc
      )
	  as
  (select [Student_Id]
      ,[Admit_Term]
      ,[Frozen_Date]
      ,[Acad_Prog]
	    ,School_Centre AS School
	       ,[Admit_Term] as frozen_intake_term
		   ,PT1.Prog_Formal_Descr
       from RPDW_STG.[dbo].[STG_OASIS_STUDENT_FROZEN_ENROLMT]
	    FE LEFT JOIN 
	   (SELECT  Prog_Code,Prog_Formal_Descr,School_Centre,ROW_NUMBER()OVER( PARTITION BY Prog_Code ORDER BY EFFECTIVE_DATE DESC)ROW_NUM 
FROM RPDW_STG.[dbo].STG_OASIS_PROG_TABLE) PT1 ON PT1.ROW_NUM=1 and PT1.Prog_Code=FE.Acad_Prog
	  where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate
)

--select * from  TMP_OASIS_STUDENT_FROZEN_ENROLMENT

MERGE INTO DW_STUDENT_FROZEN_ENROLMENT AS dst
   USING TMP_OASIS_STUDENT_FROZEN_ENROLMENT AS src
   ON( dst.[Admit_Term]=src.[Admit_Term] and dst.[Student_Id]=src.[Student_Id] 
   and dst.Frozen_Date=src.Frozen_Date)

   WHEN NOT MATCHED THEN
   INSERT
	  ([Student_Id]
      ,[Admit_Term]
      ,[Frozen_Date]
      ,[Acad_Prog]
	  ,School
	  ,frozen_intake_term
           ,[Created_On]
      ,[Modified_On]
	  ,Programme_Desc)
   VALUES(  
	   src.[Student_Id]
	   ,src.[Admit_Term]
	   ,src.[Frozen_Date]
	   ,src.[Acad_Prog]
	   ,src.School
	   ,frozen_intake_term
		,  @processeddatetime,@processeddatetime
		,src.Programme_Desc)
   WHEN MATCHED
   
   THEN UPDATE
   SET 	dst.[Acad_Prog]=src.[Acad_Prog]
		,dst.frozen_intake_term=src.frozen_intake_term
		,dst.school=src.School
		,dst.Programme_Desc=src.Programme_Desc
,dst.Modified_On=@processeddatetime;
		

--------------------------------------
COMMIT TRANSACTION
END TRY
BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
  
END CATCH  
GO</DTS:VariableValue>
