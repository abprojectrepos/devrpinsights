      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(5)&#xA;select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_CET_APPLICANT_INFO&#xA;select  @processeddatetime= getdate()&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JobStatusId] +&quot;&#xA;--select @LastDWProcessdate&#xA;&#xA;;WITH TMP_DW_CET_APPLICANT_INFO&#xA;AS&#xA;(&#xA;SELECT &#xA;AD.Applicant_ID&#xA;,No_of_Siblings&#xA;,Household_Size&#xA;,Type_of_residence&#xA;,House_Ownership&#xA;,Total_Mth_Family_Household_Income&#xA;,Place_of_Birth&#xA;,ISNULL(NAME,'NA') AS Emergency_Contact_Person&#xA;,ISNULL(Relationship,'NA')Relationship&#xA;,Contact_Number&#xA;FROM &quot;+@[$Project::Staging_Initial_Catalog]+&quot;..STG_OASIS_CET_APPLICANT_DATA AD&#xA;LEFT JOIN &quot;+@[$Project::Staging_Initial_Catalog]+&quot;.[dbo].[STG_OASIS_CET_EMERGENCY_CONTACT] EC&#xA;ON EC.Applicant_ID=AD.Applicant_ID AND EC.Latest_Ind='Y'&#xA;WHERE AD.LATEST_IND='Y'&#xA;and AD.[Modified_On]&gt;@LastDWProcessdate&#xA;)&#xA;MERGE INTO DW_CET_APPLICANT_INFO AS dst&#xA;   USING TMP_DW_CET_APPLICANT_INFO AS src&#xA;   ON( dst.[Applicant_ID]=src.[Applicant_ID] and dst.[Emergency_Contact_Person]=src.[Emergency_Contact_Person] and dst.[Relationship]=src.[Relationship]  )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT&#xA;&#x9;  ([Applicant_ID]&#xA;      ,[No_of_Siblings]&#xA;      ,[Household_Size]&#xA;      ,[Type_of_residence]&#xA;      ,[House_Ownership]&#xA;      ,[Total_Mth_Family_Household_Income]&#xA;      ,[Place_of_Birth]&#xA;      ,[Emergency_Contact_Person]&#xA;      ,[Relationship]&#xA;      ,[Contact_Number]&#xA;      ,[CREATED_ON]&#xA;      ,[MODIFIED_ON])&#xA;   VALUES(  &#xA;&#x9;   SRC.[Applicant_ID]&#xA;      ,SRC.[No_of_Siblings]&#xA;      ,SRC.[Household_Size]&#xA;      ,SRC.[Type_of_residence]&#xA;      ,SRC.[House_Ownership]&#xA;      ,SRC.[Total_Mth_Family_Household_Income]&#xA;      ,SRC.[Place_of_Birth]&#xA;      ,SRC.[Emergency_Contact_Person]&#xA;      ,SRC.[Relationship]&#xA;      ,SRC.[Contact_Number]&#xA;      ,@processeddatetime&#xA;&#x9;  ,@processeddatetime)&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET &#x9;&#xA;   &#xA;    dst.[Contact_Number]=src.[Contact_Number]&#xA;   ,dst.[No_of_Siblings]=src.[No_of_Siblings]&#xA;   ,dst.[Household_Size]=src.[Household_Size]&#xA;   ,dst.[Type_of_residence]=src.[Type_of_residence]&#xA;   ,dst.[House_Ownership]=src.[House_Ownership]&#xA;   ,dst.[Total_Mth_Family_Household_Income]=src.[Total_Mth_Family_Household_Income]&#xA;   ,dst.[Place_of_Birth]=src.[Place_of_Birth]&#xA;   &#x9;&#x9;&#xA;,dst.Modified_On=@processeddatetime;&#xA;&#x9;&#x9;&#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,@processeddatetime  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="InsertQuery">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_CET_APPLICANT_INFO
select  @processeddatetime= getdate()
select @jobstatusid=0
--select @LastDWProcessdate

;WITH TMP_DW_CET_APPLICANT_INFO
AS
(
SELECT 
AD.Applicant_ID
,No_of_Siblings
,Household_Size
,Type_of_residence
,House_Ownership
,Total_Mth_Family_Household_Income
,Place_of_Birth
,ISNULL(NAME,'NA') AS Emergency_Contact_Person
,ISNULL(Relationship,'NA')Relationship
,Contact_Number
FROM RPDW_STG..STG_OASIS_CET_APPLICANT_DATA AD
LEFT JOIN RPDW_STG.[dbo].[STG_OASIS_CET_EMERGENCY_CONTACT] EC
ON EC.Applicant_ID=AD.Applicant_ID AND EC.Latest_Ind='Y'
WHERE AD.LATEST_IND='Y'
and AD.[Modified_On]&gt;@LastDWProcessdate
)
MERGE INTO DW_CET_APPLICANT_INFO AS dst
   USING TMP_DW_CET_APPLICANT_INFO AS src
   ON( dst.[Applicant_ID]=src.[Applicant_ID] and dst.[Emergency_Contact_Person]=src.[Emergency_Contact_Person] and dst.[Relationship]=src.[Relationship]  )

   WHEN NOT MATCHED THEN
   INSERT
	  ([Applicant_ID]
      ,[No_of_Siblings]
      ,[Household_Size]
      ,[Type_of_residence]
      ,[House_Ownership]
      ,[Total_Mth_Family_Household_Income]
      ,[Place_of_Birth]
      ,[Emergency_Contact_Person]
      ,[Relationship]
      ,[Contact_Number]
      ,[CREATED_ON]
      ,[MODIFIED_ON])
   VALUES(  
	   SRC.[Applicant_ID]
      ,SRC.[No_of_Siblings]
      ,SRC.[Household_Size]
      ,SRC.[Type_of_residence]
      ,SRC.[House_Ownership]
      ,SRC.[Total_Mth_Family_Household_Income]
      ,SRC.[Place_of_Birth]
      ,SRC.[Emergency_Contact_Person]
      ,SRC.[Relationship]
      ,SRC.[Contact_Number]
      ,@processeddatetime
	  ,@processeddatetime)
   WHEN MATCHED
   
   THEN UPDATE
   SET 	
   
    dst.[Contact_Number]=src.[Contact_Number]
   ,dst.[No_of_Siblings]=src.[No_of_Siblings]
   ,dst.[Household_Size]=src.[Household_Size]
   ,dst.[Type_of_residence]=src.[Type_of_residence]
   ,dst.[House_Ownership]=src.[House_Ownership]
   ,dst.[Total_Mth_Family_Household_Income]=src.[Total_Mth_Family_Household_Income]
   ,dst.[Place_of_Birth]=src.[Place_of_Birth]
   		
,dst.Modified_On=@processeddatetime;
		

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
END CATCH  
GO</DTS:VariableValue>
