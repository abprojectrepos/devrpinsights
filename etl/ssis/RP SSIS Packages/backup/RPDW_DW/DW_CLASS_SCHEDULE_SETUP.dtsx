      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_CLASS_SCHEDULE_SETUP
select  @processeddatetime= getdate()
select @jobstatusid=0
--select @LastDWProcessdate
;WITH TMP_DW_CLASS_SCHEDULE_SETUP
(
Institution
,Qual_Type
,Acad_Term
,Sch_Ctr_of_Module
,Acad_Day
,Module_ID
,Lecturer_ID
,Module_Code
,Module_Desc
,Class_ID
,RP_Class
,Lecturer_Name
,Sch_of_Lecturer
,Lecturer_Type
,Class_Capacity
,Class_Vacancy

)

AS 

(
select 
Inst AS Institution
,Qual_Type
,Term AS Acad_Term
,Sch_Ctr_of_Module
,Acad_Day
,Module_ID
,Lecturer_ID
,Module_Code
,Module_Desc
,Class_ID
,RP_Class
,Lecturer_Name
,Sch_of_Lecturer
,Lecturer_Type
,Class_Capacity
,Class_Vacancy
from RPDW_DEV_STG..STG_EXT_CLASS_SCHEDULE 
where latest_ind='Y')

MERGE INTO DW_CLASS_SCHEDULE_SETUP AS dst
   USING TMP_DW_CLASS_SCHEDULE_SETUP AS src
   ON( dst.[Acad_Term]=src.[Acad_Term] and dst.[Acad_Day]=src.[Acad_Day] and dst.[Module_ID]=src.[Module_ID] 
   AND dst.[Lecturer_ID]=src.[Lecturer_ID] and dst.[Class_ID]=src.[Class_ID] and dst.[RP_Class]=src.[RP_Class]  )

   WHEN NOT MATCHED THEN
   INSERT
	  (Institution
,Qual_Type
,Acad_Term
,Sch_Ctr_of_Module
,Acad_Day
,Module_ID
,Lecturer_ID
,Module_Code
,Module_Desc
,Class_ID
,RP_Class
,Lecturer_Name
,Sch_of_Lecturer
,Lecturer_Type
,Class_Capacity
,Class_Vacancy
      ,[CREATED_ON]
      ,[MODIFIED_ON])
   VALUES(  
 SRC.Institution
,SRC.Qual_Type
,SRC.Acad_Term
,SRC.Sch_Ctr_of_Module
,SRC.Acad_Day
,SRC.Module_ID
,SRC.Lecturer_ID
,SRC.Module_Code
,SRC.Module_Desc
,SRC.Class_ID
,SRC.RP_Class
,SRC.Lecturer_Name
,SRC.Sch_of_Lecturer
,SRC.Lecturer_Type
,SRC.Class_Capacity
,SRC.Class_Vacancy
      ,@processeddatetime
	  ,@processeddatetime)
   WHEN MATCHED
   
   THEN UPDATE
   SET 	
   
    dst.[Class_Vacancy]=src.[Class_Vacancy]
   ,dst.Class_Capacity=src.Class_Capacity
   ,dst.Lecturer_Type=src.Lecturer_Type
   ,dst.Sch_of_Lecturer=src.Sch_of_Lecturer
   ,dst.Lecturer_Name=src.Lecturer_Name
   ,dst.[Module_Desc]=src.[Module_Desc]
   ,dst.[Institution]=src.[Institution]
   ,DST.Sch_Ctr_of_Module=SRC.Sch_Ctr_of_Module
   ,DST.Qual_Type=SRC.Qual_Type
   ,DST.Module_Code=SRC.Module_Code	
,dst.Modified_On=@processeddatetime;
		

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
END CATCH  
GO</DTS:VariableValue>
