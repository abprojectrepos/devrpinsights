      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(5)&#xA;select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_CLASS_SCHEDULE_SETUP&#xA;select  @processeddatetime= getdate()&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JobStatusId] +&quot;&#xA;--select @LastDWProcessdate&#xA;;WITH TMP_DW_CLASS_SCHEDULE_SETUP&#xA;(&#xA;Institution&#xA;,Qual_Type&#xA;,Acad_Term&#xA;,Sch_Ctr_of_Module&#xA;,Acad_Day&#xA;,Module_ID&#xA;,Lecturer_ID&#xA;,Module_Code&#xA;,Module_Desc&#xA;,Class_ID&#xA;,RP_Class&#xA;,Lecturer_Name&#xA;,Sch_of_Lecturer&#xA;,Lecturer_Type&#xA;,Class_Capacity&#xA;,Class_Vacancy&#xA;&#xA;)&#xA;&#xA;AS &#xA;&#xA;(&#xA;select &#xA;Inst AS Institution&#xA;,Qual_Type&#xA;,Term AS Acad_Term&#xA;,Sch_Ctr_of_Module&#xA;,Acad_Day&#xA;,Module_ID&#xA;,Lecturer_ID&#xA;,Module_Code&#xA;,Module_Desc&#xA;,Class_ID&#xA;,RP_Class&#xA;,Lecturer_Name&#xA;,Sch_of_Lecturer&#xA;,Lecturer_Type&#xA;,Class_Capacity&#xA;,Class_Vacancy&#xA;from &quot;+@[$Project::Staging_Initial_Catalog]+&quot;..STG_EXT_CLASS_SCHEDULE &#xA;where latest_ind='Y')&#xA;&#xA;MERGE INTO DW_CLASS_SCHEDULE_SETUP AS dst&#xA;   USING TMP_DW_CLASS_SCHEDULE_SETUP AS src&#xA;   ON( dst.[Acad_Term]=src.[Acad_Term] and dst.[Acad_Day]=src.[Acad_Day] and dst.[Module_ID]=src.[Module_ID] &#xA;   AND dst.[Lecturer_ID]=src.[Lecturer_ID] and dst.[Class_ID]=src.[Class_ID] and dst.[RP_Class]=src.[RP_Class]  )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT&#xA;&#x9;  (Institution&#xA;,Qual_Type&#xA;,Acad_Term&#xA;,Sch_Ctr_of_Module&#xA;,Acad_Day&#xA;,Module_ID&#xA;,Lecturer_ID&#xA;,Module_Code&#xA;,Module_Desc&#xA;,Class_ID&#xA;,RP_Class&#xA;,Lecturer_Name&#xA;,Sch_of_Lecturer&#xA;,Lecturer_Type&#xA;,Class_Capacity&#xA;,Class_Vacancy&#xA;      ,[CREATED_ON]&#xA;      ,[MODIFIED_ON])&#xA;   VALUES(  &#xA; SRC.Institution&#xA;,SRC.Qual_Type&#xA;,SRC.Acad_Term&#xA;,SRC.Sch_Ctr_of_Module&#xA;,SRC.Acad_Day&#xA;,SRC.Module_ID&#xA;,SRC.Lecturer_ID&#xA;,SRC.Module_Code&#xA;,SRC.Module_Desc&#xA;,SRC.Class_ID&#xA;,SRC.RP_Class&#xA;,SRC.Lecturer_Name&#xA;,SRC.Sch_of_Lecturer&#xA;,SRC.Lecturer_Type&#xA;,SRC.Class_Capacity&#xA;,SRC.Class_Vacancy&#xA;      ,@processeddatetime&#xA;&#x9;  ,@processeddatetime)&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET &#x9;&#xA;   &#xA;    dst.[Class_Vacancy]=src.[Class_Vacancy]&#xA;   ,dst.Class_Capacity=src.Class_Capacity&#xA;   ,dst.Lecturer_Type=src.Lecturer_Type&#xA;   ,dst.Sch_of_Lecturer=src.Sch_of_Lecturer&#xA;   ,dst.Lecturer_Name=src.Lecturer_Name&#xA;   ,dst.[Module_Desc]=src.[Module_Desc]&#xA;   ,dst.[Institution]=src.[Institution]&#xA;   ,DST.Sch_Ctr_of_Module=SRC.Sch_Ctr_of_Module&#xA;   ,DST.Qual_Type=SRC.Qual_Type&#xA;   ,DST.Module_Code=SRC.Module_Code&#x9;&#xA;,dst.Modified_On=@processeddatetime;&#xA;&#x9;&#x9;&#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,@processeddatetime  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&quot;"
      DTS:IncludeInDebugDump="2345"
