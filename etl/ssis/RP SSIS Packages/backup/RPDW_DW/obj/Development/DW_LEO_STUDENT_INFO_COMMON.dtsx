      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_LEO_STUDENT_INFO_COMMON
select  @processeddatetime= getdate()
--select @LastDWProcessdate
;with TMP_DW_LEO_STUDENT_INFO_COMMON
([Student_Id]
,[Qual_Type]
,[ID]
,[School]
,[Prog_ID]
,[Matriculation_Year]
,[UserID]
,[Student_Email]
,[Student_Status]
	  ,CREATED_ON
	  ,MODIFIED_ON
	  ,Student_Name
	  ,Programme_Code
	  ,Student_Status_Desc
     ) AS
(SELECT DISTINCT
AD.[Student_Id]
,[Qual_Type]
,AD.[ID]
,[School]
,[Prog_ID]
,[Matriculation_Year]
,[UserID]
,[Student_Email]
,AD.[Student_Status]
,@processeddatetime
,@processeddatetime
,SD.Student_Name
,Pro.Programme_Code
,MC.[Description] As Student_Descr
from    RPDW_UAT_STG..STG_LEO_STUDENT_INFO_COMMON AD
LEFT JOIN (Select  
Student_Name,Student_ID,Effective_Date,ROW_NUMBER() over(Partition by  Student_Id order by Effective_Date desc) as ROW_no
from  RPDW_UAT_STG..STG_OASIS_STUDENT_DETAILS 
where Latest_Ind='Y') SD ON SD.Student_ID=AD.Student_Id and SD.ROW_no=1
LEft JOIN  RPDW_UAT_STG..STG_LEO_PROG Pro on AD.Prog_ID=Pro.ID and Pro.Latest_Ind='Y'
Left JOIN  RPDW_UAT_STG..STG_LEO_MASTER_CODE MC on AD.Student_Status=MC.[Value] and MC.[Filename]='Student_Info_Common'
and MC.Data_Element='Student_Status'
WHERE AD.Latest_Ind='Y'
AND AD.[Modified_On]&gt;@LastDWProcessdate)
--SELECT * FROM TMP_DW_LEO_STUDENT_INFO_COMMON
MERGE INTO DW_LEO_STUDENT_INFO_COMMON AS dst
   USING TMP_DW_LEO_STUDENT_INFO_COMMON AS src
ON
( dst.[Student_ID]=src.[Student_ID]
AND DST.Qual_Type=SRC.Qual_Type
   )
   WHEN NOT MATCHED THEN
   INSERT
   (  [Student_Id]
,[Qual_Type]
,[ID]
,[School]
,[Prog_ID]
,[Matriculation_Year]
,[UserID]
,[Student_Email]
,[Student_Status]

	  ,CREATED_ON,
	  MODIFIED_ON
	  ,Student_Name
	  ,Programme_Code
	  ,Student_Status_Desc)
VALUES
(	   src.[Student_Id]
,src.[Qual_Type]
,src.[ID]
,src.[School]
,src.[Prog_ID]
,src.[Matriculation_Year]
,src.[UserID]
,src.[Student_Email]
,src.[Student_Status]

      ,SRC.CREATED_ON,
	 SRC. MODIFIED_ON
	 ,SRC.Student_Name
	  ,SRC.Programme_Code
	  ,SRC.Student_Status_Desc)
	   WHEN MATCHED  THEN UPDATE
   SET DST.[ID] = SRC.[ID]
,DST.[School] = SRC.[School]
,DST.[Prog_ID] = SRC.[Prog_ID]
,DST.[Matriculation_Year] = SRC.[Matriculation_Year]
,DST.[UserID] = SRC.[UserID]
,DST.[Student_Email] = SRC.[Student_Email]
,DST.[Student_Status] = SRC.[Student_Status]
,DST.Student_Name=SRC.Student_Name
,DST.Programme_Code=SRC.Programme_Code
,DST.Student_Status_Desc=SRC.Student_Status_Desc
,DST.MODIFIED_ON=getdate()
;
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
ROLLBACK TRANSACTION
 INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
  
  
END CATCH  
GO</DTS:VariableValue>
