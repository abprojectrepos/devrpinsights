      DTS:ObjectName="SQL_DW_Load">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_LEO_PROG]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_LEO_PROG ( [ID]
      ,[Programme_Code]
      ,[Programme_Name]
      ,[Programme_Managers]
      )
         as
  (
SELECT a.[ID]
      ,a.[Programme_Code]
      ,a.[Programme_Name]
      ,a.[Programme_Managers]
            
  FROM RPDW_UAT_STG.[dbo].[STG_LEO_PROG] a
         where a.[Latest_Ind]='Y' and a.[Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_LEO_PROG AS dst
   USING TMP_DW_LEO_PROG AS src
   ON( dst.ID=src.ID )

   WHEN NOT MATCHED THEN
   INSERT
         (  [ID]
      ,[Programme_Code]
      ,[Programme_Name]
      ,[Programme_Managers]
       ,[Created_On]
      ,[Modified_On])
   VALUES(  
          SRC.[ID]
      ,SRC.[Programme_Code]
      ,SRC.[Programme_Name]
      ,SRC.[Programme_Managers]
       ,  getdate(),getdate())
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.Programme_Code=src.Programme_Code
           
              ,dst.Programme_Name=src.Programme_Name
              ,dst.Programme_Managers=src.Programme_Managers
			   		
			  ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO



