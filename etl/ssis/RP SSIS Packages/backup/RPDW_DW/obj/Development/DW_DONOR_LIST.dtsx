      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_DONOR_LIST]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;&#xA;&#xA;;with TMP_DW_DONOR_LIST (&#xA;&#x9;&#x9;[Donor_ID]&#xA;      ,[Donor_Name]&#xA;      ,[Prefix]&#xA;      ,[First_Name]&#xA;      ,[Last_Name]&#xA;      ,[Official_Des]&#xA;      ,[Donor_Classification]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT &#x9; &#xA;&#x9;&#x9;[Donor_ID]&#xA;      ,[Donor_Name]&#xA;      ,[Prefix]&#xA;      ,[First_Name]&#xA;      ,[Last_Name]&#xA;      ,[Official_Des]&#xA;      ,[Donor_Classification]&#xA;  FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_DONOR_LIST]&#xA;&#x9;where [Latest_Ind]='Y'  and [Modified_On]&gt;@LastDWProcessdate)&#xA;&#xA;       &#xA;&#xA;MERGE INTO dbo.DW_DONOR_LIST AS dst&#xA;   USING TMP_DW_DONOR_LIST AS src&#xA;   ON( dst.[Donor_ID]=src.[Donor_ID] )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT(&#xA;&#x9;&#x9;&#x9;[Donor_ID]&#xA;&#x9;&#x9;&#x9;,[Donor_Name]&#xA;&#x9;&#x9;&#x9;,[Prefix]&#xA;&#x9;&#x9;&#x9;,[First_Name]&#xA;&#x9;&#x9;&#x9;,[Last_Name]&#xA;&#x9;&#x9;&#x9;,[Official_Des]&#xA;&#x9;&#x9;&#x9;,[Donor_Classification]&#xA;&#x9;&#x9;&#x9;,[Created_On]&#xA;&#x9;&#x9;&#x9;,[Modified_On] )&#xA;   VALUES(  &#xA;&#x9;&#x9;&#x9;src.[Donor_ID]&#xA;&#x9;&#x9;&#x9;,src.[Donor_Name]&#xA;&#x9;&#x9;&#x9;,src.[Prefix]&#xA;&#x9;&#x9;&#x9;,src.[First_Name]&#xA;&#x9;&#x9;&#x9;,src.[Last_Name]&#xA;&#x9;&#x9;&#x9;,src.[Official_Des]&#xA;&#x9;&#x9;&#x9;,src.[Donor_Classification]&#xA;&#x9;&#x9;&#x9;,getdate()&#xA;&#x9;&#x9;&#x9;,getdate())&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET   &#xA;&#x9;&#x9;&#x9;dst.[Donor_Name]=src.[Donor_Name]&#xA;&#x9;&#x9;&#x9;,dst.[Prefix]=src.[Prefix]&#xA;&#x9;&#x9;&#x9;,dst.[First_Name]=src.[First_Name]&#xA;&#x9;&#x9;&#x9;,dst.[Last_Name]=src.[Last_Name]&#xA;&#x9;&#x9;&#x9;,dst.[Official_Des]=src.[Official_Des]&#xA;&#x9;&#x9;&#x9;,dst.[Donor_Classification]=src.[Donor_Classification]&#xA;&#x9;&#x9;&#x9;,dst.Modified_On=getdate();&#xA;&#x9;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&quot;"
      DTS:IncludeInDebugDump="2345"
