      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_SOT_STUDENT_APPLICATIONS]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;&#xA;&#xA;;with TMP_DW_SOT_STUDENT_APPLICATIONS (&#xA;      [Qual_Type]&#xA;      ,[Qual_Type_Desc]&#xA;      ,[Event_ID]&#xA;      ,[CCE_ID]&#xA;      ,[Student_ID]&#xA;      ,[Status]&#xA;&#x9;  ,[Status_Desc]&#xA;      ,[Remarks]&#xA;      ,[Role]&#xA;&#x9;  ,[Role_Desc]&#xA;      ,[Achievemt]&#xA;&#x9;  ,[Achievemt_Desc]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT a.[Qual_Type]&#xA;      ,b.[Long_Name]&#xA;      ,a.[Event_ID]&#xA;      ,a.[CCE_ID]&#xA;      ,a.[Student_ID]&#xA;      ,a.[Status]&#xA;&#x9;  ,c.[Long_Name]&#xA;      ,a.[Remarks]&#xA;      ,a.[Role]&#xA;&#x9;  ,d.[Long_Name]&#xA;      ,a.[Achievemt]&#xA;&#x9;  ,e.[Long_Name]&#xA;FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_SOT_STUD_APP] a &#xA;Left JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_MASTER_CODE] b&#xA;on a.Qual_Type=b.FIELD_VALUE and b.FIELD_NAME='ACAD_CAREER' and b.EFFECTIVE_STATUS = 'A' &#xA;Left JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_MASTER_CODE] c&#xA;on a.[Status]=c.FIELD_VALUE and c.FIELD_NAME='A_EV_REG_STATUS' and c.EFFECTIVE_STATUS = 'A' &#xA;Left JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_MASTER_CODE] d&#xA;on a.[Role]=d.FIELD_VALUE and d.FIELD_NAME='A_SL_ROLE' and d.EFFECTIVE_STATUS = 'A' &#xA;Left JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_MASTER_CODE] e&#xA;on a.[Achievemt]=e.FIELD_VALUE and e.FIELD_NAME='A_SL_ACV' and e.EFFECTIVE_STATUS = 'A' &#xA;         where a.[Latest_Ind]='Y'  and a.[Modified_On]&gt;@LastDWProcessdate)&#xA;&#xA;       &#xA;&#xA;MERGE INTO dbo.DW_SOT_STUDENT_APPLICATIONS AS dst&#xA;   USING TMP_DW_SOT_STUDENT_APPLICATIONS AS src&#xA;   ON( dst.[Qual_Type]=src.[Qual_Type] and dst.[Event_ID]=src.[Event_ID] and dst.[Student_ID]=Src.[Student_ID] and &#xA;   dst.[Achievemt]=src.[Achievemt]  )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT&#xA;         (       &#xA;&#x9;&#x9;[Qual_Type]&#xA;&#x9;&#x9;,[Qual_Type_Desc]&#xA;&#x9;&#x9;,[Event_ID]&#xA;&#x9;&#x9;,[CCE_ID]&#xA;&#x9;&#x9;,[Student_ID]&#xA;&#x9;&#x9;,[Status]&#xA;&#x9;&#x9;,[Status_Desc]&#xA;&#x9;&#x9;,[Remarks]&#xA;&#x9;&#x9;,[Role]&#xA;&#x9;&#x9;,[Role_Desc]&#xA;&#x9;&#x9;,[Achievemt]&#xA;&#x9;&#x9;,[Achievemt_Desc]&#xA;&#x9;&#x9;,[Created_On]&#xA;&#x9;&#x9;,[Modified_On])&#xA;   VALUES(  &#xA;      src.[Qual_Type]&#xA;      ,src.[Qual_Type_Desc]&#xA;      ,src.[Event_ID]&#xA;      ,src.[CCE_ID]&#xA;      ,src.[Student_ID]&#xA;      ,src.[Status]&#xA;&#x9;  ,src.[Status_Desc]&#xA;      ,src.[Remarks]&#xA;      ,src.[Role]&#xA;&#x9;  ,src.[Role_Desc]&#xA;      ,src.[Achievemt]&#xA;&#x9;  ,src.[Achievemt_Desc]&#xA;      ,getdate()&#xA;&#x9;  ,getdate()&#xA;&#x9;)&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET        &#xA;&#x9;&#x9;dst.[Qual_Type_Desc]=src.[Qual_Type_Desc]&#xA;&#x9;&#x9;,dst.[CCE_ID]=src.[CCE_ID]&#xA;&#x9;&#x9;,dst.[Status]=src.[Status]&#xA;&#x9;&#x9;,dst.[Status_Desc]=src.[Status_Desc]&#xA;&#x9;&#x9;,dst.[Role]=src.[Role]&#xA;&#x9;&#x9;,dst.[Role_Desc]=src.[Role_Desc]&#xA;&#x9;&#x9;,dst.[Remarks]=src.[Remarks]&#xA;&#x9;&#x9;,dst.[Achievemt_Desc]=src.[Achievemt_Desc]&#xA;&#x9;&#x9;,dst.Modified_On=getdate();&#xA;&#x9;&#x9;&#x9; &#xA;              &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&quot;"
