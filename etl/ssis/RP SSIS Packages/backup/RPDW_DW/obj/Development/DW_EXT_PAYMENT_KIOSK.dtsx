      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from  [dbo].[DW_EXT_PAYMENT_KIOSK]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate
;with  TMP_DW_EXT_PAYMENT_KIOSK ( [Txn_ID]
      ,[Date_Submit]
      ,[Payment_Type]
      ,[NETS_Ref]
      ,[Kiosk_ID]
      ,[Terminal_ID]
      ,[OASIS_Ref]
      ,[Approval_Code]
      ,[Payee]
      ,[Amt]
      ,[Status]
      ,[Event_Name]
      )
         as
  (
select  [Txn_ID]
      ,[Date_Submit]
      ,[Payment_Type]
      ,[NETS_Ref]
      ,[Kiosk_ID]
      ,[Terminal_ID]
      ,[OASIS_Ref]
      ,[Approval_Code]
      ,[Payee]
      ,[Amt]
      ,[Status]
      ,[Event_Name]
  from RPDW_PRD_STG.dbo.STG_EXT_PAYMENT_KIOSK a
 where a.[Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_EXT_PAYMENT_KIOSK AS dst
   USING TMP_DW_EXT_PAYMENT_KIOSK AS src
   ON( dst.[Txn_ID]=src.[Txn_ID] )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Txn_ID]
      ,[Date_Submit]
      ,[Payment_Type]
      ,[NETS_Ref]
      ,[Kiosk_ID]
      ,[Terminal_ID]
      ,[OASIS_Ref]
      ,[Approval_Code]
      ,[Payee]
      ,[Amt]
      ,[Status]
      ,[Event_Name]
      ,[Created_On]
      ,[Modified_On])
   VALUES(  
          Src.[Txn_ID]
      ,Src.[Date_Submit]
      ,Src.[Payment_Type]
      ,Src.[NETS_Ref]
      ,Src.[Kiosk_ID]
      ,Src.[Terminal_ID]
      ,Src.[OASIS_Ref]
      ,Src.[Approval_Code]
      ,Src.[Payee]
      ,Src.[Amt]
      ,Src.[Status]
      ,Src.[Event_Name]
    
      ,getdate()
      ,getdate())
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.[Date_Submit]=src.[Date_Submit]
              ,dst.[Payment_Type]=src.[Payment_Type]
              ,dst.[NETS_Ref]=src.[NETS_Ref]
              ,dst.[Kiosk_ID]=src.[Kiosk_ID]
			  ,dst.[Terminal_ID]=src.[Terminal_ID]
			  ,dst.[OASIS_Ref]=src.[OASIS_Ref]

			  ,dst.[Approval_Code]=src.[Approval_Code]
			  ,dst.[Payee]=src.[Payee]
			  ,dst.[Amt]=src.[Amt]
			  ,dst.[Status]=src.[Status]
			  ,dst.[Event_Name]=src.[Event_Name]
			  
			   ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO



