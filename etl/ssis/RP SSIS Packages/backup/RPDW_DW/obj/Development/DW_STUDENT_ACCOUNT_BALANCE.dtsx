      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_STUDENT_ACCOUNT_BALANCE]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_STUDENT_ACCOUNT_BALANCE (
		[Business_Unit]
		,[Term]
		,[Student_ID]
		,[Bal_All_Acct]
		,[Student_Name]
		,[Term_Bal]
      )
         as
  (
SELECT 	 
		[Business_Unit]
		,[Term]
		,[Student_ID]
		,[Bal_All_Acct]
		,[Student_Name]
		,[Term_Bal]
FROM RPDW_PRD_STG.[dbo].[STG_OASIS_STUD_ACCT_BAL]
where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)


MERGE INTO dbo.DW_STUDENT_ACCOUNT_BALANCE AS dst
   USING TMP_DW_STUDENT_ACCOUNT_BALANCE AS src
   ON( dst.[Business_Unit]=src.[Business_Unit] and dst.[Term]=src.[Term] and dst.[Student_ID]=src.[Student_ID] )

   WHEN NOT MATCHED THEN
   INSERT(
			[Business_Unit]
			,[Term]
			,[Student_ID]
			,[Bal_All_Acct]
			,[Student_Name]
			,[Term_Bal]
			,[Created_On]
			,[Modified_On] )
   VALUES(  
			src.[Business_Unit]
			,src.[Term]
			,src.[Student_ID]
			,src.[Bal_All_Acct]
			,src.[Student_Name]
			,src.[Term_Bal]
			,getdate()
			,getdate())
   WHEN MATCHED
   
   THEN UPDATE
   SET   
      dst.[Bal_All_Acct]=src.[Bal_All_Acct]
      ,dst.[Student_Name]=src.[Student_Name]
      ,dst.[Term_Bal]=src.[Term_Bal]
			,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO</DTS:VariableValue>
