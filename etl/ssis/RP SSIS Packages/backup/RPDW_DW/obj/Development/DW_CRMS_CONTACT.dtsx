      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from  [dbo].[DW_CRMS_CONTACT]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate
;with TMP_DW_CRMS_CONTACT ([Contact_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[First_Name]
      ,[Last_Name]
      ,[Designation]
      ,[Job_Function]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
      )
         as
  (
SELECT  [Contact_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[First_Name]
      ,[Last_Name]
      ,[Designation]
      ,[Job_Function]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
  FROM RPDW_PRD_STG.[dbo].[STG_CRMS_CONTACT] a
         where a.[Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_CRMS_CONTACT AS dst
   USING TMP_DW_CRMS_CONTACT AS src
   ON( dst.[Contact_ID]=src.[Contact_ID] )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Contact_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[First_Name]
      ,[Last_Name]
      ,[Designation]
      ,[Job_Function]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
	  ,[CREATED_ON]
      ,[MODIFIED_ON])
   VALUES(  
           src.[Contact_ID]
      ,src.[Acct_ID]
      ,src.[Acct_Name]
      ,src.[First_Name]
      ,src.[Last_Name]
      ,src.[Designation]
      ,src.[Job_Function]
      ,src.[CRMS_Created_On]
      ,src.[CRMS_Modified_On]
       ,  getdate(),getdate()
	  )
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.[Acct_ID]=src.[Acct_ID]
              ,dst.[Acct_Name]=src.[Acct_Name]
            
              ,dst.[First_Name]=src.[First_Name]
		     
			  ,dst.[Last_Name]=src.[Last_Name]
			
			    ,dst.[Designation]=src.[Designation]
			    ,dst.[Job_Function]=src.[Job_Function]
				    
			      ,dst.[CRMS_Created_On]=src.[CRMS_Created_On]
			     ,dst.[CRMS_Modified_On]=src.[CRMS_Modified_On]
			

			   ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
