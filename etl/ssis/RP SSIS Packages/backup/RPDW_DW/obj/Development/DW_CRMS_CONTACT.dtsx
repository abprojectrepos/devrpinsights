st.[Acct_ID]=src.[Acct_ID]&#xA;              ,dst.[Acct_Name]=src.[Acct_Name]&#xA;            &#xA;              ,dst.[First_Name]=src.[First_Name]&#xA;&#x9;&#x9;     &#xA;&#x9;&#x9;&#x9;  ,dst.[Last_Name]=src.[Last_Name]&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;    ,dst.[Designation]=src.[Designation]&#xA;&#x9;&#x9;&#x9;    ,dst.[Job_Function]=src.[Job_Function]&#xA;&#x9;&#x9;&#x9;&#x9;    &#xA;&#x9;&#x9;&#x9;      ,dst.[CRMS_Created_On]=src.[CRMS_Created_On]&#xA;&#x9;&#x9;&#x9;     ,dst.[CRMS_Modified_On]=src.[CRMS_Modified_On]&#xA;&#x9;&#x9;&#x9;&#xA;&#xA;&#x9;&#x9;&#x9;   ,dst.Modified_On=getdate();&#xA;&#x9;&#x9;&#x9; &#xA;              &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&#xA;&quot;"
