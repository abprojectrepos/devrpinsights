BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_SCHOLARSHIP_BURSARY_LISTING]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_SCHOLARSHIP_BURSARY_LISTING (
		[Award_ID]
		,[Award_Desc]
		,[Item_Type]
      )
         as
  (
SELECT 	 
		[Award_ID]
		,[Award_Desc]
		,[Item_Type]
  FROM RPDW_PRD_STG.[dbo].[STG_OASIS_SCH_BUR_ITEM_TYPE]
	where [Latest_Ind]='Y'  and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_SCHOLARSHIP_BURSARY_LISTING AS dst
   USING TMP_DW_SCHOLARSHIP_BURSARY_LISTING AS src
   ON( dst.[Award_ID]=src.[Award_ID] )

   WHEN NOT MATCHED THEN
   INSERT(
			[Award_ID]
			,[Award_Desc]
			,[Item_Type]
			,[Created_On]
			,[Modified_On] )
   VALUES(  
			src.[Award_ID]
			,src.[Award_Desc]
			,src.[Item_Type]
			,getdate()
			,getdate())
   WHEN MATCHED
   
   THEN UPDATE
   SET   
		dst.[Award_Desc]=src.[Award_Desc]
		,dst.[Item_Type]=src.[Item_Type]
		,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
