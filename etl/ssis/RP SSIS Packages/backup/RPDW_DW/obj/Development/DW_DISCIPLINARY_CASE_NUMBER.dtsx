      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_DISCIPLINARY_CASE_NUMBER]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate

;with tmp_STG_OASIS_PROG_TABLE (qual_type,PROG_CODE,Prog_Formal_Descr,EFFECTIVE_DATE,latest_ind,ra)
as

(select distinct qual_type,PROG_CODE,Prog_Formal_Descr,EFFECTIVE_DATE,latest_ind,
row_number()over(partition by PROG_CODE order by EFFECTIVE_DATE desc)ra from RPDW_PRD_STG..STG_OASIS_PROG_TABLE
--where PROG_CODE='DBA'
)

, TMP_DW_DISCIPLINARY_CASE_NUMBER (
      [Inst]
      ,[Qual_Type]
      ,[Qual_Type_Descr]
      ,[Case_Num]
      ,[Student_ID]
      ,[Name]
      ,[Sch]
      ,[Prog]
      ,[Prog_Descr]
      ,[Offence_Code]
      ,[Offence_Desc]
      ,[Seq_Num]
      )
         as
  (
SELECT a.[Inst]
      ,a.[Qual_Type]
      ,b.Long_Name
      ,a.[Case_Num]
      ,a.[Student_ID]
      ,a.[Name]
      ,a.[Sch]
      ,a.[Prog]
      ,c.Prog_Formal_Descr
      ,a.[Offence_Code]
      ,a.[Offence_Desc]
      ,a.[Seq_Num]
  FROM RPDW_PRD_STG.[dbo].[STG_OASIS_GENERATE_CASE_NUM] a 
    Left JOIN RPDW_PRD_STG.[dbo].[STG_OASIS_MASTER_CODE] b
  on a.Qual_Type=b.FIELD_VALUE and b.FIELD_NAME='ACAD_CAREER' and b.EFFECTIVE_STATUS = 'A' 
  Left Join tmp_STG_OASIS_PROG_TABLE c on a.Prog=c.PROG_CODE and c.latest_ind='Y' and c.ra=1
         where a.[Latest_Ind]='Y'  and a.[Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_DISCIPLINARY_CASE_NUMBER AS dst
   USING TMP_DW_DISCIPLINARY_CASE_NUMBER AS src
   ON( dst.[Inst]=src.[Inst] and dst.[Qual_Type]=src.[Qual_Type] and dst.[Case_Num]=Src.[Case_Num] and 
   dst.[Student_ID]=src.[Student_ID]  )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Inst]
      ,[Qual_Type]
      ,[Qual_Type_Descr]
      ,[Case_Num]
      ,[Student_ID]
      ,[Name]
      ,[Sch]
      ,[Prog]
      ,[Prog_Descr]
      ,[Offence_Code]
      ,[Offence_Desc]
      ,[Seq_Num]
       ,[Created_On]
      ,[Modified_On])
   VALUES(  
           src.[Inst]
      ,src.[Qual_Type]
      ,src.[Qual_Type_Descr]
      ,src.[Case_Num]
      ,src.[Student_ID]
      ,src.[Name]
      ,src.[Sch]
      ,src.[Prog]
      ,src.[Prog_Descr]
      ,src.[Offence_Code]
      ,src.[Offence_Desc]
      ,src.[Seq_Num]
       ,  getdate(),getdate())
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.Qual_Type_Descr=src.Qual_Type_Descr
              ,dst.[Name]=src.[Name]
              ,dst.[Sch]=src.[Sch]
              ,dst.[Prog]=src.[Prog]

			   ,dst.[Prog_Descr]=src.[Prog_Descr]
			    ,dst.[Offence_Code]=src.[Offence_Code]
				 ,dst.[Offence_Desc]=src.[Offence_Desc]
				  ,dst.[Seq_Num]=src.[Seq_Num]
                  
			   ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
