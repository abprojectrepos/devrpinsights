      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_EXT_ACE_CUSTOMIZED_SHORT_CSE]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_EXT_ACE_CUSTOMIZED_SHORT_CSE (
		[Prog_Title]
		,[Lesson_Date]
		,[Start_Time]
		,[End_Time]
		,[Duration_Hours]
		,[Sch_Ctr]
		,[Venue]
		,[Num_Training_Places]
		,[Trainer_Name]
		,[Trainer_ID]
		,[Trainer_ADID]
		,[Staff_Dept]
		,[Employee_Grp]
      )
         as
  (
SELECT 		
		[Prog_Title]
		,[Lesson_Date]
		,[Start_Time]
		,[End_Time]
		,[Duration_Hours]
		,[Sch_Ctr]
		,[Venue]
		,[Num_Training_Places]
		,[Trainer_Name]
		,[Trainer_ID]
		,[Trainer_ADID]
		,[Staff_Dept]
		,[Employee_Grp]
FROM RPDW_PRD_STG.[dbo].[STG_EXT_ACE_CUSTOM_WORKSHOP]
where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)


MERGE INTO dbo.DW_EXT_ACE_CUSTOMIZED_SHORT_CSE AS dst
   USING TMP_DW_EXT_ACE_CUSTOMIZED_SHORT_CSE AS src
   ON( dst.[Prog_Title]=src.[Prog_Title] and dst.[Lesson_Date]=src.[Lesson_Date] 
		and dst.[Trainer_ADID]=src.[Trainer_ADID] )

   WHEN NOT MATCHED THEN
   INSERT
		( 
		[Prog_Title]
		,[Lesson_Date]
		,[Start_Time]
		,[End_Time]
		,[Duration_Hours]
		,[Sch_Ctr]
		,[Venue]
		,[Num_Training_Places]
		,[Trainer_Name]
		,[Trainer_ID]
		,[Trainer_ADID]
		,[Staff_Dept]
		,[Employee_Grp]
		,[Created_On]
		,[Modified_On] )
   VALUES(  
		src.[Prog_Title]
		,src.[Lesson_Date]
		,src.[Start_Time]
		,src.[End_Time]
		,src.[Duration_Hours]
		,src.[Sch_Ctr]
		,src.[Venue]
		,src.[Num_Training_Places]
		,src.[Trainer_Name]
		,src.[Trainer_ID]
		,src.[Trainer_ADID]
		,src.[Staff_Dept]
		,src.[Employee_Grp]
		,getdate()
		,getdate()
	   )
   WHEN MATCHED
   
   THEN UPDATE
   SET   
		dst.[Start_Time]=src.[Start_Time]
		,dst.[End_Time]=src.[End_Time]
		,dst.[Duration_Hours]=src.[Duration_Hours]
		,dst.[Sch_Ctr]=src.[Sch_Ctr]
		,dst.[Venue]=src.[Venue]
		,dst.[Num_Training_Places]=src.[Num_Training_Places]
		,dst.[Trainer_Name]=src.[Trainer_Name]
		,dst.[Trainer_ID]=src.[Trainer_ID]
		,dst.[Staff_Dept]=src.[Staff_Dept]
		,dst.[Employee_Grp]=src.[Employee_Grp]
		,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO</DTS:VariableValue>
