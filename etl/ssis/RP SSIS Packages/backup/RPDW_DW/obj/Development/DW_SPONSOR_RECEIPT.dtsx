      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_SPONSOR_RECEIPT]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_SPONSOR_RECEIPT (
		[Inst]
      ,[Award_ID]
      ,[Line_Num]
      ,[Fin_Yr]
      ,[Txn_Date]
      ,[Txn_Type]
	  ,[Txn_Type_Desc]
      ,[Txn_Mode]
	  ,[Txn_Mode_Desc]
      ,[Txn_Amt]
      ,[Ref_Num]
      )
         as
  (
SELECT 	
		[Inst]
      ,[Award_ID]
      ,[Line_Num]
      ,[Fin_Yr]
      ,[Txn_Date]
      ,[Txn_Type]
	  ,case when b.[Long_Name] is not NULL then b.[Long_Name]
		else NULL
		end [Txn_Type_Desc]
      ,[Txn_Mode]
	  ,case when c.[Long_Name] is not NULL then c.[Long_Name]
		else NULL
		end [Txn_Mode_Desc]
      ,[Txn_Amt]
      ,[Ref_Num]
   FROM RPDW_PRD_STG.[dbo].[STG_OASIS_SPONSOR_RECEIPT] a
LEFT JOIN RPDW_PRD_STG.[dbo].[STG_OASIS_MASTER_CODE] b 
	on a.[Txn_Type]=b.FIELD_VALUE and b.FIELD_NAME='A_SF_TRANSAC_TYP' and b.EFFECTIVE_STATUS = 'A'
LEFT JOIN RPDW_PRD_STG.[dbo].[STG_OASIS_MASTER_CODE] c
	on a.[Txn_Mode]=c.FIELD_VALUE and c.FIELD_NAME='PAYMENT_METHOD' and c.EFFECTIVE_STATUS = 'A'
		where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)
       

MERGE INTO dbo.DW_SPONSOR_RECEIPT AS dst
   USING TMP_DW_SPONSOR_RECEIPT AS src
   ON( dst.[Inst]=src.[Inst] and dst.[Award_ID]=src.[Award_ID] 
   and dst.[Line_Num]=src.[Line_Num] and dst.[Fin_Yr]=src.[Fin_Yr])

   WHEN NOT MATCHED THEN
	INSERT(	
			[Inst]
			,[Award_ID]
			,[Line_Num]
			,[Fin_Yr]
			,[Txn_Date]
			,[Txn_Type]
			,[Txn_Type_Desc]
			,[Txn_Mode]
			,[Txn_Mode_Desc]
			,[Txn_Amt]
			,[Ref_Num]
			,[Created_On]
			,[Modified_On] )
   VALUES(  
				src.[Inst]
			,src.[Award_ID]
			,src.[Line_Num]
			,src.[Fin_Yr]
			,src.[Txn_Date]
			,src.[Txn_Type]
			,src.[Txn_Type_Desc]
			,src.[Txn_Mode]
			,src.[Txn_Mode_Desc]
			,src.[Txn_Amt]
			,src.[Ref_Num]
			,getdate()
			,getdate()
	   )
   WHEN MATCHED
   
   THEN UPDATE
   SET   
		dst.[Txn_Date]=src.[Txn_Date]
		,dst.[Txn_Type]=src.[Txn_Type]
		,dst.[Txn_Type_Desc]=src.[Txn_Type_Desc]
		,dst.[Txn_Mode]=src.[Txn_Mode]
		,dst.[Txn_Mode_Desc]=src.[Txn_Mode_Desc]
		,dst.[Txn_Amt]=src.[Txn_Amt]
		,dst.[Ref_Num]=src.[Ref_Num]
		,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO</DTS:VariableValue>
