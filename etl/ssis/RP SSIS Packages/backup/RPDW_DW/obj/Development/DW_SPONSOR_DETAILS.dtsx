BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_SPONSOR_DETAILS]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_SPONSOR_DETAILS (
		[Name_Spnsr]
      ,[Date_Joined]
      ,[Internal_External]
      ,[Status]
      ,[Spnsr_ID]
      )
         as
  (
SELECT 	
		[Name_Spnsr]
      ,[Date_Joined]
      ,[Internal_External]
      ,[Status]
      ,[Spnsr_ID]
  FROM RPDW_PRD_STG.[dbo].[STG_OASIS_SPONSOR_DETAILS]
  where [Latest_Ind]='Y'  and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_SPONSOR_DETAILS AS dst
   USING TMP_DW_SPONSOR_DETAILS AS src
   ON( dst.[Spnsr_ID]=src.[Spnsr_ID] )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Name_Spnsr]
		  ,[Date_Joined]
		  ,[Internal_External]
		  ,[Status]
		  ,[Spnsr_ID]
		  ,[Created_On]
		  ,[Modified_On] )
   VALUES(  
				src.[Name_Spnsr]
			,src.[Date_Joined]
			,src.[Internal_External]
			,src.[Status]
			,src.[Spnsr_ID]
			,getdate()
			,getdate()
	   )
   WHEN MATCHED
   
   THEN UPDATE
   SET   
		dst.[Name_Spnsr]=src.[Name_Spnsr]
		,dst.[Date_Joined]=src.[Date_Joined]
		,dst.[Internal_External]=src.[Internal_External]
		,dst.[Status]=src.[Status]
		,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
