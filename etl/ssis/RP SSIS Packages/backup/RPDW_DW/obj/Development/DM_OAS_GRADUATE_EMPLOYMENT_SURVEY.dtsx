      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
--------------------------------------
DECLARE @jobstatusid varchar(5)
select @jobstatusid=0
Delete from [dbo].[DM_OAS_GRADUATE_EMPLOYMENT_SURVEY]

INSERT INTO [dbo].[DM_OAS_GRADUATE_EMPLOYMENT_SURVEY]

Select 
CASE WHEN a.RP_School=a.Diploma_Code and  a.Diploma_Code='RP' THEN 'RP'
	WHEN a.RP_School=a.Diploma_Code and  a.Diploma_Code&lt;&gt;'RP' THEN 'Department' ELSE 'Programme' END AS Level_of_Analysis
,CASE WHEN a.RP_School=a.Diploma_Code and  a.Diploma_Code='RP' THEN 'RP' ELSE a.RP_School END AS  Department
,CASE WHEN a.RP_School=a.Diploma_Code and  a.Diploma_Code='RP' THEN 'Republic Polytechnic' ELSE b.Descr END AS Department_Descr
,a.Diploma_Code +' - ' +a.Diploma as Programme
,a.Survey_Type
,a.Year_of_GES
,a.Emp_Type
,a.No_Of_Resp_For_Emp_Type
,a.Median_Gross_Salary_FTP
,a.No_Of_Resp_For_Median_Gross_Salary
,getdate()
,getdate()
from dbo.DW_EXT_GES_DATA a
Left JOIN dbo.DW_EXT_DEPARTMENT b ON a.RP_School=b.Department




UPDATE ges
SET ges.No_of_Respondents_for_Employment_Type = dagg.Department_No_of_Respondents_for_Employment_Type
FROM DM_OAS_GRADUATE_EMPLOYMENT_SURVEY ges
       INNER JOIN (
       SELECT Department, Survey_Type, Year_of_GES, Employment_Type, SUM(No_of_Respondents_for_Employment_Type) AS Department_No_of_Respondents_for_Employment_Type
       FROM DM_OAS_GRADUATE_EMPLOYMENT_SURVEY
       WHERE Level_of_Analysis = 'Programme'
       GROUP BY Department, Survey_Type, Year_of_GES, Employment_Type) dagg
ON ges.Department = dagg.Department
AND ges.Survey_Type = dagg.Survey_Type
AND ges.Year_of_GES = dagg.Year_of_GES
AND ges.Employment_Type = dagg.Employment_Type
WHERE Level_of_Analysis = 'Department'

UPDATE ges
SET ges.No_of_Respondents_for_Employment_Type = ragg.RP_No_of_Respondents_for_Employment_Type
FROM DM_OAS_GRADUATE_EMPLOYMENT_SURVEY ges
INNER JOIN (
       SELECT Survey_Type, Year_of_GES, Employment_Type, SUM(No_of_Respondents_for_Employment_Type) AS RP_No_of_Respondents_for_Employment_Type
       FROM DM_OAS_GRADUATE_EMPLOYMENT_SURVEY
       WHERE Level_of_Analysis = 'Programme'
       GROUP BY Survey_Type, Year_of_GES, Employment_Type) ragg
ON ges.Survey_Type = ragg.Survey_Type
AND ges.Year_of_GES = ragg.Year_of_GES
AND ges.Employment_Type = ragg.Employment_Type
WHERE Level_of_Analysis = 'RP'



COMMIT TRANSACTION
END TRY

BEGIN CATCH
ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
END CATCH  
GO

