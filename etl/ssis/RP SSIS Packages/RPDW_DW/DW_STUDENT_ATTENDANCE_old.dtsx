      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;&#xA;BEGIN TRY&#xA;&#xA;------------&#xA;&#xA;DECLARE @LastDWProcessdate datetime&#xA;&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(5)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_STUDENT_ATTENDANCE]--dw table&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;--select  @processeddatetime=?&#xA;&#xA;--select @LastDWProcessdate&#xA;&#xA;&#xA;&#xA;&#xA;&#xA;;with TMP_DW_STUDENT_ATTENDANCE ([Student_ID]&#xA;&#xA;      ,[Acad_Year]&#xA;&#xA;      ,[Semester]&#xA;&#xA;      ,[Semester_Code]&#xA;&#xA;      ,[Cohort]&#xA;&#xA;      ,[Qual_Type]&#xA;&#xA;      ,[Class_Code]&#xA;&#xA;      ,[Lesson_No]&#xA;&#xA;      ,LEO_Attendance_Status&#xA;&#x9;  ,LEO_Attendance_Status_Descr&#xA;&#xA;      ,[LOA_Status]&#xA;&#xA;      ,[Remarks1]&#xA;&#xA;      ,[Debarment_Status]&#xA;&#x9;  ,Debarment_Status_Descr&#xA;&#xA;      ,Department&#xA;&#x9;  ,Module_Owner&#xA;      ,[Prog]&#xA;&#xA;      ,[Boe_Term]&#xA;&#xA;      ,[Module_Id]&#xA;&#xA;      ,[Reqmt_Term]&#xA;&#xA;      ,[Module_Code]&#xA;&#xA;      ,[Class_Nbr]&#xA;&#xA;      ,[Student_Name]&#xA;&#xA;      ,[Module_Descr]&#xA;&#xA;      ,[Institution]&#xA;&#xA;      ,[Class_Date]&#xA;&#xA;      ,OASIS_Attendance_Status&#xA;&#xA;      ,[Remarks2]&#xA;&#xA;      ,[Admit_Term]&#xA;&#xA;      )&#xA;&#xA;         as&#xA;&#xA;  (&#xA;&#xA;SELECT &#xA;a.Student_Id&#xA;,a.Academic_Year&#xA;,a.Semester&#xA;,CASE WHEN a.SemesterCode IS NOT NULL and a.SemesterCode &lt;&gt;''&#xA;THEN a.SemesterCode ELSE &#xA;(SUBSTRING(cast(a.Academic_Year as varchar),3,2)+'0')+Cast(a.[Semester] as varchar)  END as SemesterCode&#xA;,a.Cohort&#xA;,ISNULL(a.Qual_Type,'NA')&#xA;,a.Class_Code&#xA;,a.Lesson_No&#xA;,a.Attendance_Status&#xA;,c.[Description]&#xA;,a.LOA_Status&#xA;,b.Remarks&#xA;,a.Debarment_Status&#xA;,d.[Description]&#xA;,ISNULL(Substring(f.School,1,10),'NA') as Department&#xA;,Substring(g1.School,1,10) as Module_Owner&#xA;,ISNULL(Substring(h.Programme_Code,1,5),'NA')&#xA;,Substring(a.SemesterCode,1,4)&#xA;,ISNULL(substring(e.Module_Id,1,6),'NA')&#xA;,b.Reqmt_Term&#xA;,substring(e.Module_Code,1,10)&#xA;,ISNULL(b.Class_Nbr,-1)&#xA;,b.Student_Name&#xA;,Substring(e.Module_Name,1,30)&#xA;,ISnull(b.Inst,'NA')&#xA;,ISNULL(b.Class_Date,'')&#xA;,b.Attd_Status&#xA;,b.Remarks&#xA;,b.Admit_Term&#xA;FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_REGULAR_ATTENDANCE] a left join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_STUDENT_ATTD] b&#xA;  on b.[Latest_Ind]='Y' and a.Student_ID=b.Student_ID and a.Class_Code=b.Class and a.Lesson_No=b.Class_Mtg_Nbr &#xA;   and a.SemesterCode=b.BOE_Term&#xA;&#xA;Left Join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_MASTER_CODE] c ON a.Attendance_Status=c.[value] and c.Data_Element='Attendance_Status'&#xA;Left Join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_MASTER_CODE] d ON a.Debarment_Status=d.[value] and d.Data_Element='Debarment_Status'&#xA;LEFT JOIN (select distinct b1.Module_Code,b1.Module_Name,b1.Id,b1.Module_Id from &#xA;&quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_REGULAR_ATTENDANCE] a1 left join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_MODULE] b1&#xA;ON a1.Module_ID=b1.Id and a1.SemesterCode=b1.Semester_Code)e ON a.Module_ID=e.Id&#xA;LEFT JOIN(select distinct a2.school,a2.Student_Id  from &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_REGULAR_ATTENDANCE] a1 left join &#xA;&quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_STUDENT_INFO_COMMON] a2 ON a1.Student_Id=a2.Student_Id and a1.Qual_Type=a2.Qual_Type) f on f.Student_Id=a.Student_Id&#xA;LEFT join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_STUDENT_INFO_COMMON] g on g.Student_Id=a.Student_Id and g.Qual_Type=a.Qual_Type&#xA;LEFT JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_PROG] h ON h.ID=g.Prog_ID&#xA;LEFT JOIN  &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_MODULE] g1 on a.Module_ID=g1.ID&#xA;where a.[Latest_Ind]='Y' &#xA;and a.SemesterCode&gt;='1700' and a.Qual_Type&lt;&gt;'STAFFQT'&#xA;&#xA;&#xA;&#xA;UNION &#xA;&#xA;SELECT a.Student_Id&#xA;,cast('20'+substring(a.Boe_Term,1,2) as INT) as Academic_Year&#xA;,CAST(substring(a.Boe_Term,3,1) as INT) as Semester&#xA;,a.Boe_Term&#xA;,b.Cohort&#xA;,ISNULL(a.Qual_Type,'NA')&#xA;,a.Class&#xA;,a.Class_Mtg_Nbr&#xA;,b.Attendance_Status&#xA;,c.[Description]&#xA;,b.LOA_Status&#xA;,b.Remarks&#xA;,b.Debarment_Status&#xA;,d.[Description]&#xA;,ISnull(e.School_Centre,'NA')&#xA;,a.School_Centre&#xA;,ISNUll(a.Prog,'NA')&#xA;,a.Boe_Term&#xA;,ISNULL(a.Module_Id,'NA')&#xA;,a.Reqmt_Term&#xA;,a.Module_Code&#xA;,ISNULL(a.Class_Nbr,-1)&#xA;,a.Student_Name&#xA;,a.Module_Desc&#xA;,ISNULL(a.Inst,'NA')&#xA;,ISNULL(a.Class_Date,'')&#xA;,a.Attd_Status&#xA;,b.Remarks&#xA;,a.Admit_Term&#xA;&#xA;FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_STUDENT_ATTD] a left join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_REGULAR_ATTENDANCE] b&#xA;   on a.[Latest_Ind]='Y' and a.Student_ID=b.Student_ID and a.Class=b.Class_Code and a.Class_Mtg_Nbr=b.Lesson_No &#xA;   and a.Boe_Term=b.SemesterCode&#xA;&#xA; Left Join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_MASTER_CODE] c ON b.Attendance_Status=c.[value] and c.Data_Element='Attendance_Status'&#xA;Left Join &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_LEO_MASTER_CODE] d ON b.Debarment_Status=d.[value] and d.Data_Element='Debarment_Status'&#xA;&#xA;Left Join (select  PROG_CODE,Prog_Formal_Descr,School_Centre from &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_PROG_TABLE] a1&#xA;&#x9;&#x9; where a1.LATEST_IND='Y'  and a1.EFFECTIVE_DATE in (select max(b1.EFFECTIVE_DATE) from &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_PROG_TABLE] b1&#xA;&#x9;&#x9; where b1.Prog_Code=a1.PROG_CODE)) e on a.Prog=e.Prog_Code&#xA;where a.[Latest_Ind]='Y' and (a.Boe_Term&lt;'1700' or a.Boe_Term in('1741','1751','1752','2013','2014','2015','2016','2017','2018','2019'))&#xA;and a.Qual_Type&lt;&gt;'STFF' and a.[Modified_On]&gt;@LastDWProcessdate)&#xA;&#xA;&#xA;&#xA;       &#xA;&#xA;&#xA;&#xA;MERGE INTO dbo.DW_STUDENT_ATTENDANCE AS dst&#xA;&#xA;   USING TMP_DW_STUDENT_ATTENDANCE AS src&#xA;&#xA;   ON( dst.Student_Id=src.Student_Id and dst.Class_Code=src.Class_Code and dst.Lesson_No=Src.Lesson_No and dst.Semester_Code=src.Semester_Code&#xA;&#xA;  and  dst.Class_Nbr=src.Class_Nbr and dst.Qual_Type=src.Qual_Type and dst.Department=src.Department and dst.Prog=src.Prog&#xA;  and dst.Module_Id=src.Module_Id and dst.Institution=src.Institution and dst.Class_Date=src.Class_Date)&#xA;&#xA;&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;&#xA;   INSERT&#xA;&#xA;         ( [Student_ID]&#xA;&#xA;      ,[Acad_Year]&#xA;&#xA;      ,[Semester]&#xA;&#xA;      ,[Semester_Code]&#xA;&#xA;      ,[Cohort]&#xA;&#xA;      ,[Qual_Type]&#xA;&#xA;      ,[Class_Code]&#xA;&#xA;      ,[Lesson_No]&#xA;&#xA;      ,LEO_Attendance_Status&#xA;&#x9;  ,LEO_Attendance_Status_Descr&#xA;&#xA;      ,[LOA_Status]&#xA;&#xA;      ,[Remarks1]&#xA;&#xA;      ,[Debarment_Status]&#xA;&#x9;  ,Debarment_Status_Descr&#xA;&#xA;      ,Department&#xA;&#x9;  ,Module_Owner&#xA;&#xA;      ,[Prog]&#xA;&#xA;      ,[Boe_Term]&#xA;&#xA;      ,[Module_Id]&#xA;&#xA;      ,[Reqmt_Term]&#xA;&#xA;      ,[Module_Code]&#xA;&#xA;      ,[Class_Nbr]&#xA;&#xA;      ,[Student_Name]&#xA;&#xA;      ,[Module_Descr]&#xA;&#xA;      ,[Institution]&#xA;&#xA;      ,[Class_Date]&#xA;&#xA;      ,OASIS_Attendance_Status&#xA;&#xA;      ,[Remarks2]&#xA;&#xA;      ,[Admit_Term]&#xA;&#xA;       ,[Created_On]&#xA;&#xA;      ,[Modified_On])&#xA;&#xA;   VALUES(  &#xA;&#xA;           SRC.[Student_ID]&#xA;&#xA;      ,SRC.[Acad_Year]&#xA;&#xA;      ,SRC.[Semester]&#xA;&#xA;      ,SRC.[Semester_Code]&#xA;&#xA;      ,SRC.[Cohort]&#xA;&#xA;      ,SRC.[Qual_Type]&#xA;&#xA;      ,SRC.[Class_Code]&#xA;&#xA;      ,SRC.[Lesson_No]&#xA;&#xA;      ,SRC.LEO_Attendance_Status&#xA;&#x9;  ,SRC.LEO_Attendance_Status_Descr&#xA;&#xA;      ,SRC.[LOA_Status]&#xA;&#xA;      ,SRC.[Remarks1]&#xA;&#xA;      ,SRC.[Debarment_Status]&#xA;&#x9;  ,SRC.Debarment_Status_Descr&#xA;      ,SRC.Department&#xA;&#x9;  ,SRC.Module_Owner&#xA;&#xA;      ,SRC.[Prog]&#xA;&#xA;      ,SRC.[Boe_Term]&#xA;&#xA;      ,SRC.[Module_Id]&#xA;&#xA;      ,SRC.[Reqmt_Term]&#xA;&#xA;      ,SRC.[Module_Code]&#xA;&#xA;      ,SRC.[Class_Nbr]&#xA;&#xA;      ,SRC.[Student_Name]&#xA;&#xA;      ,SRC.[Module_Descr]&#xA;&#xA;      ,SRC.[Institution]&#xA;&#xA;      ,SRC.[Class_Date]&#xA;&#xA;      ,SRC.OASIS_Attendance_Status&#xA;&#xA;      ,SRC.[Remarks2]&#xA;&#xA;      ,SRC.[Admit_Term]&#xA;&#xA;       ,  getdate(),getdate())&#xA;&#xA;   WHEN MATCHED&#xA;&#xA;   &#xA;&#xA;   THEN UPDATE&#xA;&#xA;   SET        dst.Acad_Year=src.Acad_Year&#xA;&#xA;              ,dst.Semester=src.Semester&#xA;&#xA;           &#xA;&#xA;              ,dst.Cohort=src.Cohort&#xA;&#xA;&#x9;&#x9;&#x9;  --,dst.Qual_Type=src.Qual_Type&#xA;&#xA;&#x9;&#x9;&#x9;  ,dst.LEO_Attendance_Status=src.LEO_Attendance_Status&#xA;&#xA;&#x9;&#x9;&#x9;  ,dst.Debarment_Status=src.Debarment_Status&#xA;&#xA;&#x9;&#x9;&#x9;  ,dst.Module_Owner=src.Module_Owner&#xA;&#xA;&#x9;&#x9;&#x9; -- ,dst.Prog=src.Prog&#xA;&#xA;&#x9;&#x9;&#x9;  ,dst.Boe_Term=src.Boe_Term&#xA;&#xA;&#x9;&#x9;&#x9;  --,dst.Module_Id=src.Module_Id&#xA;&#xA;&#x9;&#x9;&#x9;  ,dst.Reqmt_Term=src.Reqmt_Term&#xA;&#xA;&#x9;&#x9;&#x9;  ,dst.Module_Code=src.Module_Code&#xA;&#xA;&#x9;&#x9;&#x9;&#xA;&#xA;&#x9;&#x9;&#x9;  ,dst.Student_Name=src.Student_Name&#xA;&#xA;&#x9;&#x9;&#x9; ,dst.Module_Descr=src.Module_Descr&#xA;&#xA;&#x9;&#x9;&#x9; -- ,dst.Institution=src.Institution&#xA;&#xA;&#x9;&#x9;&#x9;  -- ,dst.Class_Date=src.Class_Date&#xA;&#xA;&#x9;&#x9;&#x9;    ,dst.OASIS_Attendance_Status=src.OASIS_Attendance_Status&#xA;&#xA;&#x9;&#x9;&#x9;&#x9; ,dst.Remarks2=src.Remarks2&#xA;&#xA;&#x9;&#x9;&#x9;&#x9;  ,dst.Admit_Term=src.Admit_Term&#xA;&#x9;&#x9;&#x9;&#x9;  ,dst.Debarment_Status_Descr=src.Debarment_Status_Descr&#xA;&#x9;&#x9;&#x9;&#x9;  ,dst.LEO_Attendance_Status_Descr=src.LEO_Attendance_Status_Descr&#xA;&#xA;&#xA;&#xA;&#x9;&#x9;&#x9; ,dst.Modified_On=getdate();&#xA;&#xA;              &#xA;&#xA;&#xA;&#xA;--------------------------------------&#xA;&#xA;COMMIT TRANSACTION&#xA;&#xA;END TRY&#xA;&#xA;&#xA;&#xA;BEGIN CATCH&#xA;&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;&#xA;  &#xA;&#xA;END CATCH  &#xA;&#xA;GO&#xA;&quot;"
