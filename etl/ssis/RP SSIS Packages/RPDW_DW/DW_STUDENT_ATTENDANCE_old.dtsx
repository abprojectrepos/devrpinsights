      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION

BEGIN TRY

------------

DECLARE @LastDWProcessdate datetime

,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0

select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_STUDENT_ATTENDANCE]--dw table









--select  @processeddatetime=?

--select @LastDWProcessdate





;with TMP_DW_STUDENT_ATTENDANCE ([Student_ID]

      ,[Acad_Year]

      ,[Semester]

      ,[Semester_Code]

      ,[Cohort]

      ,[Qual_Type]

      ,[Class_Code]

      ,[Lesson_No]

      ,LEO_Attendance_Status
	  ,LEO_Attendance_Status_Descr

      ,[LOA_Status]

      ,[Remarks1]

      ,[Debarment_Status]
	  ,Debarment_Status_Descr

      ,Department
	  ,Module_Owner
      ,[Prog]

      ,[Boe_Term]

      ,[Module_Id]

      ,[Reqmt_Term]

      ,[Module_Code]

      ,[Class_Nbr]

      ,[Student_Name]

      ,[Module_Descr]

      ,[Institution]

      ,[Class_Date]

      ,OASIS_Attendance_Status

      ,[Remarks2]

      ,[Admit_Term]

      )

         as

  (

SELECT 
a.Student_Id
,a.Academic_Year
,a.Semester
,CASE WHEN a.SemesterCode IS NOT NULL and a.SemesterCode &lt;&gt;''
THEN a.SemesterCode ELSE 
(SUBSTRING(cast(a.Academic_Year as varchar),3,2)+'0')+Cast(a.[Semester] as varchar)  END as SemesterCode
,a.Cohort
,ISNULL(a.Qual_Type,'NA')
,a.Class_Code
,a.Lesson_No
,a.Attendance_Status
,c.[Description]
,a.LOA_Status
,b.Remarks
,a.Debarment_Status
,d.[Description]
,ISNULL(Substring(f.School,1,10),'NA') as Department
,Substring(g1.School,1,10) as Module_Owner
,ISNULL(Substring(h.Programme_Code,1,5),'NA')
,Substring(a.SemesterCode,1,4)
,ISNULL(substring(e.Module_Id,1,6),'NA')
,b.Reqmt_Term
,substring(e.Module_Code,1,10)
,ISNULL(b.Class_Nbr,-1)
,b.Student_Name
,Substring(e.Module_Name,1,30)
,ISnull(b.Inst,'NA')
,ISNULL(b.Class_Date,'')
,b.Attd_Status
,b.Remarks
,b.Admit_Term
FROM RPDW_DEV_STG.[dbo].[STG_LEO_REGULAR_ATTENDANCE] a left join RPDW_DEV_STG.[dbo].[STG_OASIS_STUDENT_ATTD] b
  on b.[Latest_Ind]='Y' and a.Student_ID=b.Student_ID and a.Class_Code=b.Class and a.Lesson_No=b.Class_Mtg_Nbr 
   and a.SemesterCode=b.BOE_Term

Left Join RPDW_DEV_STG.[dbo].[STG_LEO_MASTER_CODE] c ON a.Attendance_Status=c.[value] and c.Data_Element='Attendance_Status'
Left Join RPDW_DEV_STG.[dbo].[STG_LEO_MASTER_CODE] d ON a.Debarment_Status=d.[value] and d.Data_Element='Debarment_Status'
LEFT JOIN (select distinct b1.Module_Code,b1.Module_Name,b1.Id,b1.Module_Id from 
RPDW_DEV_STG.[dbo].[STG_LEO_REGULAR_ATTENDANCE] a1 left join RPDW_DEV_STG.[dbo].[STG_LEO_MODULE] b1
ON a1.Module_ID=b1.Id and a1.SemesterCode=b1.Semester_Code)e ON a.Module_ID=e.Id
LEFT JOIN(select distinct a2.school,a2.Student_Id  from RPDW_DEV_STG.[dbo].[STG_LEO_REGULAR_ATTENDANCE] a1 left join 
RPDW_DEV_STG.[dbo].[STG_LEO_STUDENT_INFO_COMMON] a2 ON a1.Student_Id=a2.Student_Id and a1.Qual_Type=a2.Qual_Type) f on f.Student_Id=a.Student_Id
LEFT join RPDW_DEV_STG.[dbo].[STG_LEO_STUDENT_INFO_COMMON] g on g.Student_Id=a.Student_Id and g.Qual_Type=a.Qual_Type
LEFT JOIN RPDW_DEV_STG.[dbo].[STG_LEO_PROG] h ON h.ID=g.Prog_ID
LEFT JOIN  RPDW_DEV_STG.[dbo].[STG_LEO_MODULE] g1 on a.Module_ID=g1.ID
where a.[Latest_Ind]='Y' 
and a.SemesterCode&gt;='1700' and a.Qual_Type&lt;&gt;'STAFFQT'



UNION 

SELECT a.Student_Id
,cast('20'+substring(a.Boe_Term,1,2) as INT) as Academic_Year
,CAST(substring(a.Boe_Term,3,1) as INT) as Semester
,a.Boe_Term
,b.Cohort
,ISNULL(a.Qual_Type,'NA')
,a.Class
,a.Class_Mtg_Nbr
,b.Attendance_Status
,c.[Description]
,b.LOA_Status
,b.Remarks
,b.Debarment_Status
,d.[Description]
,ISnull(e.School_Centre,'NA')
,a.School_Centre
,ISNUll(a.Prog,'NA')
,a.Boe_Term
,ISNULL(a.Module_Id,'NA')
,a.Reqmt_Term
,a.Module_Code
,ISNULL(a.Class_Nbr,-1)
,a.Student_Name
,a.Module_Desc
,ISNULL(a.Inst,'NA')
,ISNULL(a.Class_Date,'')
,a.Attd_Status
,b.Remarks
,a.Admit_Term

FROM RPDW_DEV_STG.[dbo].[STG_OASIS_STUDENT_ATTD] a left join RPDW_DEV_STG.[dbo].[STG_LEO_REGULAR_ATTENDANCE] b
   on a.[Latest_Ind]='Y' and a.Student_ID=b.Student_ID and a.Class=b.Class_Code and a.Class_Mtg_Nbr=b.Lesson_No 
   and a.Boe_Term=b.SemesterCode

 Left Join RPDW_DEV_STG.[dbo].[STG_LEO_MASTER_CODE] c ON b.Attendance_Status=c.[value] and c.Data_Element='Attendance_Status'
Left Join RPDW_DEV_STG.[dbo].[STG_LEO_MASTER_CODE] d ON b.Debarment_Status=d.[value] and d.Data_Element='Debarment_Status'

Left Join (select  PROG_CODE,Prog_Formal_Descr,School_Centre from RPDW_DEV_STG.[dbo].[STG_OASIS_PROG_TABLE] a1
		 where a1.LATEST_IND='Y'  and a1.EFFECTIVE_DATE in (select max(b1.EFFECTIVE_DATE) from RPDW_DEV_STG.[dbo].[STG_OASIS_PROG_TABLE] b1
		 where b1.Prog_Code=a1.PROG_CODE)) e on a.Prog=e.Prog_Code
where a.[Latest_Ind]='Y' and (a.Boe_Term&lt;'1700' or a.Boe_Term in('1741','1751','1752','2013','2014','2015','2016','2017','2018','2019'))
and a.Qual_Type&lt;&gt;'STFF' and a.[Modified_On]&gt;@LastDWProcessdate)



       



MERGE INTO dbo.DW_STUDENT_ATTENDANCE AS dst

   USING TMP_DW_STUDENT_ATTENDANCE AS src

   ON( dst.Student_Id=src.Student_Id and dst.Class_Code=src.Class_Code and dst.Lesson_No=Src.Lesson_No and dst.Semester_Code=src.Semester_Code

  and  dst.Class_Nbr=src.Class_Nbr and dst.Qual_Type=src.Qual_Type and dst.Department=src.Department and dst.Prog=src.Prog
  and dst.Module_Id=src.Module_Id and dst.Institution=src.Institution and dst.Class_Date=src.Class_Date)



   WHEN NOT MATCHED THEN

   INSERT

         ( [Student_ID]

      ,[Acad_Year]

      ,[Semester]

      ,[Semester_Code]

      ,[Cohort]

      ,[Qual_Type]

      ,[Class_Code]

      ,[Lesson_No]

      ,LEO_Attendance_Status
	  ,LEO_Attendance_Status_Descr

      ,[LOA_Status]

      ,[Remarks1]

      ,[Debarment_Status]
	  ,Debarment_Status_Descr

      ,Department
	  ,Module_Owner

      ,[Prog]

      ,[Boe_Term]

      ,[Module_Id]

      ,[Reqmt_Term]

      ,[Module_Code]

      ,[Class_Nbr]

      ,[Student_Name]

      ,[Module_Descr]

      ,[Institution]

      ,[Class_Date]

      ,OASIS_Attendance_Status

      ,[Remarks2]

      ,[Admit_Term]

       ,[Created_On]

      ,[Modified_On])

   VALUES(  

           SRC.[Student_ID]

      ,SRC.[Acad_Year]

      ,SRC.[Semester]

      ,SRC.[Semester_Code]

      ,SRC.[Cohort]

      ,SRC.[Qual_Type]

      ,SRC.[Class_Code]

      ,SRC.[Lesson_No]

      ,SRC.LEO_Attendance_Status
	  ,SRC.LEO_Attendance_Status_Descr

      ,SRC.[LOA_Status]

      ,SRC.[Remarks1]

      ,SRC.[Debarment_Status]
	  ,SRC.Debarment_Status_Descr
      ,SRC.Department
	  ,SRC.Module_Owner

      ,SRC.[Prog]

      ,SRC.[Boe_Term]

      ,SRC.[Module_Id]

      ,SRC.[Reqmt_Term]

      ,SRC.[Module_Code]

      ,SRC.[Class_Nbr]

      ,SRC.[Student_Name]

      ,SRC.[Module_Descr]

      ,SRC.[Institution]

      ,SRC.[Class_Date]

      ,SRC.OASIS_Attendance_Status

      ,SRC.[Remarks2]

      ,SRC.[Admit_Term]

       ,  getdate(),getdate())

   WHEN MATCHED

   

   THEN UPDATE

   SET        dst.Acad_Year=src.Acad_Year

              ,dst.Semester=src.Semester

           

              ,dst.Cohort=src.Cohort

			  --,dst.Qual_Type=src.Qual_Type

			  ,dst.LEO_Attendance_Status=src.LEO_Attendance_Status

			  ,dst.Debarment_Status=src.Debarment_Status

			  ,dst.Module_Owner=src.Module_Owner

			 -- ,dst.Prog=src.Prog

			  ,dst.Boe_Term=src.Boe_Term

			  --,dst.Module_Id=src.Module_Id

			  ,dst.Reqmt_Term=src.Reqmt_Term

			  ,dst.Module_Code=src.Module_Code

			

			  ,dst.Student_Name=src.Student_Name

			 ,dst.Module_Descr=src.Module_Descr

			 -- ,dst.Institution=src.Institution

			  -- ,dst.Class_Date=src.Class_Date

			    ,dst.OASIS_Attendance_Status=src.OASIS_Attendance_Status

				 ,dst.Remarks2=src.Remarks2

				  ,dst.Admit_Term=src.Admit_Term
				  ,dst.Debarment_Status_Descr=src.Debarment_Status_Descr
				  ,dst.LEO_Attendance_Status_Descr=src.LEO_Attendance_Status_Descr



			 ,dst.Modified_On=getdate();

              



--------------------------------------

COMMIT TRANSACTION

END TRY



