      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from  [dbo].[DW_EXT_NON_ACAD_STUDENT_AWARDS]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;;with &#xA;TMP_DW_EXT_NON_ACAD_STUDENT_AWARDS ([Student_ID]&#xA;      ,[Student_Name]&#xA;      ,[Sch]&#xA;      ,[Dip]&#xA;      ,[Mentor]&#xA;      ,[Award_Category]&#xA;      ,[Award_Yr]&#xA;      ,[Award_Name]&#xA;      ,[Club]&#xA;      ,[Interest_Grp]&#xA;      ,[Qualifying_Period]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT  [Student_ID]&#xA;      ,[Student_Name]&#xA;      ,[Sch]&#xA;      ,[Dip]&#xA;      ,[Mentor]&#xA;      ,[Award_Category]&#xA;      ,[Award_Yr]&#xA;      ,[Award_Name]&#xA;      ,[Club]&#xA;      ,[Interest_Grp]&#xA;      ,[Qualifying_Period]&#xA;  FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_EXT_NON_ACAD_STUD_AWARDS] a&#xA;         where a.[Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)&#xA;&#xA;       &#xA;&#xA;MERGE INTO dbo.DW_EXT_NON_ACAD_STUDENT_AWARDS AS dst&#xA;   USING TMP_DW_EXT_NON_ACAD_STUDENT_AWARDS AS src&#xA;   ON( dst.[Student_ID]=src.[Student_ID] and dst.[Award_Yr]=src.[Award_Yr]&#xA;   and dst.[Award_Name]=src.[Award_Name] and dst.[Interest_Grp]=src.[Interest_Grp])&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT&#xA;         ( [Student_ID]&#xA;      ,[Student_Name]&#xA;      ,[Sch]&#xA;      ,[Dip]&#xA;      ,[Mentor]&#xA;      ,[Award_Category]&#xA;      ,[Award_Yr]&#xA;      ,[Award_Name]&#xA;      ,[Club]&#xA;      ,[Interest_Grp]&#xA;      ,[Qualifying_Period]&#xA;&#x9;  ,[CREATED_ON]&#xA;      ,[MODIFIED_ON])&#xA;   VALUES(  &#xA;           src.[Student_ID]&#xA;      ,src.[Student_Name]&#xA;      ,src.[Sch]&#xA;      ,src.[Dip]&#xA;      ,src.[Mentor]&#xA;      ,src.[Award_Category]&#xA;      ,src.[Award_Yr]&#xA;      ,src.[Award_Name]&#xA;      ,src.[Club]&#xA;      ,src.[Interest_Grp]&#xA;      ,src.[Qualifying_Period]&#xA;       ,  getdate(),getdate()&#xA;&#x9;  )&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET        dst.[Student_Name]=src.[Student_Name]&#xA;              ,dst.[Sch]=src.[Sch]&#xA;              ,dst.[Dip]=src.[Dip]&#xA;              ,dst.[Mentor]=src.[Mentor]&#xA;&#xA;&#xA;&#x9;&#x9;&#x9;     ,dst.[Award_Category]=src.[Award_Category]&#xA;&#x9;&#x9;&#x9;&#x9;    ,dst.[Club]=src.[Club]&#xA;&#x9;&#x9;,dst.[Qualifying_Period]=src.[Qualifying_Period]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;      &#x9;&#x9;&#xA;&#xA;&#x9;&#x9;&#x9;   ,dst.Modified_On=getdate();&#xA;&#x9;&#x9;&#x9; &#xA;              &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
