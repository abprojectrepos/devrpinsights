      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_SCHOLARSHIP_BURSARY_SETUP]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;&#xA;&#xA;;with TMP_DW_SCHOLARSHIP_BURSARY_SETUP (&#xA;&#x9;&#x9;&#x9;[Inst]&#xA;&#x9;&#x9;&#x9;,[App_Type]&#xA;&#x9;&#x9;&#x9;,[Desc]&#xA;&#x9;&#x9;&#x9;,[Award_Category]&#xA;&#x9;&#x9;&#x9;,[Award_Category_Desc]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT &#x9; &#xA;&#x9;&#x9;&#x9;[Inst]&#xA;&#x9;&#x9;&#x9;,[App_Type]&#xA;&#x9;&#x9;&#x9;,[Desc]&#xA;&#x9;&#x9;&#x9;,[Award_Category]&#xA;&#x9;&#x9;&#x9;,case when b.Long_Name is not null then b.Long_Name&#xA;&#x9;&#x9;&#x9;&#x9;else NULL&#xA;&#x9;&#x9;&#x9;&#x9;end [Award_Category_Desc]&#xA;  FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_SCH_BUR_SETUP] a&#xA;&#x9;LEFT JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_MASTER_CODE] b on a.[Award_Category]=b.FIELD_VALUE and b.FIELD_NAME='A_AWD_CATG' and b.EFFECTIVE_STATUS = 'A' &#xA;&#x9;where a.[Latest_Ind]='Y' and a.[Modified_On]&gt;@LastDWProcessdate)&#xA;       &#xA;&#xA;MERGE INTO dbo.DW_SCHOLARSHIP_BURSARY_SETUP AS dst&#xA;   USING TMP_DW_SCHOLARSHIP_BURSARY_SETUP AS src&#xA;   ON( dst.[Inst]=src.[Inst] and dst.[App_Type]=src.[App_Type] and dst.[Award_Category]=src.[Award_Category] )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT(&#xA;&#x9;&#x9;&#x9;[Inst]&#xA;&#x9;&#x9;&#x9;,[App_Type]&#xA;&#x9;&#x9;&#x9;,[Desc]&#xA;&#x9;&#x9;&#x9;,[Award_Category]&#xA;&#x9;&#x9;&#x9;,[Award_Category_Desc]&#xA;&#x9;&#x9;&#x9;,[Created_On]&#xA;&#x9;&#x9;&#x9;,[Modified_On] )&#xA;   VALUES(  &#xA;&#x9;&#x9;&#x9;src.[Inst]&#xA;&#x9;&#x9;&#x9;,src.[App_Type]&#xA;&#x9;&#x9;&#x9;,src.[Desc]&#xA;&#x9;&#x9;&#x9;,src.[Award_Category]&#xA;&#x9;&#x9;&#x9;,src.[Award_Category_Desc]&#xA;&#x9;&#x9;&#x9;,getdate()&#xA;&#x9;&#x9;&#x9;,getdate())&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET   &#xA;&#x9;&#x9;&#x9;dst.[Desc]=src.[Desc]&#xA;&#x9;&#x9;&#x9;,dst.[Award_Category_Desc]=src.[Award_Category_Desc]&#xA;&#x9;&#x9;&#x9;,dst.Modified_On=getdate();&#xA;&#x9;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&quot;"
