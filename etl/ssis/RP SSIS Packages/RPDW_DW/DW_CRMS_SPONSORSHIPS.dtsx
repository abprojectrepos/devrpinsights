      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_CRMS_SPONSORSHIPS]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_CRMS_SPONSORSHIPS (
		[Sponsorship_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[Award_Name]
      ,[Effective_Date]
      ,[RP_Dept]
      ,[Award_ID]
      ,[Owner]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
      )
         as
  (
SELECT 	
		[Sponsorship_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[Award_Name]
      ,[Effective_Date]
      ,[RP_Dept]
      ,[Award_ID]
      ,[Owner]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
  FROM RPDW_STG.[dbo].[STG_CRMS_SPONSORSHIP]
  where [Latest_Ind]='Y'  and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_CRMS_SPONSORSHIPS AS dst
   USING TMP_DW_CRMS_SPONSORSHIPS AS src
   ON( dst.[Sponsorship_ID]=src.[Sponsorship_ID] )

   WHEN NOT MATCHED THEN
   INSERT
		( 	
		[Sponsorship_ID]
		,[Acct_ID]
		,[Acct_Name]
		,[Award_Name]
		,[Effective_Date]
		,[RP_Dept]
		,[Award_ID]
		,[Owner]
		,[CRMS_Created_On]
		,[CRMS_Modified_On]
		,[Created_On]
		,[Modified_On] )
   VALUES(  
		src.[Sponsorship_ID]
		,src.[Acct_ID]
		,src.[Acct_Name]
		,src.[Award_Name]
		,src.[Effective_Date]
		,src.[RP_Dept]
		,src.[Award_ID]
		,src.[Owner]
		,src.[CRMS_Created_On]
		,src.[CRMS_Modified_On]
		,getdate()
		,getdate()
	   )
   WHEN MATCHED
   
   THEN UPDATE
   SET   
		dst.[Acct_ID]=src.[Acct_ID]
		,dst.[Acct_Name]=src.[Acct_Name]
		,dst.[Award_Name]=src.[Award_Name]
		,dst.[Effective_Date]=src.[Effective_Date]
		,dst.[RP_Dept]=src.[RP_Dept]
		,dst.[Award_ID]=src.[Award_ID]
		,dst.[Owner]=src.[Owner]
		,dst.[CRMS_Created_On]=src.[CRMS_Created_On]
		,dst.[CRMS_Modified_On]=src.[CRMS_Modified_On]
		,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO</DTS:VariableValue>
