      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(5)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_CET_COMPLETION_REPORT]--dw table&#xA;&#xA;&#xA;&#xA;&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;&#xA;DELETE FROM [dbo].[DW_CET_COMPLETION_REPORT]&#xA;&#xA; INSERT [dbo].[DW_CET_COMPLETION_REPORT]&#xA;SELECT  a.Student_ID&#xA;,a.Run_Title&#xA;,a.School_Centre&#xA;,a.Prog&#xA;,d.Prog_Formal_Descr&#xA;,a.Prog_Type&#xA;,a.Qual_Type&#xA;,b.Long_Name&#xA;,a.Admit_Term&#xA;,a.Admit_Date&#xA;,a.Specialisatn&#xA;,a.Run_Number&#xA;,a.Prog_Status&#xA;,c.Long_Name&#xA;,a.Effective_Date&#xA;,a.Reqmt_Term&#xA;,a.Grad_Term&#xA;,a.Gpa&#xA;,a.C_Gpa&#xA;,a.Student_Career_Number&#xA;,a.Effective_Seq&#xA;,a.Term&#xA;,getdate()&#xA;,getdate()&#xA;&#xA;  FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_COMPLETION_REPORT] a &#xA;         &#xA;&#x9;&#x9;Left JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_MASTER_CODE] b&#xA;  on a.QUAL_TYPE=b.FIELD_VALUE and b.FIELD_NAME='ACAD_CAREER' and b.EFFECTIVE_STATUS = 'A' &#xA;&#xA;  Left JOIN &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_MASTER_CODE] c&#xA;  on a.Prog_Status=c.FIELD_VALUE and c.FIELD_NAME='PROG_STATUS' and c.EFFECTIVE_STATUS = 'A' &#xA;&#xA;   Left JOIN&#xA;  ( select  PROG_CODE,Prog_Formal_Descr from &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_PROG_TABLE] a1&#xA;&#x9;&#x9; where a1.LATEST_IND='Y'  and a1.EFFECTIVE_DATE in (select max(b1.EFFECTIVE_DATE) from &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_PROG_TABLE] b1&#xA;&#x9;&#x9; where b1.Prog_Code=a1.PROG_CODE)) d on a.Prog=d.Prog_Code&#xA;&#xA;INNER JOIN (select Student_ID,Qual_Type,max(Term) as Term,Prog&#xA;&#xA;from &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_OASIS_COMPLETION_REPORT]&#xA;group by Student_ID,Qual_Type,Prog) e ON a.Student_ID=e.Student_ID and a.Prog=e.Prog and a.Term=e.Term and a.Qual_Type=e.Qual_Type&#xA;&#x9;&#x9; &#xA;&#x9;&#x9; where a.[Latest_Ind]='Y'&#xA;&#xA;   &#xA;              &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&#xA;&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
