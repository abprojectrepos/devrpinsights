      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_CET_COMPLETION_REPORT]--dw table




--select  @processeddatetime=?
--select @LastDWProcessdate

DELETE FROM [dbo].[DW_CET_COMPLETION_REPORT]

 INSERT [dbo].[DW_CET_COMPLETION_REPORT]
SELECT  a.Student_ID
,a.Run_Title
,a.School_Centre
,a.Prog
,d.Prog_Formal_Descr
,a.Prog_Type
,a.Qual_Type
,b.Long_Name
,a.Admit_Term
,a.Admit_Date
,a.Specialisatn
,a.Run_Number
,a.Prog_Status
,c.Long_Name
,a.Effective_Date
,a.Reqmt_Term
,a.Grad_Term
,a.Gpa
,a.C_Gpa
,a.Student_Career_Number
,a.Effective_Seq
,a.Term
,getdate()
,getdate()

  FROM RPDW_STG_STG.[dbo].[STG_OASIS_COMPLETION_REPORT] a 
         
		Left JOIN RPDW_STG_STG.[dbo].[STG_OASIS_MASTER_CODE] b
  on a.QUAL_TYPE=b.FIELD_VALUE and b.FIELD_NAME='ACAD_CAREER' and b.EFFECTIVE_STATUS = 'A' 

  Left JOIN RPDW_STG_STG.[dbo].[STG_OASIS_MASTER_CODE] c
  on a.Prog_Status=c.FIELD_VALUE and c.FIELD_NAME='PROG_STATUS' and c.EFFECTIVE_STATUS = 'A' 

   Left JOIN
  ( select  PROG_CODE,Prog_Formal_Descr from RPDW_STG_STG.[dbo].[STG_OASIS_PROG_TABLE] a1
		 where a1.LATEST_IND='Y'  and a1.EFFECTIVE_DATE in (select max(b1.EFFECTIVE_DATE) from RPDW_STG_STG.[dbo].[STG_OASIS_PROG_TABLE] b1
		 where b1.Prog_Code=a1.PROG_CODE)) d on a.Prog=d.Prog_Code

INNER JOIN (select Student_ID,Qual_Type,max(Term) as Term,Prog

from RPDW_STG_STG.[dbo].[STG_OASIS_COMPLETION_REPORT]
group by Student_ID,Qual_Type,Prog) e ON a.Student_ID=e.Student_ID and a.Prog=e.Prog and a.Term=e.Term and a.Qual_Type=e.Qual_Type
		 
		 where a.[Latest_Ind]='Y'

   
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO

