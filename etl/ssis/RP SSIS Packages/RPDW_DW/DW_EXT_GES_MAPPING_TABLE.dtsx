      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_EXT_GES_MAPPING_TABLE]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;&#xA;&#xA;;with TMP_DW_EXT_GES_MAPPING_TABLE (&#xA;&#x9;&#x9;[RP_Column_Header]&#xA;&#x9;&#x9;,[Column_Header]&#xA;&#x9;&#x9;,[Label]&#xA;&#x9;&#x9;,[Type]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT &#x9;&#x9;&#xA;&#x9;&#x9;a.[RP_Column_Header]&#xA;&#x9;&#x9;,a.[Column_Header]&#xA;&#x9;&#x9;,a.[Label]&#xA;&#x9;&#x9;,a.[Type]&#xA;FROM (&#xA;SELECT &#x9;&#xA;&#x9;&#x9;[RP_Column_Header]&#xA;&#x9;&#x9;,[Column_Header]&#xA;&#x9;&#x9;,[Label]&#xA;&#x9;&#x9;,'FGPNS' [Type]&#xA;FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_EXT_GES_FGPNS_MAPPNG] &#xA;where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate&#xA;UNION &#xA;SELECT &#x9;&#xA;&#x9;&#x9;[RP_Column_Header]&#xA;&#x9;&#x9;,[Column_Header]&#xA;&#x9;&#x9;,[Label]&#xA;&#x9;&#x9;,'PG' [Type]&#xA;from &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_EXT_GES_PG_MAPPING] &#xA;where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate) a&#xA;)&#xA;       &#xA;MERGE INTO dbo.DW_EXT_GES_MAPPING_TABLE AS dst&#xA;   USING TMP_DW_EXT_GES_MAPPING_TABLE AS src&#xA;   ON( dst.[RP_Column_Header]=src.[RP_Column_Header] )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT&#xA;&#x9;&#x9;( &#xA;&#x9;&#x9;&#x9;[RP_Column_Header]&#xA;&#x9;&#x9;&#x9;,[Column_Header]&#xA;&#x9;&#x9;&#x9;,[Label]&#xA;&#x9;&#x9;&#x9;,[Type]&#xA;&#x9;&#x9;&#x9;,[Created_On]&#xA;&#x9;&#x9;&#x9;,[Modified_On] )&#xA;   VALUES(  &#xA;&#x9;&#x9;&#x9;src.[RP_Column_Header]&#xA;&#x9;&#x9;&#x9;,src.[Column_Header]&#xA;&#x9;&#x9;&#x9;,src.[Label]&#xA;&#x9;&#x9;&#x9;,src.[Type]&#xA;&#x9;&#x9;&#x9;,getdate()&#xA;&#x9;&#x9;&#x9;,getdate()&#xA;&#x9;   )&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET   &#xA;&#x9;&#x9;&#x9;dst.[Column_Header]=src.[Column_Header]&#xA;&#x9;&#x9;&#x9;,dst.[Label]=src.[Label]&#xA;&#x9;&#x9;&#x9;,dst.[Type]=src.[Type]&#xA;&#x9;&#x9;&#x9;,dst.Modified_On=getdate();&#xA;&#x9;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL_DW_Load">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_EXT_GES_MAPPING_TABLE]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_EXT_GES_MAPPING_TABLE (
		[RP_Column_Header]
		,[Column_Header]
		,[Label]
		,[Type]
      )
         as
  (
SELECT 		
		a.[RP_Column_Header]
		,a.[Column_Header]
		,a.[Label]
		,a.[Type]
FROM (
SELECT 	
		[RP_Column_Header]
		,[Column_Header]
		,[Label]
		,'FGPNS' [Type]
FROM RPDW_STG.[dbo].[STG_EXT_GES_FGPNS_MAPPNG] 
where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate
UNION 
SELECT 	
		[RP_Column_Header]
		,[Column_Header]
		,[Label]
		,'PG' [Type]
from RPDW_STG.[dbo].[STG_EXT_GES_PG_MAPPING] 
where [Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate) a
)
       
MERGE INTO dbo.DW_EXT_GES_MAPPING_TABLE AS dst
   USING TMP_DW_EXT_GES_MAPPING_TABLE AS src
   ON( dst.[RP_Column_Header]=src.[RP_Column_Header] )

   WHEN NOT MATCHED THEN
   INSERT
		( 
			[RP_Column_Header]
			,[Column_Header]
			,[Label]
			,[Type]
			,[Created_On]
			,[Modified_On] )
   VALUES(  
			src.[RP_Column_Header]
			,src.[Column_Header]
			,src.[Label]
			,src.[Type]
			,getdate()
			,getdate()
	   )
   WHEN MATCHED
   
   THEN UPDATE
   SET   
			dst.[Column_Header]=src.[Column_Header]
			,dst.[Label]=src.[Label]
			,dst.[Type]=src.[Type]
			,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO</DTS:VariableValue>
