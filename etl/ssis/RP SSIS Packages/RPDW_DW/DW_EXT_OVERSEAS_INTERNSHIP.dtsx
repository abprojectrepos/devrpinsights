      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from  [dbo].[DW_EXT_OVERSEAS_INTERNSHIP]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;;with &#xA;TMP_DW_EXT_OVERSEAS_INTERNSHIP ([Student_ID]&#xA;      ,[Sch]&#xA;      ,[Course_Code]&#xA;      ,[Internship_Yr]&#xA;      ,[Internship_Sem]&#xA;      ,[Internship_Country]&#xA;      ,[City]&#xA;      ,[Comp]&#xA;      ,[Country_Maturity]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT  [Student_ID]&#xA;      ,[Sch]&#xA;      ,[Course_Code]&#xA;      ,[Internship_Yr]&#xA;      ,[Internship_Sem]&#xA;      ,[Internship_Country]&#xA;      ,[City]&#xA;      ,[Comp]&#xA;      ,[Country_Maturity]&#xA;  FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_EXT_OVERSEAS_INTERNSHIP] a&#xA;         where a.[Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)&#xA;&#xA;       &#xA;&#xA;MERGE INTO dbo.DW_EXT_OVERSEAS_INTERNSHIP AS dst&#xA;   USING TMP_DW_EXT_OVERSEAS_INTERNSHIP AS src&#xA;   ON( dst.[Student_ID]=src.[Student_ID] and dst.[Internship_Yr]=src.[Internship_Yr]&#xA;   and dst.[Internship_Sem]=src.[Internship_Sem] and dst.[Comp]=src.[Comp]&#xA;    )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT&#xA;         ( [Student_ID]&#xA;      ,[Sch]&#xA;      ,[Course_Code]&#xA;      ,[Internship_Yr]&#xA;      ,[Internship_Sem]&#xA;      ,[Internship_Country]&#xA;      ,[City]&#xA;      ,[Comp]&#xA;      ,[Country_Maturity]&#xA;&#x9;  ,[CREATED_ON]&#xA;      ,[MODIFIED_ON])&#xA;   VALUES(  &#xA;           src.[Student_ID]&#xA;      ,src.[Sch]&#xA;      ,src.[Course_Code]&#xA;      ,src.[Internship_Yr]&#xA;      ,src.[Internship_Sem]&#xA;      ,src.[Internship_Country]&#xA;      ,src.[City]&#xA;      ,src.[Comp]&#xA;      ,src.[Country_Maturity]&#xA;       ,  getdate(),getdate()&#xA;&#x9;  )&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET        dst.[Sch]=src.[Sch]&#xA;              ,dst.[Course_Code]=src.[Course_Code]&#xA;              ,dst.[Internship_Country]=src.[Internship_Country]&#xA;&#x9;&#x9;&#x9;  ,dst.[City]=src.[City]&#xA;&#x9;&#x9;&#x9;  ,dst.[Country_Maturity]=src.[Country_Maturity]&#xA;              &#xA;&#xA;&#x9;&#x9;&#x9;   ,dst.Modified_On=getdate();&#xA;&#x9;&#x9;&#x9; &#xA;              &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="SQL_DW_Load">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from  [dbo].[DW_EXT_OVERSEAS_INTERNSHIP]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate
;with 
TMP_DW_EXT_OVERSEAS_INTERNSHIP ([Student_ID]
      ,[Sch]
      ,[Course_Code]
      ,[Internship_Yr]
      ,[Internship_Sem]
      ,[Internship_Country]
      ,[City]
      ,[Comp]
      ,[Country_Maturity]
      )
         as
  (
SELECT  [Student_ID]
      ,[Sch]
      ,[Course_Code]
      ,[Internship_Yr]
      ,[Internship_Sem]
      ,[Internship_Country]
      ,[City]
      ,[Comp]
      ,[Country_Maturity]
  FROM RPDW_STG.[dbo].[STG_EXT_OVERSEAS_INTERNSHIP] a
         where a.[Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_EXT_OVERSEAS_INTERNSHIP AS dst
   USING TMP_DW_EXT_OVERSEAS_INTERNSHIP AS src
   ON( dst.[Student_ID]=src.[Student_ID] and dst.[Internship_Yr]=src.[Internship_Yr]
   and dst.[Internship_Sem]=src.[Internship_Sem] and dst.[Comp]=src.[Comp]
    )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Student_ID]
      ,[Sch]
      ,[Course_Code]
      ,[Internship_Yr]
      ,[Internship_Sem]
      ,[Internship_Country]
      ,[City]
      ,[Comp]
      ,[Country_Maturity]
	  ,[CREATED_ON]
      ,[MODIFIED_ON])
   VALUES(  
           src.[Student_ID]
      ,src.[Sch]
      ,src.[Course_Code]
      ,src.[Internship_Yr]
      ,src.[Internship_Sem]
      ,src.[Internship_Country]
      ,src.[City]
      ,src.[Comp]
      ,src.[Country_Maturity]
       ,  getdate(),getdate()
	  )
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.[Sch]=src.[Sch]
              ,dst.[Course_Code]=src.[Course_Code]
              ,dst.[Internship_Country]=src.[Internship_Country]
			  ,dst.[City]=src.[City]
			  ,dst.[Country_Maturity]=src.[Country_Maturity]
              

			   ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
