      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_SCHLRSP_BURSY_DISB_TXN]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_SCHLRSP_BURSY_DISB_TXN (
      [Inst]
      ,[Award_ID]
      ,[Effective_Date]
      ,[Fin_Yr]
      ,[Line_Num]
      ,[Spnsr_Name]
      ,[Schship_Bur_Name]
      ,[Award_Category]
      ,[Disbursed_Amt]
      ,[Disbursement_Date]
      )
         as
  (
SELECT [Inst]
      ,[Award_ID]
      ,[Effective_Date]
      ,[Fin_Yr]
      ,[Line_Num]
      ,[Spnsr_Name]
      ,[Schship_Bur_Name]
      ,[Award_Category]
      ,[Disbursed_Amt]
      ,[Disbursement_Date]
  FROM RPDW_STG.[dbo].[STG_OASIS_SCH_BUR_DISBURSE_TXN] a 
   where a.[Latest_Ind]='Y'  and a.[Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_SCHLRSP_BURSY_DISB_TXN AS dst
   USING TMP_DW_SCHLRSP_BURSY_DISB_TXN AS src
   ON( dst.[Inst]=src.[Inst] and dst.[Award_ID]=src.[Award_ID] and dst.[Effective_Date]=Src.[Effective_Date] and 
   dst.[Fin_Yr]=src.[Fin_Yr] and dst.[Line_Num] =src.[Line_Num] )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Inst]
      ,[Award_ID]
      ,[Effective_Date]
      ,[Fin_Yr]
      ,[Line_Num]
      ,[Spnsr_Name]
      ,[Schship_Bur_Name]
      ,[Award_Category]
      ,[Disbursed_Amt]
      ,[Disbursement_Date]
       ,[Created_On]
      ,[Modified_On])
   VALUES(  
           [Inst]
      ,[Award_ID]
      ,[Effective_Date]
      ,[Fin_Yr]
      ,[Line_Num]
      ,[Spnsr_Name]
      ,[Schship_Bur_Name]
      ,[Award_Category]
      ,[Disbursed_Amt]
      ,[Disbursement_Date]
       ,  getdate(),getdate())
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.[Spnsr_Name]=src.[Spnsr_Name]
              ,dst.[Schship_Bur_Name]=src.[Schship_Bur_Name]
              ,dst.[Award_Category]=src.[Award_Category]
              ,dst.[Disbursed_Amt]=src.[Disbursed_Amt]

			   ,dst.[Disbursement_Date]=src.[Disbursement_Date]
			    
			   ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
