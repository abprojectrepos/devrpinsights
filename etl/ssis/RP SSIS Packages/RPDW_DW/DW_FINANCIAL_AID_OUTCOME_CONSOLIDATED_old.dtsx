	  WHEN Item='Payment by Mendaki'THEN 'MDK'
	  WHEN Item='Payment by TFL'THEN 'TFL' END AS App_Type
,'PAYM' as App_Status
,'Payment' as App_Status_Desc
,Item_Type as Award_ID
,Item as Award_Description
,Acct_Term
