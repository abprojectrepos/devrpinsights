      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_EXT_GES_DATA
select  @processeddatetime= getdate()
select @jobstatusid=0
--select @LastDWProcessdate
;WITH TMP_DW_EXT_GES_DATA
([RP_School]
      ,[Diploma]
      ,[Survey_Type]
      ,[Year_of_GES]
      ,[Emp_Type]
      ,[No_Of_Resp_For_Emp_Type]
      ,[Median_Gross_Salary_FTP]
      ,[No_Of_Resp_For_Median_Gross_Salary]
      ,[Diploma_Code]

)

AS 

(
select 
[RP_School]
      ,[Diploma]
      ,[Survey_Type]
      ,[Year_of_GES]
      ,[Emp_Type]
      ,[No_Of_Resp_For_Emp_Type]
      ,[Median_Gross_Salary_FTP]
      ,[No_Of_Resp_For_Median_Gross_Salary]
      ,[Diploma_Code]
from RPDW_UAT_STG..STG_EXT_GES_DATA
where latest_ind='Y')

MERGE INTO DW_EXT_GES_DATA AS dst
   USING TMP_DW_EXT_GES_DATA AS src
   ON( dst.[Diploma]=src.[Diploma] and dst.[Year_of_GES]=src.[Year_of_GES] and dst.[Emp_Type]=src.[Emp_Type] 
  )

   WHEN NOT MATCHED THEN
   INSERT
	  ([RP_School]
      ,[Diploma]
      ,[Survey_Type]
      ,[Year_of_GES]
      ,[Emp_Type]
      ,[No_Of_Resp_For_Emp_Type]
      ,[Median_Gross_Salary_FTP]
      ,[No_Of_Resp_For_Median_Gross_Salary]
      ,[Diploma_Code]
      ,[CREATED_ON]
      ,[MODIFIED_ON])
   VALUES(  
 SRC.[RP_School]
,SRC.[Diploma]
,SRC.[Survey_Type]
,SRC.[Year_of_GES]
,SRC.[Emp_Type]
,SRC.[No_Of_Resp_For_Emp_Type]
,SRC.[Median_Gross_Salary_FTP]
,SRC.[No_Of_Resp_For_Median_Gross_Salary]
,SRC.[Diploma_Code]
      ,@processeddatetime
	  ,@processeddatetime)
   WHEN MATCHED
   
   THEN UPDATE
   SET 	
   
    dst.[RP_School]=src.[RP_School]
   ,dst.Survey_Type=src.Survey_Type
      ,dst.No_Of_Resp_For_Emp_Type=src.No_Of_Resp_For_Emp_Type
   ,dst.Median_Gross_Salary_FTP=src.Median_Gross_Salary_FTP
   ,dst.[No_Of_Resp_For_Median_Gross_Salary]=src.[No_Of_Resp_For_Median_Gross_Salary]
   ,dst.[Diploma_Code]=src.[Diploma_Code]
,dst.Modified_On=@processeddatetime;
		

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
END CATCH  
GO</DTS:VariableValue>
