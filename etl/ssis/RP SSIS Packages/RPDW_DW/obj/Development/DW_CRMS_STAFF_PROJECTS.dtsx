      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from  [dbo].[DW_CRMS_STAFF_PROJECTS]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate
;with TMP_DW_CRMS_STAFF_PROJECTS ([Staff_Proj_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[Start_Date]
      ,[End_Date]
      ,[RP_Dept]
      ,[RP_Staff_ID]
      ,[RP_Staff_Name]
      ,[Proj_Title]
      ,[Proj_Status]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
      )
         as
  (
SELECT  [Staff_Proj_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[Start_Date]
      ,[End_Date]
      ,[RP_Dept]
      ,[RP_Staff_ID]
      ,[RP_Staff_Name]
      ,[Proj_Title]
      ,[Proj_Status]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
  FROM RPDW_UAT_STG.[dbo].[STG_CRMS_STAFF_PROJECTS] a
         where a.[Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_CRMS_STAFF_PROJECTS AS dst
   USING TMP_DW_CRMS_STAFF_PROJECTS AS src
   ON( dst.[Staff_Proj_ID]=src.[Staff_Proj_ID] )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Staff_Proj_ID]
      ,[Acct_ID]
      ,[Acct_Name]
      ,[Start_Date]
      ,[End_Date]
      ,[RP_Dept]
      ,[RP_Staff_ID]
      ,[RP_Staff_Name]
      ,[Proj_Title]
      ,[Proj_Status]
      ,[CRMS_Created_On]
      ,[CRMS_Modified_On]
	  ,[CREATED_ON]
      ,[MODIFIED_ON])
   VALUES(  
           src.[Staff_Proj_ID]
      ,src.[Acct_ID]
      ,src.[Acct_Name]
      ,src.[Start_Date]
      ,src.[End_Date]
      ,src.[RP_Dept]
      ,src.[RP_Staff_ID]
      ,src.[RP_Staff_Name]
      ,src.[Proj_Title]
      ,src.[Proj_Status]
      ,src.[CRMS_Created_On]
      ,src.[CRMS_Modified_On]
       ,  getdate(),getdate()
	  )
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.[Acct_ID]=src.[Acct_ID]
              ,dst.[Acct_Name]=src.[Acct_Name]
            
              ,dst.[RP_Dept]=src.[RP_Dept]
		     ,dst.[Proj_Title]=src.[Proj_Title]
			  ,dst.[Proj_Status]=src.[Proj_Status]
			
			    ,dst.[RP_Staff_Name]=src.[RP_Staff_Name]
			    ,dst.[Start_Date]=src.[Start_Date]
				    
				,dst.[End_Date]=src.[End_Date]
		   ,dst.[RP_Staff_ID]=src.[RP_Staff_ID]
		      ,dst.[CRMS_Created_On]=src.[CRMS_Created_On]
			     ,dst.[CRMS_Modified_On]=src.[CRMS_Modified_On]
			

			   ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
