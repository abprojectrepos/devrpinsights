      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_SCHLRSP_BURSY_DONATN_RECV]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_SCHLRSP_BURSY_DONATN_RECV (
      [Inst]
      ,[Award_ID]
      ,[Effective_Date]
      ,[Fin_Yr]
      ,[Line_Num]
      ,[Spnsr_Name]
      ,[Schship_Bur_Name]
      ,[Receipt_Num]
      ,[Donated_Amt]
      ,[Award_Category]
      ,[Txn_Date]
      ,[Item_Type]
      ,[Item_Type_Desc]
      )
         as
  (
SELECT [Inst]
      ,a.[Award_ID]
      ,a.[Effective_Date]
      ,a.[Fin_Yr]
      ,a.[Line_Num]
      ,a.[Spnsr_Name]
      ,a.[Schship_Bur_Name]
      ,a.[Receipt_Num]
      ,a.[Donated_Amt]
      ,a.[Award_Category]
      ,a.[Txn_Date]
      ,a.[Item_Type]
      ,a.[Item_Type_Desc]
  FROM RPDW_UAT_STG.[dbo].[STG_OASIS_SCH_BUR_DONATE_RECV] a 
   where a.[Latest_Ind]='Y'  and a.[Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_SCHLRSP_BURSY_DONATN_RECV AS dst
   USING TMP_DW_SCHLRSP_BURSY_DONATN_RECV AS src
   ON( dst.[Inst]=src.[Inst] and dst.[Award_ID]=src.[Award_ID] and dst.[Effective_Date]=Src.[Effective_Date] and 
   dst.[Fin_Yr]=src.[Fin_Yr] and dst.[Line_Num] =src.[Line_Num] )

   WHEN NOT MATCHED THEN
   INSERT
         ( [Inst]
      ,[Award_ID]
      ,[Effective_Date]
      ,[Fin_Yr]
      ,[Line_Num]
      ,[Spnsr_Name]
      ,[Schship_Bur_Name]
      ,[Receipt_Num]
      ,[Donated_Amt]
      ,[Award_Category]
      ,[Txn_Date]
      ,[Item_Type]
      ,[Item_Type_Desc]
       ,[Created_On]
      ,[Modified_On])
   VALUES(  
           src.[Inst]
      ,src.[Award_ID]
      ,src.[Effective_Date]
      ,src.[Fin_Yr]
      ,src.[Line_Num]
      ,src.[Spnsr_Name]
      ,src.[Schship_Bur_Name]
      ,src.[Receipt_Num]
      ,src.[Donated_Amt]
      ,src.[Award_Category]
      ,src.[Txn_Date]
      ,src.[Item_Type]
      ,src.[Item_Type_Desc]
       ,  getdate(),getdate())
   WHEN MATCHED
   
   THEN UPDATE
   SET        dst.[Spnsr_Name]=src.[Spnsr_Name]
              ,dst.[Schship_Bur_Name]=src.[Schship_Bur_Name]
              ,dst.[Receipt_Num]=src.[Receipt_Num]
              ,dst.[Donated_Amt]=src.[Donated_Amt]

			   ,dst.[Award_Category]=src.[Award_Category]

			     ,dst.[Txn_Date]=src.[Txn_Date]
				   ,dst.[Item_Type]=src.[Item_Type]
				     ,dst.[Item_Type_Desc]=src.[Item_Type_Desc]
			    
			   ,dst.Modified_On=getdate();
			 
              

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO
