      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from [dbo].[DW_EXT_DPAEAE_CDP]
select  @processeddatetime= getdate()

;WITH TMP_EXT
AS
(
SELECT 
       [ACADEMIC_YEAR]
,[SCHOOL_CODE]
,[UIN_FIN_NO]
,[DOMAIN_ICODE]
,[Module_Name]
,[FILE_NAME_ACADEMIC_YEAR]
,[FILE_NAME_SEMESTER]
,[CREATED_ON]
,[MODIFIED_ON]

	  FROM RPDW_STG.[dbo].[STG_EXT_DPAEAE_CDP]
   where [Latest_Ind] ='Y' 
AND [MODIFIED_ON]&gt;@LastDWProcessdate
)


MERGE INTO [DW_EXT_DPAEAE_CDP] AS DST
   USING TMP_EXT AS SRC
  ON (
  SRC. ACADEMIC_YEAR = DST.ACADEMIC_YEAR AND   SRC. FILE_NAME_ACADEMIC_YEAR = DST.FILE_NAME_ACADEMIC_YEAR AND   SRC. FILE_NAME_SEMESTER = DST.FILE_NAME_SEMESTER AND   SRC. SCHOOL_CODE = DST.SCHOOL_CODE AND   SRC. UIN_FIN_NO = DST.UIN_FIN_NO
     )

   WHEN NOT MATCHED THEN
   INSERT
	(
	   [ACADEMIC_YEAR]
,[SCHOOL_CODE]
,[UIN_FIN_NO]
,[DOMAIN_ICODE]
,[Module_Name]
,[FILE_NAME_ACADEMIC_YEAR]
,[FILE_NAME_SEMESTER]
,[CREATED_ON]
,[MODIFIED_ON]
	  )
VALUES
	(  
 src.[ACADEMIC_YEAR]
,src.[SCHOOL_CODE]
,src.[UIN_FIN_NO]
,src.[DOMAIN_ICODE]
,src.[Module_Name]
,src.[FILE_NAME_ACADEMIC_YEAR]
,src.[FILE_NAME_SEMESTER]
,src.[CREATED_ON]
,src.[MODIFIED_ON]

	)
   WHEN MATCHED
   
   THEN UPDATE
   SET 	 
 dst.[ACADEMIC_YEAR] = isnull(src.[ACADEMIC_YEAR],' '),
dst.[SCHOOL_CODE] = isnull(src.[SCHOOL_CODE],' '),
dst.[UIN_FIN_NO] = isnull(src.[UIN_FIN_NO],' '),
dst.[DOMAIN_ICODE] = isnull(src.[DOMAIN_ICODE],' '),
dst.[Module_Name] = isnull(src.[Module_Name],' '),
dst.[FILE_NAME_ACADEMIC_YEAR] = isnull(src.[FILE_NAME_ACADEMIC_YEAR],' '),
dst.[FILE_NAME_SEMESTER] = isnull(src.[FILE_NAME_SEMESTER],' '),
dst.[CREATED_ON] = isnull(src.[CREATED_ON],' '),
dst.[MODIFIED_ON] = isnull(src.[MODIFIED_ON],' ');

--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH
  ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
  
END CATCH  
GO</DTS:VariableValue>
