      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
--------------------------------------
DECLARE @jobstatusid varchar(10)
select @jobstatusid=0

DELETE FROM DM_OPN_OIC_ACTIVE_ENGAGEMENTS
INSERT INTO DM_OPN_OIC_ACTIVE_ENGAGEMENTS
select 'AY'+CAST(AY as varchar(6)),count(Acct_ID),
getdate()
,getdate()

  from (

select distinct Acct_ID, Right(Left(Acad_Yr_Sem,6),4) as AY from DW_CRMS_INTERNSHIPS
union
select distinct Acct_ID, 
	CASE WHEN Month(Effective_Date) &gt;= '4' THEN Year(Effective_Date)
	ELSE Year(Effective_Date) - 1 
	END AS AY
 from DW_CRMS_MOU_AGREEMENTS
 union
 select distinct Acct_ID, 
	CASE WHEN Month(Effective_Date) &gt;= '4' THEN Year(Effective_Date)
	ELSE Year(Effective_Date) - 1 
	END AS AY
 from DW_CRMS_SPONSORSHIPS

 union

  select distinct Acct_ID, 
	CASE WHEN Month(Start_Date) &gt;= '4' THEN Year(Start_Date)
	ELSE Year(Start_Date) - 1 
	END AS AY
 from DW_CRMS_STAFF_PROJECTS

 union

   select distinct Acct_ID, Right(Left(Acad_Yr_Sem,6),4) as AY from DW_CRMS_STUDENT_PROJECTS
   where Acad_Yr_Sem&lt;&gt;''
union

 select distinct Acct_ID, 
	CASE WHEN Month(Start_Date) &gt;= '4' THEN Year(Start_Date)
	ELSE Year(Start_Date) - 1 
	END AS AY from DW_CRMS_CET_TRAINING_PROGRAMS

	union

select distinct Acct_ID, 
	CASE WHEN Month(Start_Date) &gt;= '4' THEN Year(Start_Date)
	ELSE Year(Start_Date) - 1 
	END AS AY from DW_CRMS_EARN_AND_LEARN

union

select distinct Acct_ID, 
	CASE WHEN Month(Start_Date) &gt;= '4' THEN Year(Start_Date)
	ELSE Year(Start_Date) - 1 
	END AS AY from DW_CRMS_STAFF_ATTACHMENTS

union

select distinct Acct_ID, 
	CASE WHEN Month(Meeting_Date) &gt;= '4' THEN Year(Meeting_Date)
	ELSE Year(Meeting_Date) - 1 
	END AS AY from DW_CRMS_CONTACT_REPORT 
	where Status='Active'

)main
where AY IS NOT NULL
group by 'AY'+CAST(AY as varchar(6))



COMMIT TRANSACTION
END TRY

BEGIN CATCH
ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
END CATCH  
GO</DTS:VariableValue>
