      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
BEGIN
----------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_IIP_COMP_EVAL_QUESTION
select  @processeddatetime= getdate()
--select @LastDWProcessdate


;WITH TMP_DW_IIP_COMP_EVAL_QUESTION

AS 
(
SELECT 
IC.Id
,IC.Module_ID
,LM.Module_ID AS OASIS_Module_ID
,IC.Student_Id
,IC.Qual_Type
,MC.Long_Name AS Qual_Type_Descr
,IC.Acad_Year AS  Academic_Year
,IC.Semester
,IC.Semester_Code
,IC.Cohort
,IC.[Name]
,IC.Score
--,IC1.[Name] as Comment_Descr
--,IC1.Comment as Comments

from RPDW_UAT_STG..STG_LEO_IIP_COMPANY_EVAL_QUESTION IC
LEFT JOIN RPDW_UAT_STG..STG_LEO_MODULE LM ON LM.Latest_Ind='Y' AND  IC.Module_ID=LM.ID
LEFT JOIN RPDW_UAT_STG..STG_OASIS_MASTER_CODE MC ON IC.Qual_Type=MC.FIELD_VALUE AND MC.FIELD_NAME='ACAD_CAREER' and MC.EFFECTIVE_STATUS = 'A'
--LEFT JOIN RPDW_UAT_STG..STG_LEO_IIP_COMPANY_EVAL_COMMENTS IC1 ON IC.ID=IC1.ID and IC.Student_Id=IC1.Student_Id
--and IC.Module_ID=IC1.Module_ID and IC.Qual_Type=IC1.Qual_Type and IC.Cohort=IC1.Cohort
--and IC.Semester_Code=IC1.Semester_Code

where ic.Latest_Ind='Y' and IC.Modified_on&gt;@LastDWProcessdate)
 



MERGE INTO  DW_IIP_COMP_EVAL_QUESTION AS dst
USING  TMP_DW_IIP_COMP_EVAL_QUESTION AS src
ON( dst.ID=src.ID and dst.Name=src.Name 
--and dst.Comment_Descr=src.Comment_Descr
)

WHEN NOT MATCHED THEN
INSERT
([ID]
      ,[Module_ID]
      ,[OASIS_Module_ID]
      ,[Student_ID]
      ,[Qual_Type]
      ,[Qual_Type_Descr]
      ,[Academic_Year]
      ,[Semester]
      ,[Semester_Code]
      ,[Cohort]
      ,[Name]
      ,[score]
,[Created_On]
,[Modified_On]
--,Comment_Descr
--,Comments
)
VALUES(  
 SRC.[ID]
,SRC.[Module_ID]
,SRC.[OASIS_Module_ID]
,SRC.[Student_ID]
,SRC.[Qual_Type]
,SRC.[Qual_Type_Descr]
,SRC.[Academic_Year]
,SRC.[Semester]
,SRC.[Semester_Code]
,SRC.[Cohort]
,SRC.[Name]
,SRC.[score]

,  @processeddatetime,@processeddatetime
--,SRC.Comment_Descr
--,SRC.Comments
)
WHEN MATCHED
   
THEN UPDATE
SET

DST.[Module_ID]=SRC.[Module_ID]
,DST.[OASIS_Module_ID]=SRC.[OASIS_Module_ID]
,DST.[Student_ID]=SRC.[Student_ID]
,DST.[Qual_Type]=SRC.[Qual_Type]
,DST.[Qual_Type_Descr]=SRC.[Qual_Type_Descr]
,DST.[Academic_Year]=SRC.[Academic_Year]
,DST.[Semester]=SRC.[Semester]
,DST.[Semester_Code]=SRC.[Semester_Code]
,DST.[Cohort]=SRC.[Cohort]
,DST.[score]=SRC.[score]
--,DST.Comments=SRC.Comments

,dst.Modified_On=@processeddatetime;
		
	

------------------------------------
COMMIT TRANSACTION
END
END TRY

BEGIN CATCH
 ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
END CATCH  
GO</DTS:VariableValue>
