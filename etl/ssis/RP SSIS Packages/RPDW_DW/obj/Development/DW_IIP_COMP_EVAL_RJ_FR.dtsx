      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
BEGIN
----------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_IIP_COMP_EVAL_RJ_FR
select  @processeddatetime= getdate()
--select @LastDWProcessdate

;with  TMP_DW_IIP_COMP_EVAL_RJ_FR
as 
(select
IC.Id
,IC.Module_ID
,LM.Module_ID AS OASIS_Module_ID
,IC.Student_Id
,IC.Qual_Type
,MC.Long_Name AS Qual_Type_Descr
,IC.Acad_Year AS  Academic_Year
,IC.Semester
,IC.Semester_Code
,IC.Cohort
,IC.Name
,IC.LO_Type
,IC.Grade
 from RPDW_UAT_STG..STG_LEO_IIP_COMPANY_EVAL_RJ_FR IC
LEFT JOIN RPDW_UAT_STG..STG_LEO_MODULE LM ON LM.Latest_Ind='Y' AND  IC.Module_ID=LM.ID
LEFT JOIN RPDW_UAT_STG..STG_OASIS_MASTER_CODE MC ON IC.Qual_Type=MC.FIELD_VALUE AND MC.FIELD_NAME='ACAD_CAREER' and MC.EFFECTIVE_STATUS = 'A'
where ic.Latest_Ind='Y' and ic.Modified_on&gt;@LastDWProcessdate)



MERGE INTO  DW_IIP_COMP_EVAL_RJ_FR AS dst
USING  TMP_DW_IIP_COMP_EVAL_RJ_FR AS src
ON( dst.ID=src.ID and dst.Name=src.Name)

WHEN NOT MATCHED THEN
INSERT
([ID]
      ,[Module_ID]
      ,[OASIS_Module_ID]
      ,[Student_ID]
      ,[Qual_Type]
      ,[Qual_Type_Descr]
      ,[Academic_Year]
      ,[Semester]
      ,[Semester_Code]
      ,[Cohort]
      ,[Name]
      ,[LO_Type]
      ,[Grade]
,[Created_On]
,[Modified_On])
VALUES(  
 SRC.[ID]
,SRC.[Module_ID]
,SRC.[OASIS_Module_ID]
,SRC.[Student_ID]
,SRC.[Qual_Type]
,SRC.[Qual_Type_Descr]
,SRC.[Academic_Year]
,SRC.[Semester]
,SRC.[Semester_Code]
,SRC.[Cohort]
,SRC.[Name]
,SRC.[LO_Type]
,SRC.[Grade]
,  @processeddatetime,@processeddatetime)
WHEN MATCHED
   
THEN UPDATE
SET

DST.[Module_ID]=SRC.[Module_ID]
,DST.[OASIS_Module_ID]=SRC.[OASIS_Module_ID]
,DST.[Student_ID]=SRC.[Student_ID]
,DST.[Qual_Type]=SRC.[Qual_Type]
,DST.[Qual_Type_Descr]=SRC.[Qual_Type_Descr]
,DST.[Academic_Year]=SRC.[Academic_Year]
,DST.[Semester]=SRC.[Semester]
,DST.[Semester_Code]=SRC.[Semester_Code]
,DST.[Cohort]=SRC.[Cohort]
,DST.[LO_Type]=SRC.[LO_Type]
,DST.[Grade]=SRC.[Grade]


,dst.Modified_On=@processeddatetime;
		
	

------------------------------------
COMMIT TRANSACTION
END
END TRY

BEGIN CATCH
 ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
END CATCH  
GO</DTS:VariableValue>
