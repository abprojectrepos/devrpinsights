      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
--------------------------------------
DECLARE @jobstatusid varchar(5)
select @jobstatusid=0

DELETE FROM dbo.DM_ACE_CET_STAFF
INSERT INTO dbo.DM_ACE_CET_STAFF

select 
distinct
a.Staff_ID
,a.Staff_Name
,a.Dept
,c.Descr
,CASE WHEN a.Staff_Profile='FT' and a.Acad_Prog='B' THEN 'FT Admin Staff'
	  WHEN a.Staff_Profile='FT' and a.Acad_Prog='A' THEN 'FT Acad Staff'
	  WHEN  a.Acad_Prog='H' THEN a.Acad_Prog_Descr ELSE 'Other Staff' END AS Staff_Type
,CASE WHEN b.Class_Label IS NULL THEN 0 ELSE 1 END AS Teaching_CET_Ind
--,a.Acad_Prog_Descr
,b.Module_Id
,b.Module_Code
,b.Descr
,ISNULL(b.Class_Label,'NA') As class
,b.Class_Start_Date
--,'AY20'+SUBSTRING(b.BOE_Term,1,2)
,CASE WHEN month(b.Class_Start_Date)&gt;=4 then 'AY'+CAST(Year(b.Class_Start_Date) as varchar)
ELSE 'AY'+ Cast(Year(b.Class_Start_Date)-1 as varchar)END as Academic_Year
,ISNULL(CAST(b.Class_Meeting_Number as varchar),'NA')
,RIGHT('0'+CAST(DATEPART(hour, b.Meeting_Time_Start) as varchar(2)),2) + ':' +
       RIGHT('0'+CAST(DATEPART(minute, b.Meeting_Time_Start)as varchar(2)),2) +':'+
	   RIGHT('0'+CAST(DATEPART(SECOND, b.Meeting_Time_Start)as varchar(2)),2) as Meeting_Time_Start
,RIGHT('0'+CAST(DATEPART(hour, b.Meeting_Time_End) as varchar(2)),2) + ':' +
       RIGHT('0'+CAST(DATEPART(minute, b.Meeting_Time_End)as varchar(2)),2) +':'+
	   RIGHT('0'+CAST(DATEPART(SECOND, b.Meeting_Time_End)as varchar(2)),2) as Meeting_Time_End
--,CAST(datediff(HOUR,b.Meeting_Time_Start,b.Meeting_Time_End) as decimal(5,2)) 
,cast(Duration as decimal(5,2))
,getdate()
,getdate()
from dbo.DW_STAFF_DETAILS a LEFT JOIN (SELECT Lecturer_Id, Module_Id, Module_Code, Descr, Class_Label, Class_Start_Date, BOE_Term, Class_Meeting_Number, 
MIN(Meeting_Time_Start) AS Meeting_Time_Start, 
MAX(Meeting_Time_End) AS Meeting_Time_End,
SUM(CAST(DATEDIFF(minute, Meeting_Time_Start, Meeting_Time_End) AS decimal)/60.0) AS Duration
FROM (
       SELECT Lecturer_Id, Module_Id, Module_Code, Descr, Class_Label, Class_Start_Date, BOE_Term, Class_Meeting_Number, Meeting_Time_Start, Meeting_Time_End,
       ROW_NUMBER() OVER (
       PARTITION BY Lecturer_Id, Module_Id, Module_Code, Descr, Class_Label, Class_Start_Date, BOE_Term, Class_Meeting_Number, Meeting_Time_Start, Meeting_Time_End
       ORDER BY Instructor_Assign_Seq) AS rownum
       FROM DW_STUDENT_CLASS_SCHEDULE
       WHERE (Qual_Type IN ('SD','PTD')
       OR Qual_Type LIKE 'MC%'
       OR Qual_Type LIKE 'PDC%')) sc
WHERE sc.rownum = 1
GROUP BY Lecturer_Id, Module_Id, Module_Code, Descr, Class_Label, Class_Start_Date, BOE_Term, Class_Meeting_Number) b ON a.Staff_ID=b.Lecturer_Id
LEFT JOIN dbo.DW_EXT_DEPARTMENT c on a.Dept=c.Department
where a.Acct_Lock=0

COMMIT TRANSACTION
END TRY

BEGIN CATCH
ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
END CATCH  
GO



