      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;BEGIN&#xA;----------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(5)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JobStatusId] +&quot;&#xA;select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_IIP_COMP_EVAL_WITHNUMOFDAY&#xA;select  @processeddatetime= getdate()&#xA;--select @LastDWProcessdate&#xA;&#xA;&#xA;;WITH TMP_DW_IIP_COMP_EVAL_WITHNUMOFDAY&#xA;&#xA;AS &#xA;(&#xA;SELECT &#xA;IC.Id&#xA;,IC.Module_ID&#xA;,LM.Module_ID AS OASIS_Module_ID&#xA;,IC.Student_Id&#xA;,IC.Qual_Type&#xA;,MC.Long_Name AS Qual_Type_Descr&#xA;,IC.Acad_Year AS  Academic_Year&#xA;,IC.Semester&#xA;,IC.Semester_Code&#xA;,IC.Cohort&#xA;,IC.NAME&#xA;,IC.Value&#xA;&#xA;from &quot;+@[$Project::Staging_Initial_Catalog]+&quot;..STG_LEO_IIP_COMPANY_EVAL_WITHNUMOFDAY IC&#xA;LEFT JOIN &quot;+@[$Project::Staging_Initial_Catalog]+&quot;..STG_LEO_MODULE LM ON LM.Latest_Ind='Y' AND  IC.Module_ID=LM.ID&#xA;LEFT JOIN &quot;+@[$Project::Staging_Initial_Catalog]+&quot;..STG_OASIS_MASTER_CODE MC ON IC.Qual_Type=MC.FIELD_VALUE AND MC.FIELD_Name='ACAD_CAREER' and MC.EFFECTIVE_STATUS = 'A'&#xA;where ic.Latest_Ind='Y' and ic.Modified_on&gt;@LastDWProcessdate)&#xA; &#xA;&#xA;&#xA;&#xA;MERGE INTO  DW_IIP_COMP_EVAL_WITHNUMOFDAY AS dst&#xA;USING  TMP_DW_IIP_COMP_EVAL_WITHNUMOFDAY AS src&#xA;ON( dst.ID=src.ID and dst.NAME=src.NAME)&#xA;&#xA;WHEN NOT MATCHED THEN&#xA;INSERT&#xA;([ID]&#xA;      ,[Module_ID]&#xA;      ,[OASIS_Module_ID]&#xA;      ,[Student_ID]&#xA;      ,[Qual_Type]&#xA;      ,[Qual_Type_Descr]&#xA;      ,[Academic_Year]&#xA;      ,[Semester]&#xA;      ,[Semester_Code]&#xA;      ,[Cohort]&#xA;      ,[NAME]&#xA;      ,[Value]&#xA;,[Created_On]&#xA;,[Modified_On])&#xA;VALUES(  &#xA; SRC.[ID]&#xA;,SRC.[Module_ID]&#xA;,SRC.[OASIS_Module_ID]&#xA;,SRC.[Student_ID]&#xA;,SRC.[Qual_Type]&#xA;,SRC.[Qual_Type_Descr]&#xA;,SRC.[Academic_Year]&#xA;,SRC.[Semester]&#xA;,SRC.[Semester_Code]&#xA;,SRC.[Cohort]&#xA;,SRC.[NAME]&#xA;,SRC.[Value]&#xA;&#xA;,  @processeddatetime,@processeddatetime)&#xA;WHEN MATCHED&#xA;   &#xA;THEN UPDATE&#xA;SET&#xA;&#xA;DST.[Module_ID]=SRC.[Module_ID]&#xA;,DST.[OASIS_Module_ID]=SRC.[OASIS_Module_ID]&#xA;,DST.[Student_ID]=SRC.[Student_ID]&#xA;,DST.[Qual_Type]=SRC.[Qual_Type]&#xA;,DST.[Qual_Type_Descr]=SRC.[Qual_Type_Descr]&#xA;,DST.[Academic_Year]=SRC.[Academic_Year]&#xA;,DST.[Semester]=SRC.[Semester]&#xA;,DST.[Semester_Code]=SRC.[Semester_Code]&#xA;,DST.[Cohort]=SRC.[Cohort]&#xA;,DST.[Value]=SRC.[Value]&#xA;,dst.Modified_On=@processeddatetime;&#xA;&#x9;&#x9;&#xA;&#x9;&#xA;&#xA;------------------------------------&#xA;COMMIT TRANSACTION&#xA;END&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA; ROLLBACK TRANSACTION&#xA;INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,@processeddatetime  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="InsertQuery">
      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
BEGIN
----------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(5)
select @jobstatusid=0
select @LastDWProcessdate=isnull(max(Modified_on),'1900-01-01 00:00:00.000') from DW_IIP_COMP_EVAL_WITHNUMOFDAY
select  @processeddatetime= getdate()
--select @LastDWProcessdate


;WITH TMP_DW_IIP_COMP_EVAL_WITHNUMOFDAY

AS 
(
SELECT 
IC.Id
,IC.Module_ID
,LM.Module_ID AS OASIS_Module_ID
,IC.Student_Id
,IC.Qual_Type
,MC.Long_Name AS Qual_Type_Descr
,IC.Acad_Year AS  Academic_Year
,IC.Semester
,IC.Semester_Code
,IC.Cohort
,IC.NAME
,IC.Value

from RPDW_UAT_STG..STG_LEO_IIP_COMPANY_EVAL_WITHNUMOFDAY IC
LEFT JOIN RPDW_UAT_STG..STG_LEO_MODULE LM ON LM.Latest_Ind='Y' AND  IC.Module_ID=LM.ID
LEFT JOIN RPDW_UAT_STG..STG_OASIS_MASTER_CODE MC ON IC.Qual_Type=MC.FIELD_VALUE AND MC.FIELD_Name='ACAD_CAREER' and MC.EFFECTIVE_STATUS = 'A'
where ic.Latest_Ind='Y' and ic.Modified_on&gt;@LastDWProcessdate)
 



MERGE INTO  DW_IIP_COMP_EVAL_WITHNUMOFDAY AS dst
USING  TMP_DW_IIP_COMP_EVAL_WITHNUMOFDAY AS src
ON( dst.ID=src.ID and dst.NAME=src.NAME)

WHEN NOT MATCHED THEN
INSERT
([ID]
      ,[Module_ID]
      ,[OASIS_Module_ID]
      ,[Student_ID]
      ,[Qual_Type]
      ,[Qual_Type_Descr]
      ,[Academic_Year]
      ,[Semester]
      ,[Semester_Code]
      ,[Cohort]
      ,[NAME]
      ,[Value]
,[Created_On]
,[Modified_On])
VALUES(  
 SRC.[ID]
,SRC.[Module_ID]
,SRC.[OASIS_Module_ID]
,SRC.[Student_ID]
,SRC.[Qual_Type]
,SRC.[Qual_Type_Descr]
,SRC.[Academic_Year]
,SRC.[Semester]
,SRC.[Semester_Code]
,SRC.[Cohort]
,SRC.[NAME]
,SRC.[Value]

,  @processeddatetime,@processeddatetime)
WHEN MATCHED
   
THEN UPDATE
SET

DST.[Module_ID]=SRC.[Module_ID]
,DST.[OASIS_Module_ID]=SRC.[OASIS_Module_ID]
,DST.[Student_ID]=SRC.[Student_ID]
,DST.[Qual_Type]=SRC.[Qual_Type]
,DST.[Qual_Type_Descr]=SRC.[Qual_Type_Descr]
,DST.[Academic_Year]=SRC.[Academic_Year]
,DST.[Semester]=SRC.[Semester]
,DST.[Semester_Code]=SRC.[Semester_Code]
,DST.[Cohort]=SRC.[Cohort]
,DST.[Value]=SRC.[Value]
,dst.Modified_On=@processeddatetime;
		
	

------------------------------------
COMMIT TRANSACTION
END
END TRY

BEGIN CATCH
 ROLLBACK TRANSACTION
INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,@processeddatetime  as created_on
  
END CATCH  
GO</DTS:VariableValue>
