      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;&#xA;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_CMIO_MAPPING]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;&#xA;&#xA;;with TMP_DW_CMIO_MAPPING (&#xA;&#x9;&#x9;[System_Race_Code]&#xA;      ,[Race_Desc]&#xA;      ,[Race_Mapping_CMIO]&#xA;      ,[MOE_Race_Code]&#xA;      ,[SSEC_Code]&#xA;      ,[SSEC_Desc]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT &#x9;&#xA;&#x9;&#x9;[System_Race_Code]&#xA;      ,[Race_Desc]&#xA;      ,[Race_Mapping_CMIO]&#xA;      ,[MOE_Race_Code]&#xA;      ,[SSEC_Code]&#xA;      ,[SSEC_Desc]&#xA;  FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_EXT_CMIO_MAPPING]&#xA;  where [Latest_Ind]='Y'  and [Modified_On]&gt;@LastDWProcessdate)&#xA;&#xA;       &#xA;&#xA;MERGE INTO dbo.DW_CMIO_MAPPING AS dst&#xA;   USING TMP_DW_CMIO_MAPPING AS src&#xA;   ON( dst.[System_Race_Code]=src.[System_Race_Code] )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT( &#xA;&#x9;&#x9;&#x9;[System_Race_Code]&#xA;&#x9;&#x9;&#x9;,[Race_Desc]&#xA;&#x9;&#x9;&#x9;,[Race_Mapping_CMIO]&#xA;&#x9;&#x9;&#x9;,[MOE_Race_Code]&#xA;&#x9;&#x9;&#x9;,[SSEC_Code]&#xA;&#x9;&#x9;&#x9;,[SSEC_Desc]&#xA;&#x9;&#x9;&#x9;,[Created_On]&#xA;&#x9;&#x9;&#x9;,[Modified_On] )&#xA;   VALUES(  &#xA;&#x9;&#x9;&#x9; src.[System_Race_Code]&#xA;&#x9;&#x9;&#x9;,src.[Race_Desc]&#xA;&#x9;&#x9;&#x9;,src.[Race_Mapping_CMIO]&#xA;&#x9;&#x9;&#x9;,src.[MOE_Race_Code]&#xA;&#x9;&#x9;&#x9;,src.[SSEC_Code]&#xA;&#x9;&#x9;&#x9;,src.[SSEC_Desc]&#xA;&#x9;&#x9;&#x9;,getdate()&#xA;&#x9;&#x9;&#x9;,getdate()&#xA;&#x9;   )&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET   &#xA;&#x9;&#x9;&#x9;dst.[Race_Desc]=src.[Race_Desc]&#xA;&#x9;&#x9;&#x9;,dst.[Race_Mapping_CMIO]=src.[Race_Mapping_CMIO]&#xA;&#x9;&#x9;&#x9;,dst.[MOE_Race_Code]=src.[MOE_Race_Code]&#xA;&#x9;&#x9;&#x9;,dst.[SSEC_Code]=src.[SSEC_Code]&#xA;&#x9;&#x9;&#x9;,dst.[SSEC_Desc]=src.[SSEC_Desc]&#xA;&#x9;&#x9;&#x9;,dst.Modified_On=getdate();&#xA;&#x9;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&#xA;&quot;"
