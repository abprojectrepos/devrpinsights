      <DTS:VariableValue
        DTS:DataType="8">BEGIN TRANSACTION
BEGIN TRY
------------
DECLARE @LastDWProcessdate datetime
,@processeddatetime datetime
,@jobstatusid varchar(10)
select @jobstatusid=0
select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from [dbo].[DW_STUDENT_CARE]--dw table
--select  @processeddatetime=?
--select @LastDWProcessdate


;with TMP_DW_STUDENT_CARE (
		[Acad_Yr]
      ,[Student_ID]
      ,[Care_Category]
      ,[Care_Main_Type]
      ,[Care_Sub_Type]
      ,[Care_Admin_Remarks]
      ,[Care_Date_Updated]
      ,[Care_Indicator]
      )
         as
  (
SELECT 	
		[Acad_Yr]
      ,[Student_ID]
      ,[Care_Category]
      ,[Care_Main_Type]
      ,[Care_Sub_Type]
      ,[Care_Admin_Remarks]
      ,[Care_Date_Updated]
      ,[Care_Indicator]
  FROM RPDW_STG.[dbo].[STG_EXT_STUDENT_CARE]
  where [Latest_Ind]='Y'  and [Modified_On]&gt;@LastDWProcessdate)

       

MERGE INTO dbo.DW_STUDENT_CARE AS dst
   USING TMP_DW_STUDENT_CARE AS src
   ON( dst.[Acad_Yr]=src.[Acad_Yr] and dst.[Student_ID]=src.[Student_ID] and dst.[Care_Category]=src.[Care_Category]
		 and dst.[Care_Main_Type]=src.[Care_Main_Type] and dst.[Care_Sub_Type]=src.[Care_Sub_Type] )

   WHEN NOT MATCHED THEN
   INSERT
		( [Acad_Yr]
		,[Student_ID]
		,[Care_Category]
		,[Care_Main_Type]
		,[Care_Sub_Type]
		,[Care_Admin_Remarks]
		,[Care_Date_Updated]
		,[Care_Indicator]
		,[Created_On]
		,[Modified_On] )
   VALUES(  
		src.[Acad_Yr]
		,src.[Student_ID]
		,src.[Care_Category]
		,src.[Care_Main_Type]
		,src.[Care_Sub_Type]
		,src.[Care_Admin_Remarks]
		,src.[Care_Date_Updated]
		,src.[Care_Indicator]
		,getdate()
		,getdate()
	   )
   WHEN MATCHED
   
   THEN UPDATE
   SET   
		dst.[Care_Admin_Remarks]=src.[Care_Admin_Remarks]
		,dst.[Care_Date_Updated]=src.[Care_Date_Updated]
		,dst.[Care_Indicator]=src.[Care_Indicator]
		,dst.Modified_On=getdate();
	
--------------------------------------
COMMIT TRANSACTION
END TRY

BEGIN CATCH

  ROLLBACK TRANSACTION

  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)
  SELECT   
         CAST(@jobstatusid as int) as JOB_STATUS_ID
        ,ERROR_MESSAGE() AS ErrorMessage
		,getdate()  as created_on
  
END CATCH  
GO</DTS:VariableValue>
