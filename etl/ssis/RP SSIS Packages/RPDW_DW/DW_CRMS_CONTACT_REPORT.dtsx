      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;BEGIN TRANSACTION&#xA;BEGIN TRY&#xA;------------&#xA;DECLARE @LastDWProcessdate datetime&#xA;,@processeddatetime datetime&#xA;,@jobstatusid varchar(10)&#xA;select @jobstatusid=&quot;+ (DT_STR, 5,1252) @[User::JOB_STATUS_ID] +&quot;&#xA;select @LastDWProcessdate= Isnull(max(Modified_on),'1990-01-01 00:00:00.000') from  [dbo].[DW_CRMS_CONTACT_REPORT]--dw table&#xA;--select  @processeddatetime=?&#xA;--select @LastDWProcessdate&#xA;;with TMP_DW_CRMS_CONTACT_REPORT ([Contact_Report_ID]&#xA;      ,[Subj]&#xA;      ,[Acct_ID]&#xA;      ,[Acct_Name]&#xA;      ,[Meeting_Date]&#xA;      ,[Status]&#xA;      ,[Summary]&#xA;      ,[CRMS_Created_On]&#xA;      ,[CRMS_Modified_On]&#xA;      )&#xA;         as&#xA;  (&#xA;SELECT  [Contact_Report_ID]&#xA;      ,[Subj]&#xA;      ,[Acct_ID]&#xA;      ,[Acct_Name]&#xA;      ,[Meeting_Date]&#xA;      ,[Status]&#xA;      ,[Summary]&#xA;      ,[CRMS_Created_On]&#xA;      ,[CRMS_Modified_On]&#xA;  FROM &quot;+ @[$Project::Staging_Initial_Catalog] +&quot;.[dbo].[STG_CRMS_CONTACT_REPORT] a&#xA;         where a.[Latest_Ind]='Y' and [Modified_On]&gt;@LastDWProcessdate)&#xA;&#xA;       &#xA;&#xA;MERGE INTO dbo.DW_CRMS_CONTACT_REPORT AS dst&#xA;   USING TMP_DW_CRMS_CONTACT_REPORT AS src&#xA;   ON( dst.[Contact_Report_ID]=src.[Contact_Report_ID] )&#xA;&#xA;   WHEN NOT MATCHED THEN&#xA;   INSERT&#xA;         ( [Contact_Report_ID]&#xA;      ,[Subj]&#xA;      ,[Acct_ID]&#xA;      ,[Acct_Name]&#xA;      ,[Meeting_Date]&#xA;      ,[Status]&#xA;      ,[Summary]&#xA;      ,[CRMS_Created_On]&#xA;      ,[CRMS_Modified_On]&#xA;&#x9;  ,[CREATED_ON]&#xA;      ,[MODIFIED_ON])&#xA;   VALUES(  &#xA;           src.[Contact_Report_ID]&#xA;      ,src.[Subj]&#xA;      ,src.[Acct_ID]&#xA;      ,src.[Acct_Name]&#xA;      ,src.[Meeting_Date]&#xA;      ,src.[Status]&#xA;      ,src.[Summary]&#xA;      ,src.[CRMS_Created_On]&#xA;      ,src.[CRMS_Modified_On]&#xA;     &#xA;       ,  getdate(),getdate()&#xA;&#x9;  )&#xA;   WHEN MATCHED&#xA;   &#xA;   THEN UPDATE&#xA;   SET        dst.[Subj]=src.[Subj]&#xA;              ,dst.[Acct_ID]=src.[Acct_ID]&#xA;              ,dst.[Acct_Name]=src.[Acct_Name]&#xA;              ,dst.[Meeting_Date]=src.[Meeting_Date]&#xA;&#x9;&#x9;&#x9;   ,dst.[Status]=src.[Status]&#xA;&#x9;&#x9;&#x9;   ,dst.[Summary]=src.[Summary]&#xA;&#x9;&#x9;&#x9;&#x9;,dst.[CRMS_Created_On]=src.[CRMS_Created_On]&#xA;&#x9;&#x9;&#x9;&#x9;,dst.[CRMS_Modified_On]=src.[CRMS_Modified_On]&#xA;&#x9;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#x9;  ,dst.Modified_On=getdate();&#xA;&#x9;&#x9;&#x9; &#xA;              &#xA;&#xA;--------------------------------------&#xA;COMMIT TRANSACTION&#xA;END TRY&#xA;&#xA;BEGIN CATCH&#xA;  ROLLBACK TRANSACTION&#xA;&#xA;  INSERT INTO ETL_ERROR_LOG(JOB_STATUS_ID,ERROR_RECORD,created_on)&#xA;  SELECT   &#xA;         CAST(@jobstatusid as int) as JOB_STATUS_ID&#xA;        ,ERROR_MESSAGE() AS ErrorMessage&#xA;&#x9;&#x9;,getdate()  as created_on&#xA;  &#xA;END CATCH  &#xA;GO&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
